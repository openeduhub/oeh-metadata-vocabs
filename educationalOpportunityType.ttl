@base <http://w3id.org/openeduhub/vocabs/educationalOffering/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<> a skos:ConceptScheme ;
    dct:title "Bildungsangebote - Typen und Formate"@de ;
    dct:title "Educational Offerings - Types and Formats"@en ;
    dct:creator <https://wirlernenonline.de> ;
    dct:created "2025-08-13"^^xsd:date ;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <educational-program>, <education-program>, <individual-offering>, <supplementary-offering>, <youth-education> .

<educational-program> a skos:Concept ;
    skos:prefLabel "Bildungsgang"@de ;
    skos:prefLabel "Educational Program"@en ;
    skos:definition "Mehrjährige, strukturierte Programme mit staatlich anerkanntem Abschluss."@de ;
    skos:definition "Multi-year, structured programs with state-recognized qualifications."@en ;
    skos:narrower <school-program>, <vocational-program>, <degree-program> .

<school-program> a skos:Concept ;
    skos:prefLabel "Schulbildungsgang"@de ;
    skos:prefLabel "School Program"@en ;
    skos:definition "Allgemeinbildende und berufsorientierte Schulformen des formalen Bildungssystems."@de ;
    skos:definition "General and vocationally oriented school forms of the formal education system."@en ;
    skos:broader <educational-program> ;
    skos:narrower <primary-education>, <secondary-education>, <career-preparation> ;
    skos:inScheme <> .

<primary-education> a skos:Concept ;
    skos:prefLabel "Grundschulbildung"@de ;
    skos:prefLabel "Primary Education"@en ;
    skos:definition "Grundlegende Schulbildung in den ersten Schuljahren."@de ;
    skos:definition "Basic school education in the first school years."@en ;
    skos:broader <school-program> ;
    skos:inScheme <> .

<secondary-education> a skos:Concept ;
    skos:prefLabel "Weiterführende Schule"@de ;
    skos:prefLabel "Secondary Education"@en ;
    skos:definition "Sekundarbildung einschließlich verschiedener Schulformen bis zum Abitur."@de ;
    skos:definition "Secondary education including various school types up to university entrance qualification."@en ;
    skos:broader <school-program> ;
    skos:inScheme <> .

<career-preparation> a skos:Concept ;
    skos:prefLabel "Berufsvorbereitende Bildung"@de ;
    skos:prefLabel "Career Preparation Education"@en ;
    skos:definition "Schulische Angebote zur Vorbereitung auf eine Berufsausbildung oder den Arbeitsmarkt."@de ;
    skos:definition "School-based offerings to prepare for vocational training or the job market."@en ;
    skos:broader <school-program> ;
    skos:inScheme <> .

<vocational-program> a skos:Concept ;
    skos:prefLabel "Ausbildungsgang"@de ;
    skos:prefLabel "Vocational Program"@en ;
    skos:definition "Duale und schulische Berufsausbildung sowie Umschulung zu anerkannten Berufen."@de ;
    skos:definition "Dual and school-based vocational training as well as retraining for recognized professions."@en ;
    skos:broader <educational-program> ;
    skos:narrower <dual-training>, <school-based-training>, <technical-college>, <retraining-program> ;
    skos:inScheme <> .

<dual-training> a skos:Concept ;
    skos:prefLabel "Duale Ausbildung"@de ;
    skos:prefLabel "Dual Training"@en ;
    skos:definition "Berufsausbildung mit praktischer Ausbildung im Betrieb und theoretischer Ausbildung in der Berufsschule."@de ;
    skos:definition "Vocational training with practical training in companies and theoretical training in vocational schools."@en ;
    skos:broader <vocational-program> ;
    skos:inScheme <> .

<school-based-training> a skos:Concept ;
    skos:prefLabel "Schulische Berufsausbildung"@de ;
    skos:prefLabel "School-based Vocational Training"@en ;
    skos:definition "Vollschulische Berufsausbildung ohne betrieblichen Anteil."@de ;
    skos:definition "Full-time school-based vocational training without company-based component."@en ;
    skos:broader <vocational-program> ;
    skos:inScheme <> .

<technical-college> a skos:Concept ;
    skos:prefLabel "Fachschulausbildung"@de ;
    skos:prefLabel "Technical College Education"@en ;
    skos:definition "Berufsfachschule, Fachoberschule und ähnliche beruflich qualifizierende Schulformen."@de ;
    skos:definition "Technical colleges and specialized schools providing professional qualifications."@en ;
    skos:broader <vocational-program> ;
    skos:inScheme <> .

<retraining-program> a skos:Concept ;
    skos:prefLabel "Umschulung"@de ;
    skos:prefLabel "Retraining Program"@en ;
    skos:definition "Berufliche Umschulung für einen Wechsel in einen anderen anerkannten Beruf."@de ;
    skos:definition "Professional retraining for a career change to another recognized profession."@en ;
    skos:broader <vocational-program> ;
    skos:inScheme <> .

<degree-program> a skos:Concept ;
    skos:prefLabel "Studiengang"@de ;
    skos:prefLabel "Degree Program"@en ;
    skos:definition "Hochschulische Bildungsprogramme, die zu einem akademischen Grad führen."@de ;
    skos:definition "Higher education programs leading to an academic degree."@en ;
    skos:broader <educational-program> ;
    skos:narrower <bachelor-program>, <master-program>, <state-exam-program>, <diploma-program>, <doctoral-program> ;
    skos:inScheme <> .

<bachelor-program> a skos:Concept ;
    skos:prefLabel "Bachelor-Studiengang"@de ;
    skos:prefLabel "Bachelor Program"@en ;
    skos:definition "Grundständiger Hochschulabschluss im gestuften Studiensystem."@de ;
    skos:definition "Undergraduate degree program in the tiered study system."@en ;
    skos:broader <degree-program> ;
    skos:inScheme <> .

<master-program> a skos:Concept ;
    skos:prefLabel "Master-Studiengang"@de ;
    skos:prefLabel "Master Program"@en ;
    skos:definition "Weiterführender Hochschulabschluss im gestuften Studiensystem."@de ;
    skos:definition "Graduate degree program in the tiered study system."@en ;
    skos:broader <degree-program> ;
    skos:inScheme <> .

<state-exam-program> a skos:Concept ;
    skos:prefLabel "Staatsexamen-Studiengang"@de ;
    skos:prefLabel "State Examination Program"@en ;
    skos:definition "Studiengänge mit staatlicher Prüfung wie Lehramt, Medizin oder Jura."@de ;
    skos:definition "Degree programs with state examinations such as teaching, medicine or law."@en ;
    skos:broader <degree-program> ;
    skos:inScheme <> .

<diploma-program> a skos:Concept ;
    skos:prefLabel "Diplom-/Magister-Studiengang"@de ;
    skos:prefLabel "Diploma/Magister Program"@en ;
    skos:definition "Traditionelle Studiengänge mit Diplom- oder Magisterabschluss (größtenteils auslaufend)."@de ;
    skos:definition "Traditional degree programs with diploma or magister qualification (mostly phased out)."@en ;
    skos:broader <degree-program> ;
    skos:inScheme <> .

<doctoral-program> a skos:Concept ;
    skos:prefLabel "Promotionsstudium"@de ;
    skos:prefLabel "Doctoral Program"@en ;
    skos:definition "Wissenschaftliche Weiterqualifikation zur Erlangung des Doktorgrades."@de ;
    skos:definition "Scientific further qualification for obtaining a doctoral degree."@en ;
    skos:broader <degree-program> ;
    skos:inScheme <> .

<education-program> a skos:Concept ;
    skos:prefLabel "Bildungsprogramm"@de ;
    skos:prefLabel "Education Program"@en ;
    skos:definition "Strukturierte, meist mehrmonatige Programme mit anerkanntem Zertifikat."@de ;
    skos:definition "Structured, usually multi-month programs with recognized certificates."@en ;
    skos:narrower <continuing-education>, <course-series>, <rehabilitation-program> .

<continuing-education> a skos:Concept ;
    skos:prefLabel "Fortbildung/Aufstiegsqualifikation"@de ;
    skos:prefLabel "Continuing Education/Advancement Qualification"@en ;
    skos:definition "Strukturierte Weiterbildung mit anerkannten Abschlüssen für berufliche Qualifikation."@de ;
    skos:definition "Structured continuing education with recognized qualifications for professional advancement."@en ;
    skos:broader <education-program> ;
    skos:narrower <advancement-training>, <adaptation-training>, <chamber-certificate> ;
    skos:inScheme <> .

<advancement-training> a skos:Concept ;
    skos:prefLabel "Aufstiegsfortbildung"@de ;
    skos:prefLabel "Advancement Training"@en ;
    skos:definition "Fortbildungen für beruflichen Aufstieg wie Meister, Fachwirt oder Techniker."@de ;
    skos:definition "Training for professional advancement such as master craftsman, specialist or technician qualifications."@en ;
    skos:broader <continuing-education> ;
    skos:inScheme <> .

<adaptation-training> a skos:Concept ;
    skos:prefLabel "Anpassungsfortbildung"@de ;
    skos:prefLabel "Adaptation Training"@en ;
    skos:definition "Fortbildungen zur Anpassung an veränderte Arbeitsanforderungen und neue Technologien."@de ;
    skos:definition "Training to adapt to changing work requirements and new technologies."@en ;
    skos:broader <continuing-education> ;
    skos:inScheme <> .

<chamber-certificate> a skos:Concept ;
    skos:prefLabel "Kammer-Zertifikat"@de ;
    skos:prefLabel "Chamber Certificate"@en ;
    skos:definition "Zertifikatslehrgänge mit anerkannten Abschlüssen von Industrie- und Handelskammern oder Handwerkskammern."@de ;
    skos:definition "Certificate courses with recognized qualifications from chambers of commerce or trade associations."@en ;
    skos:broader <continuing-education> ;
    skos:inScheme <> .

<course-series> a skos:Concept ;
    skos:prefLabel "Lehrgang/Kurs-Serie"@de ;
    skos:prefLabel "Course Series/Program"@en ;
    skos:definition "Mehrmoduliger Lehrgang mit strukturiertem Aufbau und Zertifizierung."@de ;
    skos:definition "Multi-module course program with structured progression and certification."@en ;
    skos:broader <education-program> ;
    skos:narrower <multimodule-course>, <industry-certification>, <provider-certificate> ;
    skos:inScheme <> .

<multimodule-course> a skos:Concept ;
    skos:prefLabel "Mehrmoduliger Lehrgang"@de ;
    skos:prefLabel "Multi-module Course"@en ;
    skos:definition "Strukturierter Lehrgang mit aufeinander aufbauenden Modulen."@de ;
    skos:definition "Structured course program with progressive modules building on each other."@en ;
    skos:broader <course-series> ;
    skos:inScheme <> .

<industry-certification> a skos:Concept ;
    skos:prefLabel "Branchenzertifizierung"@de ;
    skos:prefLabel "Industry Certification"@en ;
    skos:definition "Branchenspezifische Zertifizierungsprogramme und Standards."@de ;
    skos:definition "Industry-specific certification programs and standards."@en ;
    skos:broader <course-series> ;
    skos:inScheme <> .

<provider-certificate> a skos:Concept ;
    skos:prefLabel "Anbieter-Zertifikatskurs"@de ;
    skos:prefLabel "Provider Certificate Course"@en ;
    skos:definition "Zertifikatskurse mit anbieterspezifischen Abschlüssen."@de ;
    skos:definition "Certificate courses with provider-specific qualifications."@en ;
    skos:broader <course-series> ;
    skos:inScheme <> .

<rehabilitation-program> a skos:Concept ;
    skos:prefLabel "Rehabilitative Programme"@de ;
    skos:prefLabel "Rehabilitation Programs"@en ;
    skos:definition "Programme zur beruflichen Rehabilitation und Wiedereingliederung."@de ;
    skos:definition "Programs for professional rehabilitation and reintegration."@en ;
    skos:broader <education-program> ;
    skos:narrower <vocational-rehabilitation>, <requalification> ;
    skos:inScheme <> .

<vocational-rehabilitation> a skos:Concept ;
    skos:prefLabel "Berufliche Rehabilitation"@de ;
    skos:prefLabel "Vocational Rehabilitation"@en ;
    skos:definition "Rehabilitative Bildungsmaßnahmen für Menschen mit gesundheitlichen Einschränkungen."@de ;
    skos:definition "Rehabilitative educational measures for people with health limitations."@en ;
    skos:broader <rehabilitation-program> ;
    skos:inScheme <> .

<requalification> a skos:Concept ;
    skos:prefLabel "Nachqualifizierung"@de ;
    skos:prefLabel "Requalification"@en ;
    skos:definition "Nachträgliche Qualifizierung für formal nicht anerkannte Kompetenzen."@de ;
    skos:definition "Subsequent qualification for formally unrecognized competencies."@en ;
    skos:broader <rehabilitation-program> ;
    skos:inScheme <> .

<individual-offering> a skos:Concept ;
    skos:prefLabel "Einzelangebot"@de ;
    skos:prefLabel "Individual Offering"@en ;
    skos:definition "Spezifische, meist kürzere Bildungsaktivitäten zu konkreten Themen."@de ;
    skos:definition "Specific, usually shorter educational activities on concrete topics."@en ;
    skos:narrower <course-seminar>, <workshop-training>, <license-qualification> .

<course-seminar> a skos:Concept ;
    skos:prefLabel "Kurs/Seminar"@de ;
    skos:prefLabel "Course/Seminar"@en ;
    skos:definition "Einzelne Bildungsangebote zur Entwicklung spezifischer Kompetenzen und Fertigkeiten."@de ;
    skos:definition "Individual educational offerings for developing specific competencies and skills."@en ;
    skos:broader <individual-offering> ;
    skos:narrower <specialist-course>, <language-course>, <it-training>, <soft-skills-training>, <creative-course>, <health-fitness-course> ;
    skos:inScheme <> .

<specialist-course> a skos:Concept ;
    skos:prefLabel "Fachkurs/Spezialisierung"@de ;
    skos:prefLabel "Specialist Course"@en ;
    skos:definition "Kurse zur Vertiefung fachspezifischer Kompetenzen und Spezialisierung in bestimmten Bereichen."@de ;
    skos:definition "Courses for deepening domain-specific competencies and specialization in certain areas."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<language-course> a skos:Concept ;
    skos:prefLabel "Sprachkurs"@de ;
    skos:prefLabel "Language Course"@en ;
    skos:definition "Kurse zum Erlernen oder Vertiefen von Fremdsprachen oder Kommunikationsfertigkeiten."@de ;
    skos:definition "Courses for learning or improving foreign languages or communication skills."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<it-training> a skos:Concept ;
    skos:prefLabel "IT-Schulung"@de ;
    skos:prefLabel "IT Training"@en ;
    skos:definition "Schulungen zu Informationstechnologie, Software, digitalen Tools und technischen Fertigkeiten."@de ;
    skos:definition "Training on information technology, software, digital tools and technical skills."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<soft-skills-training> a skos:Concept ;
    skos:prefLabel "Soft-Skills-Training"@de ;
    skos:prefLabel "Soft Skills Training"@en ;
    skos:definition "Training zu sozialen, kommunikativen und persönlichen Kompetenzen."@de ;
    skos:definition "Training on social, communicative and personal competencies."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<creative-course> a skos:Concept ;
    skos:prefLabel "Kreativkurs"@de ;
    skos:prefLabel "Creative Course"@en ;
    skos:definition "Kurse zu Kunst, Handwerk, Musik und anderen kreativen Tätigkeiten."@de ;
    skos:definition "Courses on art, crafts, music and other creative activities."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<health-fitness-course> a skos:Concept ;
    skos:prefLabel "Gesundheits-/Fitnesskurs"@de ;
    skos:prefLabel "Health/Fitness Course"@en ;
    skos:definition "Kurse zu körperlicher Fitness, Gesundheitsvorsorge und Wohlbefinden."@de ;
    skos:definition "Courses on physical fitness, health prevention and well-being."@en ;
    skos:broader <course-seminar> ;
    skos:inScheme <> .

<workshop-training> a skos:Concept ;
    skos:prefLabel "Workshop/Training"@de ;
    skos:prefLabel "Workshop/Training"@en ;
    skos:definition "Handlungsorientierte Veranstaltungen zur praktischen Kompetenzentwicklung."@de ;
    skos:definition "Action-oriented events for practical competence development."@en ;
    skos:broader <individual-offering> ;
    skos:narrower <practical-training>, <competence-workshop>, <method-training> ;
    skos:inScheme <> .

<practical-training> a skos:Concept ;
    skos:prefLabel "Praktisches Training"@de ;
    skos:prefLabel "Practical Training"@en ;
    skos:definition "Praxisorientierte Trainings mit direkter Anwendung und Übung neuer Fertigkeiten."@de ;
    skos:definition "Practice-oriented training with direct application and practice of new skills."@en ;
    skos:broader <workshop-training> ;
    skos:inScheme <> .

<competence-workshop> a skos:Concept ;
    skos:prefLabel "Kompetenz-Workshop"@de ;
    skos:prefLabel "Competence Workshop"@en ;
    skos:definition "Workshops zur gezielten Entwicklung spezifischer Kompetenzen und Fähigkeiten."@de ;
    skos:definition "Workshops for targeted development of specific competencies and abilities."@en ;
    skos:broader <workshop-training> ;
    skos:inScheme <> .

<method-training> a skos:Concept ;
    skos:prefLabel "Methodentraining"@de ;
    skos:prefLabel "Method Training"@en ;
    skos:definition "Training zu spezifischen Methoden, Techniken und Arbeitsweisen."@de ;
    skos:definition "Training on specific methods, techniques and work approaches."@en ;
    skos:broader <workshop-training> ;
    skos:inScheme <> .

<license-qualification> a skos:Concept ;
    skos:prefLabel "Lizenz-/Befähigungsschulung"@de ;
    skos:prefLabel "License/Qualification Training"@en ;
    skos:definition "Schulungen zum Erwerb von Lizenzen, Befähigungen oder Pflichtqualifikationen."@de ;
    skos:definition "Training for acquiring licenses, qualifications or mandatory certifications."@en ;
    skos:broader <individual-offering> ;
    skos:narrower <driver-training>, <safety-training>, <compliance-training> ;
    skos:inScheme <> .

<driver-training> a skos:Concept ;
    skos:prefLabel "Führerschein-Ausbildung"@de ;
    skos:prefLabel "Driver Training"@en ;
    skos:definition "Ausbildung zum Erwerb von Fahrerlaubnissen und Führerscheinen."@de ;
    skos:definition "Training for acquiring driving licenses and permits."@en ;
    skos:broader <license-qualification> ;
    skos:inScheme <> .

<safety-training> a skos:Concept ;
    skos:prefLabel "Sicherheitsschulung"@de ;
    skos:prefLabel "Safety Training"@en ;
    skos:definition "Schulungen zu Arbeitssicherheit, Brandschutz und anderen Sicherheitsaspekten."@de ;
    skos:definition "Training on occupational safety, fire protection and other safety aspects."@en ;
    skos:broader <license-qualification> ;
    skos:inScheme <> .

<compliance-training> a skos:Concept ;
    skos:prefLabel "Compliance-Training"@de ;
    skos:prefLabel "Compliance Training"@en ;
    skos:definition "Pflichtschulungen zu rechtlichen, regulatorischen oder branchenspezifischen Vorgaben."@de ;
    skos:definition "Mandatory training on legal, regulatory or industry-specific requirements."@en ;
    skos:broader <license-qualification> ;
    skos:inScheme <> .

<supplementary-offering> a skos:Concept ;
    skos:prefLabel "Ergänzungsangebot"@de ;
    skos:prefLabel "Supplementary Offering"@en ;
    skos:definition "Unterstützende und flexible Bildungsformate zur Ergänzung regulärer Bildungswege."@de ;
    skos:definition "Supporting and flexible educational formats to complement regular educational pathways."@en ;
    skos:narrower <tutoring-support>, <counseling-coaching>, <self-learning> .

<tutoring-support> a skos:Concept ;
    skos:prefLabel "Nachhilfe/Förderung"@de ;
    skos:prefLabel "Tutoring/Support"@en ;
    skos:definition "Individuelle oder gruppenbezogene Lernunterstützung zur Verbesserung von Lernergebnissen."@de ;
    skos:definition "Individual or group-based learning support to improve learning outcomes."@en ;
    skos:broader <supplementary-offering> ;
    skos:narrower <learning-support>, <exam-preparation>, <special-support> ;
    skos:inScheme <> .

<learning-support> a skos:Concept ;
    skos:prefLabel "Lernunterstützung"@de ;
    skos:prefLabel "Learning Support"@en ;
    skos:definition "Gezielte Unterstützung bei Lernschwierigkeiten und Wissenslücken."@de ;
    skos:definition "Targeted support for learning difficulties and knowledge gaps."@en ;
    skos:broader <tutoring-support> ;
    skos:inScheme <> .

<exam-preparation> a skos:Concept ;
    skos:prefLabel "Prüfungsvorbereitung"@de ;
    skos:prefLabel "Exam Preparation"@en ;
    skos:definition "Spezielle Vorbereitung auf Prüfungen, Tests und Abschlüsse."@de ;
    skos:definition "Special preparation for exams, tests and qualifications."@en ;
    skos:broader <tutoring-support> ;
    skos:inScheme <> .

<special-support> a skos:Concept ;
    skos:prefLabel "Spezielle Förderung"@de ;
    skos:prefLabel "Special Support"@en ;
    skos:definition "Förderangebote für besondere Bedürfnisse oder Begabungen."@de ;
    skos:definition "Support offerings for special needs or talents."@en ;
    skos:broader <tutoring-support> ;
    skos:inScheme <> .

<counseling-coaching> a skos:Concept ;
    skos:prefLabel "Beratung/Coaching"@de ;
    skos:prefLabel "Counseling/Coaching"@en ;
    skos:definition "Beratende und coachende Unterstützung für Bildungs- und Lernprozesse."@de ;
    skos:definition "Counseling and coaching support for educational and learning processes."@en ;
    skos:broader <supplementary-offering> ;
    skos:narrower <educational-counseling>, <career-counseling>, <learning-coaching> ;
    skos:inScheme <> .

<educational-counseling> a skos:Concept ;
    skos:prefLabel "Bildungsberatung"@de ;
    skos:prefLabel "Educational Counseling"@en ;
    skos:definition "Beratung zu Bildungswegen, -möglichkeiten und -entscheidungen."@de ;
    skos:definition "Counseling on educational pathways, opportunities and decisions."@en ;
    skos:broader <counseling-coaching> ;
    skos:inScheme <> .

<career-counseling> a skos:Concept ;
    skos:prefLabel "Laufbahnberatung"@de ;
    skos:prefLabel "Career Counseling"@en ;
    skos:definition "Beratung zur beruflichen Laufbahn und Karriereentwicklung."@de ;
    skos:definition "Counseling on professional career and career development."@en ;
    skos:broader <counseling-coaching> ;
    skos:inScheme <> .

<learning-coaching> a skos:Concept ;
    skos:prefLabel "Lerncoaching"@de ;
    skos:prefLabel "Learning Coaching"@en ;
    skos:definition "Coaching zur Verbesserung von Lernstrategien und Lerntechniken."@de ;
    skos:definition "Coaching to improve learning strategies and learning techniques."@en ;
    skos:broader <counseling-coaching> ;
    skos:inScheme <> .

<self-learning> a skos:Concept ;
    skos:prefLabel "Selbstlernformat"@de ;
    skos:prefLabel "Self-Learning Format"@en ;
    skos:definition "Selbstgesteuerte Lernformate und -materialien für autonomes Lernen."@de ;
    skos:definition "Self-directed learning formats and materials for autonomous learning."@en ;
    skos:broader <supplementary-offering> ;
    skos:narrower <online-course-self>, <learning-material>, <elearning-module> ;
    skos:inScheme <> .

<online-course-self> a skos:Concept ;
    skos:prefLabel "Online-Kurs (selbstgesteuert)"@de ;
    skos:prefLabel "Online Course (Self-directed)"@en ;
    skos:definition "Selbstgesteuerte Online-Kurse ohne feste Termine oder Betreuung."@de ;
    skos:definition "Self-directed online courses without fixed schedules or supervision."@en ;
    skos:broader <self-learning> ;
    skos:inScheme <> .

<learning-material> a skos:Concept ;
    skos:prefLabel "Lernmaterial"@de ;
    skos:prefLabel "Learning Material"@en ;
    skos:definition "Strukturierte Lernmaterialien für selbstständiges Lernen."@de ;
    skos:definition "Structured learning materials for independent learning."@en ;
    skos:broader <self-learning> ;
    skos:inScheme <> .

