@base <http://w3id.org/openeduhub/vocabs/eventType/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ct: <http://w3id.org/openeduhub/vocabs/contentTypes/> .

<> a skos:ConceptScheme ;
    dct:title "Veranstaltungstypen im Bildungskontext"@de ;
    dct:title "Event Types in Educational Context"@en ;
    dct:creator <https://wirlernenonline.de> ;
    dct:created "2025-01-15"^^xsd:date ;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <knowledge-competence-development>, <information-orientation>, <exchange-networking>, <performance-assessment>, <community-belonging> .

<knowledge-competence-development> a skos:Concept ;
    skos:prefLabel "Wissen & Kompetenzen entwickeln"@de ;
    skos:prefLabel "Knowledge & Competence Development"@en ;
    skos:definition "Veranstaltungen zum Erwerb neuen Wissens, zur Entwicklung praktischer Fertigkeiten und zum Aufbau von Kompetenzen."@de ;
    skos:definition "Events for acquiring new knowledge, developing practical skills and building competencies."@en ;
    skos:broader ct:event ;
    skos:narrower <basic-knowledge-transfer>, <deepening-specialization>, <practical-application>, <research-innovation> .

<basic-knowledge-transfer> a skos:Concept ;
    skos:prefLabel "Grundlagenvermittlung"@de ;
    skos:prefLabel "Basic Knowledge Transfer"@en ;
    skos:definition "Einführende Veranstaltungen zur Vermittlung von Grundlagenwissen und fundamentalen Konzepten."@de ;
    skos:definition "Introductory events for teaching basic knowledge and fundamental concepts."@en ;
    skos:broader <knowledge-competence-development> ;
    skos:narrower <lecture>, <basic-course>, <introductory-seminar>, <webinar>, <online-course> ;
    skos:inScheme <> .

<lecture> a skos:Concept ;
    skos:prefLabel "Vorlesung"@de ;
    skos:prefLabel "Lecture"@en ;
    skos:definition "Traditionelle Form der frontalen Wissensvermittlung an eine größere Gruppe von Lernenden."@de ;
    skos:definition "Traditional form of frontal knowledge transmission to a larger group of learners."@en ;
    skos:broader <basic-knowledge-transfer> ;
    skos:inScheme <> .

<basic-course> a skos:Concept ;
    skos:prefLabel "Grundkurs"@de ;
    skos:prefLabel "Basic Course"@en ;
    skos:definition "Strukturierter Kurs zur systematischen Einführung in ein Fachgebiet oder Thema."@de ;
    skos:definition "Structured course for systematic introduction to a subject area or topic."@en ;
    skos:broader <basic-knowledge-transfer> ;
    skos:inScheme <> .

<introductory-seminar> a skos:Concept ;
    skos:prefLabel "Einführungsseminar"@de ;
    skos:prefLabel "Introductory Seminar"@en ;
    skos:definition "Interaktive Einführungsveranstaltung mit Diskussion und Gruppenarbeit für Einsteiger."@de ;
    skos:definition "Interactive introductory event with discussion and group work for beginners."@en ;
    skos:broader <basic-knowledge-transfer> ;
    skos:inScheme <> .

<webinar> a skos:Concept ;
    skos:prefLabel "Webinar"@de ;
    skos:prefLabel "Webinar"@en ;
    skos:definition "Online-Seminar mit digitaler Übertragung von Präsentationen und Interaktionsmöglichkeiten."@de ;
    skos:definition "Online seminar with digital transmission of presentations and interaction possibilities."@en ;
    skos:broader <basic-knowledge-transfer> ;
    skos:inScheme <> .

<online-course> a skos:Concept ;
    skos:prefLabel "Online-Kurs"@de ;
    skos:prefLabel "Online Course"@en ;
    skos:definition "Digitaler Kurs mit strukturierten Lerneinheiten und meist selbstgesteuerten Lernphasen."@de ;
    skos:definition "Digital course with structured learning units and mostly self-directed learning phases."@en ;
    skos:broader <basic-knowledge-transfer> ;
    skos:inScheme <> .

<deepening-specialization> a skos:Concept ;
    skos:prefLabel "Vertiefung & Spezialisierung"@de ;
    skos:prefLabel "Deepening & Specialization"@en ;
    skos:definition "Aufbauende Veranstaltungen zur Vertiefung vorhandenen Wissens und fachlichen Spezialisierung."@de ;
    skos:definition "Advanced events for deepening existing knowledge and professional specialization."@en ;
    skos:broader <knowledge-competence-development> ;
    skos:narrower <specialist-seminar>, <master-seminar>, <specialist-course>, <expert-lecture>, <specialist-workshop> ;
    skos:inScheme <> .

<specialist-seminar> a skos:Concept ;
    skos:prefLabel "Fachseminar"@de ;
    skos:prefLabel "Specialist Seminar"@en ;
    skos:definition "Vertiefendes Seminar zu spezifischen Fachthemen für fortgeschrittene Lernende."@de ;
    skos:definition "In-depth seminar on specific professional topics for advanced learners."@en ;
    skos:broader <deepening-specialization> ;
    skos:inScheme <> .

<master-seminar> a skos:Concept ;
    skos:prefLabel "Masterseminar"@de ;
    skos:prefLabel "Master Seminar"@en ;
    skos:definition "Hochspezialisiertes Seminar auf Masterniveau mit wissenschaftlicher Ausrichtung."@de ;
    skos:definition "Highly specialized seminar at master's level with scientific orientation."@en ;
    skos:broader <deepening-specialization> ;
    skos:inScheme <> .

<specialist-course> a skos:Concept ;
    skos:prefLabel "Spezialistenkurs"@de ;
    skos:prefLabel "Specialist Course"@en ;
    skos:definition "Intensivkurs für Experten zu hochspezialisierten Themen und Methoden."@de ;
    skos:definition "Intensive course for experts on highly specialized topics and methods."@en ;
    skos:broader <deepening-specialization> ;
    skos:inScheme <> .

<expert-lecture> a skos:Concept ;
    skos:prefLabel "Expertenvortrag"@de ;
    skos:prefLabel "Expert Lecture"@en ;
    skos:definition "Vortrag von Fachexperten zu aktuellen Entwicklungen und Spezialthemen."@de ;
    skos:definition "Lecture by subject experts on current developments and special topics."@en ;
    skos:broader <deepening-specialization> ;
    skos:inScheme <> .

<specialist-workshop> a skos:Concept ;
    skos:prefLabel "Fachworkshop"@de ;
    skos:prefLabel "Specialist Workshop"@en ;
    skos:definition "Praxisorientierter Workshop zu spezialisierten Methoden und Techniken."@de ;
    skos:definition "Practice-oriented workshop on specialized methods and techniques."@en ;
    skos:broader <deepening-specialization> ;
    skos:inScheme <> .

<practical-application> a skos:Concept ;
    skos:prefLabel "Praktische Anwendung"@de ;
    skos:prefLabel "Practical Application"@en ;
    skos:definition "Handlungsorientierte Veranstaltungen zur praktischen Umsetzung und Anwendung von Wissen."@de ;
    skos:definition "Action-oriented events for practical implementation and application of knowledge."@en ;
    skos:broader <knowledge-competence-development> ;
    skos:narrower <workshop>, <tutorial-exercise>, <lab-internship>, <online-workshop>, <fieldwork>, <excursion>, <student-group>, <holiday-workshop>, <project-week> ;
    skos:inScheme <> .

<workshop> a skos:Concept ;
    skos:prefLabel "Workshop"@de ;
    skos:prefLabel "Workshop"@en ;
    skos:definition "Interaktive Veranstaltung zur praktischen Kompetenzentwicklung mit direkter Anwendung."@de ;
    skos:definition "Interactive event for practical competence development with direct application."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<tutorial-exercise> a skos:Concept ;
    skos:prefLabel "Übung/Tutorial"@de ;
    skos:prefLabel "Tutorial/Exercise"@en ;
    skos:definition "Angeleitete Übungsveranstaltung zur Vertiefung und Anwendung von Lehrinhalten."@de ;
    skos:definition "Guided exercise session for deepening and applying learning content."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<lab-internship> a skos:Concept ;
    skos:prefLabel "Labor/Praktikum"@de ;
    skos:prefLabel "Lab/Internship"@en ;
    skos:definition "Praktische Arbeitsveranstaltung in Labor- oder Arbeitsumgebung zur Kompetenzerfahrung."@de ;
    skos:definition "Practical work event in laboratory or work environment for competence experience."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<online-workshop> a skos:Concept ;
    skos:prefLabel "Online-Workshop"@de ;
    skos:prefLabel "Online Workshop"@en ;
    skos:definition "Digital durchgeführter Workshop mit interaktiven Online-Tools und virtueller Zusammenarbeit."@de ;
    skos:definition "Digitally conducted workshop with interactive online tools and virtual collaboration."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<fieldwork> a skos:Concept ;
    skos:prefLabel "Feldarbeit"@de ;
    skos:prefLabel "Fieldwork"@en ;
    skos:definition "Praktische Forschungs- oder Lernarbeit im natürlichen oder beruflichen Umfeld."@de ;
    skos:definition "Practical research or learning work in natural or professional environment."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<excursion> a skos:Concept ;
    skos:prefLabel "Exkursion"@de ;
    skos:prefLabel "Excursion"@en ;
    skos:definition "Lehrveranstaltung außerhalb der Institution zur praktischen Anschauung und Erfahrung."@de ;
    skos:definition "Teaching event outside the institution for practical observation and experience."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<student-group> a skos:Concept ;
    skos:prefLabel "Schüler-AG"@de ;
    skos:prefLabel "Student Group"@en ;
    skos:definition "Regelmäßige außercurriculare Arbeitsgemeinschaft zu spezifischen Interessensgebieten."@de ;
    skos:definition "Regular extracurricular working group on specific areas of interest."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<holiday-workshop> a skos:Concept ;
    skos:prefLabel "Ferienworkshop"@de ;
    skos:prefLabel "Holiday Workshop"@en ;
    skos:definition "Intensiver Workshop während der Ferien zur praktischen Kompetenzentwicklung."@de ;
    skos:definition "Intensive workshop during holidays for practical competence development."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<project-week> a skos:Concept ;
    skos:prefLabel "Projektwoche"@de ;
    skos:prefLabel "Project Week"@en ;
    skos:definition "Mehrtägige projektbasierte Lernveranstaltung mit praktischer Umsetzung von Ideen."@de ;
    skos:definition "Multi-day project-based learning event with practical implementation of ideas."@en ;
    skos:broader <practical-application> ;
    skos:inScheme <> .

<research-innovation> a skos:Concept ;
    skos:prefLabel "Forschung & Innovation"@de ;
    skos:prefLabel "Research & Innovation"@en ;
    skos:definition "Wissenschaftlich orientierte Veranstaltungen zur Forschungstätigkeit und Innovationsentwicklung."@de ;
    skos:definition "Scientifically oriented events for research activities and innovation development."@en ;
    skos:broader <knowledge-competence-development> ;
    skos:narrower <colloquium>, <research-seminar>, <doctoral-seminar>, <research-workshop>, <innovation-workshop> ;
    skos:inScheme <> .

<colloquium> a skos:Concept ;
    skos:prefLabel "Kolloquium"@de ;
    skos:prefLabel "Colloquium"@en ;
    skos:definition "Wissenschaftliche Diskussionsveranstaltung zur Präsentation und Besprechung von Forschungsarbeiten."@de ;
    skos:definition "Scientific discussion event for presentation and discussion of research work."@en ;
    skos:broader <research-innovation> ;
    skos:inScheme <> .

<research-seminar> a skos:Concept ;
    skos:prefLabel "Forschungsseminar"@de ;
    skos:prefLabel "Research Seminar"@en ;
    skos:definition "Seminar zur gemeinsamen Bearbeitung aktueller Forschungsthemen und -methoden."@de ;
    skos:definition "Seminar for collaborative work on current research topics and methods."@en ;
    skos:broader <research-innovation> ;
    skos:inScheme <> .

<doctoral-seminar> a skos:Concept ;
    skos:prefLabel "Doktorandenseminar"@de ;
    skos:prefLabel "Doctoral Seminar"@en ;
    skos:definition "Spezialseminar für Doktoranden zur Diskussion von Dissertationsprojekten."@de ;
    skos:definition "Special seminar for doctoral students to discuss dissertation projects."@en ;
    skos:broader <research-innovation> ;
    skos:inScheme <> .

<research-workshop> a skos:Concept ;
    skos:prefLabel "Forschungsworkshop"@de ;
    skos:prefLabel "Research Workshop"@en ;
    skos:definition "Praktisch orientierter Workshop zur Entwicklung und Anwendung von Forschungsmethoden."@de ;
    skos:definition "Practically oriented workshop for developing and applying research methods."@en ;
    skos:broader <research-innovation> ;
    skos:inScheme <> .

<innovation-workshop> a skos:Concept ;
    skos:prefLabel "Innovationsworkshop"@de ;
    skos:prefLabel "Innovation Workshop"@en ;
    skos:definition "Kreativer Workshop zur Entwicklung innovativer Ideen und Lösungsansätze."@de ;
    skos:definition "Creative workshop for developing innovative ideas and solution approaches."@en ;
    skos:broader <research-innovation> ;
    skos:inScheme <> .

<information-orientation> a skos:Concept ;
    skos:prefLabel "Information & Orientierung erhalten"@de ;
    skos:prefLabel "Information & Orientation"@en ;
    skos:definition "Veranstaltungen zur Informationsbeschaffung, Orientierungshilfe und Entscheidungsunterstützung."@de ;
    skos:definition "Events for information gathering, orientation assistance and decision support."@en ;
    skos:broader ct:event ;
    skos:narrower <overview-introduction>, <counseling-decision-support>, <market-overview-offerings> .

<overview-introduction> a skos:Concept ;
    skos:prefLabel "Überblick & Einführung"@de ;
    skos:prefLabel "Overview & Introduction"@en ;
    skos:definition "Veranstaltungen zur ersten Orientierung und zum Überblick über Angebote und Möglichkeiten."@de ;
    skos:definition "Events for initial orientation and overview of offerings and opportunities."@en ;
    skos:broader <information-orientation> ;
    skos:narrower <info-event>, <orientation-week>, <open-house>, <info-webinar>, <taster-event> ;
    skos:inScheme <> .

<info-event> a skos:Concept ;
    skos:prefLabel "Infoveranstaltung"@de ;
    skos:prefLabel "Information Event"@en ;
    skos:definition "Veranstaltung zur gezielten Information über spezifische Programme, Angebote oder Verfahren."@de ;
    skos:definition "Event for targeted information about specific programs, offerings or procedures."@en ;
    skos:broader <overview-introduction> ;
    skos:inScheme <> .

<orientation-week> a skos:Concept ;
    skos:prefLabel "Orientierungswoche"@de ;
    skos:prefLabel "Orientation Week"@en ;
    skos:definition "Mehrtägige Einführungsveranstaltung zur umfassenden Orientierung für neue Teilnehmer."@de ;
    skos:definition "Multi-day introductory event for comprehensive orientation of new participants."@en ;
    skos:broader <overview-introduction> ;
    skos:inScheme <> .

<open-house> a skos:Concept ;
    skos:prefLabel "Tag der offenen Tür"@de ;
    skos:prefLabel "Open House"@en ;
    skos:definition "Öffentliche Veranstaltung zum Kennenlernen einer Institution und ihrer Angebote."@de ;
    skos:definition "Public event for getting to know an institution and its offerings."@en ;
    skos:broader <overview-introduction> ;
    skos:inScheme <> .

<info-webinar> a skos:Concept ;
    skos:prefLabel "Info-Webinar"@de ;
    skos:prefLabel "Info Webinar"@en ;
    skos:definition "Online-Informationsveranstaltung mit digitaler Präsentation und Fragerunde."@de ;
    skos:definition "Online information event with digital presentation and Q&A session."@en ;
    skos:broader <overview-introduction> ;
    skos:inScheme <> .

<taster-event> a skos:Concept ;
    skos:prefLabel "Schnupperveranstaltung"@de ;
    skos:prefLabel "Taster Event"@en ;
    skos:definition "Kurze Probeveranstaltung zum ersten Kennenlernen von Inhalten oder Formaten."@de ;
    skos:definition "Short trial event for initial acquaintance with content or formats."@en ;
    skos:broader <overview-introduction> ;
    skos:inScheme <> .

<counseling-decision-support> a skos:Concept ;
    skos:prefLabel "Beratung & Entscheidungshilfe"@de ;
    skos:prefLabel "Counseling & Decision Support"@en ;
    skos:definition "Personalisierte Beratungsveranstaltungen zur individuellen Unterstützung bei Entscheidungen."@de ;
    skos:definition "Personalized counseling events for individual support in decision-making."@en ;
    skos:broader <information-orientation> ;
    skos:narrower <study-counseling>, <consultation-hours>, <counseling-session>, <online-counseling>, <career-counseling>, <career-guidance> ;
    skos:inScheme <> .

<study-counseling> a skos:Concept ;
    skos:prefLabel "Studienberatung"@de ;
    skos:prefLabel "Study Counseling"@en ;
    skos:definition "Beratung zur Studienorientierung, Studienwahl und Studienplanung."@de ;
    skos:definition "Counseling for study orientation, study choice and study planning."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<consultation-hours> a skos:Concept ;
    skos:prefLabel "Sprechstunde"@de ;
    skos:prefLabel "Consultation Hours"@en ;
    skos:definition "Regelmäßige individuelle Beratungszeit für persönliche Anliegen und Fragen."@de ;
    skos:definition "Regular individual consultation time for personal concerns and questions."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<counseling-session> a skos:Concept ;
    skos:prefLabel "Beratungsgespräch"@de ;
    skos:prefLabel "Counseling Session"@en ;
    skos:definition "Strukturiertes Einzelgespräch zur individuellen Beratung und Unterstützung."@de ;
    skos:definition "Structured individual conversation for personal counseling and support."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<online-counseling> a skos:Concept ;
    skos:prefLabel "Online-Beratung"@de ;
    skos:prefLabel "Online Counseling"@en ;
    skos:definition "Digitale Beratung über Online-Plattformen und Kommunikationstools."@de ;
    skos:definition "Digital counseling via online platforms and communication tools."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<career-counseling> a skos:Concept ;
    skos:prefLabel "Karriereberatung"@de ;
    skos:prefLabel "Career Counseling"@en ;
    skos:definition "Beratung zu beruflichen Entwicklungsmöglichkeiten und Karriereplanung."@de ;
    skos:definition "Counseling on professional development opportunities and career planning."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<career-guidance> a skos:Concept ;
    skos:prefLabel "Laufbahnberatung"@de ;
    skos:prefLabel "Career Guidance"@en ;
    skos:definition "Umfassende Beratung zur langfristigen beruflichen Laufbahnplanung."@de ;
    skos:definition "Comprehensive counseling for long-term professional career planning."@en ;
    skos:broader <counseling-decision-support> ;
    skos:inScheme <> .

<market-overview-offerings> a skos:Concept ;
    skos:prefLabel "Marktüberblick & Angebote"@de ;
    skos:prefLabel "Market Overview & Offerings"@en ;
    skos:definition "Veranstaltungen zur Präsentation und zum Vergleich verschiedener Anbieter und Optionen."@de ;
    skos:definition "Events for presentation and comparison of different providers and options."@en ;
    skos:broader <information-orientation> ;
    skos:narrower <education-fair>, <university-fair>, <career-fair>, <virtual-fair>, <provider-presentation> ;
    skos:inScheme <> .

<education-fair> a skos:Concept ;
    skos:prefLabel "Bildungsmesse"@de ;
    skos:prefLabel "Education Fair"@en ;
    skos:definition "Messe zur Präsentation verschiedener Bildungsangebote und -anbieter."@de ;
    skos:definition "Fair for presenting various educational offerings and providers."@en ;
    skos:broader <market-overview-offerings> ;
    skos:inScheme <> .

<university-fair> a skos:Concept ;
    skos:prefLabel "Hochschulmesse"@de ;
    skos:prefLabel "University Fair"@en ;
    skos:definition "Spezialmesse zur Präsentation von Hochschulen und Studienmöglichkeiten."@de ;
    skos:definition "Specialized fair for presenting universities and study opportunities."@en ;
    skos:broader <market-overview-offerings> ;
    skos:inScheme <> .

<career-fair> a skos:Concept ;
    skos:prefLabel "Karrieremesse"@de ;
    skos:prefLabel "Career Fair"@en ;
    skos:definition "Messe zur Präsentation von Berufsfeldern, Arbeitgebern und Karrieremöglichkeiten."@de ;
    skos:definition "Fair for presenting career fields, employers and career opportunities."@en ;
    skos:broader <market-overview-offerings> ;
    skos:inScheme <> .

<virtual-fair> a skos:Concept ;
    skos:prefLabel "Virtuelle Messe"@de ;
    skos:prefLabel "Virtual Fair"@en ;
    skos:definition "Online durchgeführte Messe mit digitalen Messeständen und virtuellen Gesprächen."@de ;
    skos:definition "Online conducted fair with digital exhibition stands and virtual conversations."@en ;
    skos:broader <market-overview-offerings> ;
    skos:inScheme <> .

<provider-presentation> a skos:Concept ;
    skos:prefLabel "Anbieterpräsentation"@de ;
    skos:prefLabel "Provider Presentation"@en ;
    skos:definition "Präsentationsveranstaltung einzelner Anbieter zur Vorstellung ihrer Angebote."@de ;
    skos:definition "Presentation event by individual providers to showcase their offerings."@en ;
    skos:broader <market-overview-offerings> ;
    skos:inScheme <> .

<exchange-networking> a skos:Concept ;
    skos:prefLabel "Austausch & Vernetzung pflegen"@de ;
    skos:prefLabel "Exchange & Networking"@en ;
    skos:definition "Veranstaltungen zum fachlichen Gedankenaustausch, zur Diskussion und zum Aufbau professioneller Netzwerke."@de ;
    skos:definition "Events for professional exchange of ideas, discussion and building professional networks."@en ;
    skos:broader ct:event ;
    skos:narrower <professional-exchange>, <peer-learning-collaboration>, <professional-networking> .

<professional-exchange> a skos:Concept ;
    skos:prefLabel "Fachlicher Austausch"@de ;
    skos:prefLabel "Professional Exchange"@en ;
    skos:definition "Wissenschaftlich orientierte Veranstaltungen zum Austausch von Fachwissen und Erfahrungen."@de ;
    skos:definition "Scientifically oriented events for exchanging professional knowledge and experiences."@en ;
    skos:broader <exchange-networking> ;
    skos:narrower <symposium>, <professional-conference>, <conference-congress>, <online-conference>, <panel>, <roundtable>, <forum>, <debate> ;
    skos:inScheme <> .

<symposium> a skos:Concept ;
    skos:prefLabel "Symposium"@de ;
    skos:prefLabel "Symposium"@en ;
    skos:definition "Kleine, spezialisierte Expertenversammlung zu einem spezifischen Forschungsthema."@de ;
    skos:definition "Small, specialized expert gathering on a specific research topic."@en ;
    skos:broader <professional-exchange> ;
    skos:inScheme <> .

<professional-conference> a skos:Concept ;
    skos:prefLabel "Fachtagung"@de ;
    skos:prefLabel "Professional Conference"@en ;
    skos:definition "Fachspezifische Tagung zum Erfahrungsaustausch und zur beruflichen Weiterbildung."@de ;
    skos:definition "Subject-specific conference for exchange of experience and professional development."@en ;
    skos:broader <professional-exchange> ;
    skos:inScheme <> .

<conference-congress> a skos:Concept ;
    skos:prefLabel "Konferenz/Kongress"@de ;
    skos:prefLabel "Conference/Congress"@en ;
    skos:definition "Große wissenschaftliche Veranstaltung mit Präsentationen, Diskussionen und Networking."@de ;
    skos:definition "Large scientific event with presentations, discussions and networking."@en ;
    skos:broader <professional-exchange> ;
    skos:inScheme <> .

<online-conference> a skos:Concept ;
    skos:prefLabel "Online-Konferenz"@de ;
    skos:prefLabel "Online Conference"@en ;
    skos:definition "Digital durchgeführte Konferenz mit virtuellen Präsentationen und Online-Diskussionen."@de ;
    skos:definition "Digitally conducted conference with virtual presentations and online discussions."@en ;
    skos:broader <professional-exchange> ;
    skos:inScheme <> .

<panel> a skos:Concept ;
