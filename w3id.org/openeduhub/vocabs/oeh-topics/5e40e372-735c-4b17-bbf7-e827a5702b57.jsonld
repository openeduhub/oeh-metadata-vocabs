atedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c35c9e43-ef6d-49b2-81b1-41bf06140748","notation":["c35c9e43-ef6d-49b2-81b1-41bf06140748"],"prefLabel":{"de":"Verben zum Ausdruck von Veränderung"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cf852175-4c83-4cc8-8d1d-fc17d189b958","notation":["cf852175-4c83-4cc8-8d1d-fc17d189b958"],"prefLabel":{"de":"Gerundio"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/efe918f4-7db6-46c6-82ea-f02274a639c7","notation":["efe918f4-7db6-46c6-82ea-f02274a639c7"],"prefLabel":{"de":"Passiv"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f960f172-15e4-472d-9229-770d5a950f32","notation":["f960f172-15e4-472d-9229-770d5a950f32"],"prefLabel":{"de":"'Ser', 'estar' und 'hay'"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b7d3a7c1-35c7-4418-92fc-c769fc67590a","notation":["b7d3a7c1-35c7-4418-92fc-c769fc67590a"],"prefLabel":{"de":"Adjektive"},"definition":{"de":"¿Qué es un adjetivo? Man nennt sie auch Eigenschaftswörter, Wiewörter oder Beiwörter. Adjektive beschreiben Eigenschaften von Substantiven näher; dabei kann es sich um Personen, Tiere und andere Lebewesen handeln oder auch eine Sache. Hier findest du vielfältige Inhalte zur Angleichung, Bildung, Stellung sowie zum Vergleich und der Steigerung der Adjektive."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0acdb2d2-ffd2-47a3-9d93-ccec3390105a","notation":["0acdb2d2-ffd2-47a3-9d93-ccec3390105a"],"prefLabel":{"de":"Vergleich und Steigerung der Adjektive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8f55b1d7-f477-4acc-9a52-80f3fae24eaa","notation":["8f55b1d7-f477-4acc-9a52-80f3fae24eaa"],"prefLabel":{"de":"Adjektivendungen und -angleichung"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dad254d3-0a29-424d-8a3e-3db71cedb112","notation":["dad254d3-0a29-424d-8a3e-3db71cedb112"],"prefLabel":{"de":"Bildung der Adjektive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e62ba585-33a7-4a72-845e-9e7503cde978","notation":["e62ba585-33a7-4a72-845e-9e7503cde978"],"prefLabel":{"de":"Adjektive und Adverbien"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fc14de0b-91bb-4327-bdfe-89cb091084dd","notation":["fc14de0b-91bb-4327-bdfe-89cb091084dd"],"prefLabel":{"de":"Stellung der Adjektive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c3f0f111-9d64-4f3f-acdb-5d0e9f2aa0ea","notation":["c3f0f111-9d64-4f3f-acdb-5d0e9f2aa0ea"],"prefLabel":{"de":"Gegenüberstellung der spanischen Zeiten"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/106dd715-02af-4ae4-b386-e8e36687b8aa","notation":["106dd715-02af-4ae4-b386-e8e36687b8aa"],"prefLabel":{"de":"Indicativo vs. Subjuntivo"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/81b1f59f-88c3-4eba-8ee0-42ea65b7ca04","notation":["81b1f59f-88c3-4eba-8ee0-42ea65b7ca04"],"prefLabel":{"de":"Imperfecto vs. Indefinido "},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cf47ae23-2cde-4f36-b495-fc5bdcecbda5","notation":["cf47ae23-2cde-4f36-b495-fc5bdcecbda5"],"prefLabel":{"de":"Perfecto vs. Indefinido"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fc8f070d-de34-435f-8614-510a45d5f8e1","notation":["fc8f070d-de34-435f-8614-510a45d5f8e1"],"prefLabel":{"de":"Zeiten im Subjuntivo"},"definition":{"de":"„Espero que te guste el subjuntivo...” Der Subjuntivo gehört neben dem Indikativo und dem Imperativo zu den drei Modi im Spanischen. Wichtige Anwendungen sind Wünsche („Espero que vengas.“), Meinungen („Es importante que aprendas el subjuntivo.“), Unsicherheiten („No creo que compre el libro.“) und Bedingungen (“Si tuviera dinero, me compraría un móvil nuevo.”). Es gibt vier Zeitformen, in denen der Subjuntivo verwendet wird: Presente, Pretérito imperfecto, Pretérito perfecto und Pretérito pluscuamperfecto."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/27bac4df-d416-48d6-8677-58db5c76961b","notation":["27bac4df-d416-48d6-8677-58db5c76961b"],"prefLabel":{"de":"Pretérito pluscuamperfecto de subjuntivo"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2b081fad-1039-4415-a46a-74b51b5a6a93","notation":["2b081fad-1039-4415-a46a-74b51b5a6a93"],"prefLabel":{"de":"Pretérito perfecto de subjuntivo"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2f018cb3-a370-4f1a-ac43-da78ca43c49e","notation":["2f018cb3-a370-4f1a-ac43-da78ca43c49e"],"prefLabel":{"de":"Presente de subjuntivo"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/87267534-1845-41b2-8a2b-915993fc316f","notation":["87267534-1845-41b2-8a2b-915993fc316f"],"prefLabel":{"de":"Pretérito imperfecto de subjuntivo"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/10106675-1ec2-4aa1-b53c-0d7274678216","notation":["10106675-1ec2-4aa1-b53c-0d7274678216"],"prefLabel":{"de":"Substantive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/29c6aae8-c538-48cc-9261-f4c5dfe98265","notation":["29c6aae8-c538-48cc-9261-f4c5dfe98265"],"prefLabel":{"de":"Genus der Substantive"},"definition":{"de":"Oft kann man an der Endung eines Substantivs erkennen, welches Geschlecht es hat. Der Vorteil ist dabei, dass es im Spanischen nur maskuline und eine feminine Substantive gibt. Substantive auf -o sind in der Regel maskulin, die auf -a feminin. Aber das ist nicht immer so, da es auch Ausnahmen gibt. Alles, was du darüber wissen musst, findest du hier."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d4d67713-5848-4c89-93e0-cde4ca5b5171","notation":["d4d67713-5848-4c89-93e0-cde4ca5b5171"],"prefLabel":{"de":"Bildung der Substantive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f8a30f26-3f33-4001-998e-089d7a613999","notation":["f8a30f26-3f33-4001-998e-089d7a613999"],"prefLabel":{"de":"Plural der Substantive"},"definition":{"de":"Die Grundregel zur Bildung des Plurals der Substantive im Spanischen ist einfach. In den meisten Fällen bilden Substantive auf Vokal den Plural mit -s und auf Konsonant mit -es. Es gibt nur wenige Ausnahmen. Hier kannst du dich darüber informieren und unterschiedlichste Aufgabenformate zum Üben finden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8a31d1ae-e096-4fb2-b481-d6cb7696aa85","notation":["8a31d1ae-e096-4fb2-b481-d6cb7696aa85"],"prefLabel":{"de":"Adverbien"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2c371aa8-8363-4811-bc23-7894cd5e1075","notation":["2c371aa8-8363-4811-bc23-7894cd5e1075"],"prefLabel":{"de":"Vergleich und Steigerung der Adverbien"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/58661f6b-d46e-4e02-9a96-1cbac2c318d7","notation":["58661f6b-d46e-4e02-9a96-1cbac2c318d7"],"prefLabel":{"de":"Bildung von Adverbien aus Adjektiven"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a252cbc7-00c9-4c86-a75a-1d8e75452e7c","notation":["a252cbc7-00c9-4c86-a75a-1d8e75452e7c"],"prefLabel":{"de":"Klassifizierung der Adverbien"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a4e6b53c-e628-42e3-93f8-b9fa1ca9a2ee","notation":["a4e6b53c-e628-42e3-93f8-b9fa1ca9a2ee"],"prefLabel":{"de":"Adverbien und Adjektive"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ffa15916-8725-4e7b-a4cd-f01553655df2","notation":["ffa15916-8725-4e7b-a4cd-f01553655df2"],"prefLabel":{"de":"Stellung der Adverbien"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6d9bd313-b430-4d57-8ef0-7b5fede74d48","notation":["6d9bd313-b430-4d57-8ef0-7b5fede74d48"],"prefLabel":{"de":"Sätze"},"definition":{"de":"Die Stellung der einzelnen Satzglieder im Spanischen unterscheidet sich vom Deutschen. Hier findest du alles, was du über Sätze und ihre Bildungen wissen musst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/541e8ce4-d138-46ad-a878-26f2e0af81dd","notation":["541e8ce4-d138-46ad-a878-26f2e0af81dd"],"prefLabel":{"de":"indirekte Rede"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/709bd09f-47d3-45fd-9598-f61a9ddd7b2d","notation":["709bd09f-47d3-45fd-9598-f61a9ddd7b2d"],"prefLabel":{"de":"Relativsätze"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/91004a0b-1625-49e0-b2d4-b9fa1546d55f","notation":["91004a0b-1625-49e0-b2d4-b9fa1546d55f"],"prefLabel":{"de":"Aussage-, Frage- und Ausrufesätze"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/98340603-b0db-4555-8906-c10a781c37ff","notation":["98340603-b0db-4555-8906-c10a781c37ff"],"prefLabel":{"de":"Satzglieder"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a26d4b79-6d86-4bdc-89a1-1aef57225998","notation":["a26d4b79-6d86-4bdc-89a1-1aef57225998"],"prefLabel":{"de":"verkürzte Nebensätze"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c69fd7e6-06d9-4fb7-91bd-616b995b37bb","notation":["c69fd7e6-06d9-4fb7-91bd-616b995b37bb"],"prefLabel":{"de":"Konjunktionalsätze"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d3278ba3-03e9-48d4-897f-949ea2c9d374","notation":["d3278ba3-03e9-48d4-897f-949ea2c9d374"],"prefLabel":{"de":"Verneinung und Bejahung im Satz"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/da1e2583-9456-4609-8e7c-6d75d8677eb5","notation":["da1e2583-9456-4609-8e7c-6d75d8677eb5"],"prefLabel":{"de":"Konditionalsätze"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f3dc9ea1-d608-4b4e-a78c-98063a3e8461","notation":["f3dc9ea1-d608-4b4e-a78c-98063a3e8461"],"prefLabel":{"de":"Konnektoren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5d9398c8-8c9d-4760-a51b-444e9fbe0a34","notation":["5d9398c8-8c9d-4760-a51b-444e9fbe0a34"],"prefLabel":{"de":"Präpositionen und Ausdrücke"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f89a62dd-9772-428b-9598-20ab032df068","notation":["f89a62dd-9772-428b-9598-20ab032df068"],"prefLabel":{"de":"Artikel"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c5c3c98f-d7b0-49c0-992d-9cac728faa34","notation":["c5c3c98f-d7b0-49c0-992d-9cac728faa34"],"prefLabel":{"de":"lo als bestimmter Artikel"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d0c122e8-62fd-4387-9afa-db35b5693d93","notation":["d0c122e8-62fd-4387-9afa-db35b5693d93"],"prefLabel":{"de":"Nullartikel"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ff178e47-ec3b-4604-9921-779ff5b166eb","notation":["ff178e47-ec3b-4604-9921-779ff5b166eb"],"prefLabel":{"de":"bestimmter und unbestimmter Artikel"},"definition":{"de":"EL, LOS, LA, LAS und UNO, UNA, UNOS, UNAS. Bestimmter und unbestimmter Artikel richten sich in Geschlecht und Zahl nach dem Substantiv, vor dem sie stehen. Wann man den bestimmten und wann den unbestimmten Artikel benutzt, kannst du hier nachlesen und üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/03a945d7-f446-4629-b23f-1918a63a4544","notation":["03a945d7-f446-4629-b23f-1918a63a4544"],"prefLabel":{"de":"Rechtschreibung"},"definition":{"de":"Meistens wird das Spanische so geschrieben, wie man es spricht. Nur ein paar Dinge musst du berücksichtigen. So beginnen Eigennamen wie Personen, Städte, Länder und Regionen immer mit Großbuchstaben. Sollte ein Adjektiv hinzukommen, wird es aber nicht immer automatisch groß geschrieben. Warum schreibt man das Adjektiv in 'el Reino Unido' groß, aber in 'el Imperio inca' klein? Und auch in der Zeichensetzung gibt es Besonderheiten. Mit den folgenden Inhalten kannst du deine Rechtschreibung verbessern, indem du dich über die Regeln der Groß- und Kleinschreibung und der Zeichensetzung informierst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7df9394d-e9c6-4c98-8fd3-a354fc0dd326","notation":["7df9394d-e9c6-4c98-8fd3-a354fc0dd326"],"prefLabel":{"de":"Satzzeichen "},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/95f31e39-7d93-4e18-a6fa-a0b19f995199","notation":["95f31e39-7d93-4e18-a6fa-a0b19f995199"],"prefLabel":{"de":"Groß- und Kleinschreibung"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f91f0a99-66bd-4cd2-aa39-82098958c16b","notation":["f91f0a99-66bd-4cd2-aa39-82098958c16b"],"prefLabel":{"de":"Methodisches"},"definition":{"de":"Wie recherchierst du richtig und wie kannst du es dann am besten präsentieren? Wer heute eine Fremdsprache lernt, braucht nicht nur die verschiedensten Arbeitstechniken und (Sprach-)Lernstrategien, sondern muss auch wissen, wie man sicher, kreativ und verantwortungsvoll mit den unterschiedlichsten digitalen Medien umgeht und ergebnisorientiert damit arbeitet, je nach Zielsetzung oder Aufgabenstellung. Hier bekommst du neben konkreten Unterrichtsbeispielen zur Mediennutzung, vielen Impulsen und Ideen auch Werkzeuge vorgestellt, die dich beim Sprachen lernen und lehren unterstützen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9521d317-0e96-41b6-84a7-aa1e6e0b59dd","notation":["9521d317-0e96-41b6-84a7-aa1e6e0b59dd"],"prefLabel":{"de":"Mediennutzung und Medienkompetenz"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a46c107b-2503-43b3-9a14-d69294105cb6","notation":["a46c107b-2503-43b3-9a14-d69294105cb6"],"prefLabel":{"de":"Sprachlernstrategien, Lern- und Arbeitstechniken"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fab7ae6a-2a05-4b78-9267-a1bffa683c6c","notation":["fab7ae6a-2a05-4b78-9267-a1bffa683c6c"],"prefLabel":{"de":"Bachillerato"},"definition":{"de":"Du hast Spanisch im Abitur? Wir halten dich auf dem neusten Stand mit abwechslungsreichen Inhalten und Musteraufgaben rund um Abiturthemen, wie „Las diversas caras del turismo en España“, „Latinoamérica: El desafío de la pobreza infantil“, „La realidad chilena en la literatura de Antonio Skármeta“ oder „Barcelona: capital polifacética de una comunidad bilingüe“."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/20007"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/69f9ff64-93da-4d68-b849-ebdf9fbdcc77","notation":["69f9ff64-93da-4d68-b849-ebdf9fbdcc77"],"prefLabel":{"de":"Deutsch"},"definition":{"de":"Beschreibungstext"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a24eff1e-d4d1-48b7-b67c-a642d14b49b2","notation":["a24eff1e-d4d1-48b7-b67c-a642d14b49b2"],"prefLabel":{"de":"Richtig schreiben"},"definition":{"de":"Die Rechtschreibung oder auch Orthographie ist die Normierung des Schriftsystems. Bezugspunkt ist also eine externe Norm, die festlegt, welche der prinzipiell möglichen Schreibungen eines Wortes als die richtige gelten soll. Die maßgebende Instanz für die deutsche Rechtschreibung ist der Rat für deutsche Rechtschreibung (Amtliches Regelwerk). Da die Rechtschreibung auf regelhaften Strukturen beruht, kann sie auch eigenständig gut erkundet werden. Der Vorteil einer geregelten Schreibung wird vor allem in der schriftlichen Kommunikation sichtbar, denn die Rechtschreibung steht im Dienste des Lesenden, trägt also dazu bei, das Lesen zu erleichtern. Somit kann im Rechtschreibunterricht gleichermaßen das Schreiben und Lesen von eigenen Texten sowie das Üben von ausgewählten Rechtschreibphänomenen thematisiert werden. Du suchst ausgewählte Unterrichtsmaterialien für den Rechtschreibunterricht? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/04f9f59b-4de3-4a00-a917-80d491ae3f7d","notation":["04f9f59b-4de3-4a00-a917-80d491ae3f7d"],"prefLabel":{"de":"das-/dass-Schreibung"},"definition":{"de":"Die Unterscheidung von “das” und “dass” gilt als besonders fehleranfällig. Die beiden Wörter klingen gleich, haben aber unterschiedliche Funktionen: Während das “das” als Artikel, Relativum (“Er liest das Buch, das er so gerne mag.”) oder Demonstrativum (“Mir gefällt das.”) fungieren kann, tritt das “dass” lediglich als Subjunktion (“Sie kocht so gut, dass sich alle freuen.”) auf. Es gibt verschiedene Herangehensweisen, um die das- / dass- Schreibung zu üben. Hier kannst du stöbern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a57f710-725e-4328-8d41-3e43b1227ea4","notation":["0a57f710-725e-4328-8d41-3e43b1227ea4"],"prefLabel":{"de":"Groß- und Kleinschreibung"},"definition":{"de":"Während die Kleinschreibung als Normalfall gilt, gibt es im Deutschen eine Reihe von Wörtern, die mit einem Großbuchstaben (Majuskel) versehen werden, um sie für die Lesenden hervorzuheben: Überschriften, am Satzanfang, Eigennamen, Anredepronomen sowie Substantive und Substantivierungen. In der Schule spielt vor allem die satzinterne Großschreibung eine Rolle, also die Großschreibung von Nomen (wortartbasierter Ansatz) beziehungsweise die Großschreibung des Kerns einer Nominalgruppe (syntaxbasierter Ansatz). Dieser Themenbereich gilt als einer der größten Fehlerquellen der Rechtschreibung, daher findest du hier viel hilfreiches Material."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/65b390bf-5255-4045-9165-2f588479f529","notation":["65b390bf-5255-4045-9165-2f588479f529"],"prefLabel":{"de":"Laut-Buchstaben-Zuordnung"},"definition":{"de":"Die deutsche Schrift ist eine Alphabet- oder auch Buchstabenschrift. Buchstaben repräsentieren die Laute, eine 1:1 Zuordnung gibt es jedoch nicht. Denn beim genauen Hinsehen wird deutlich, dass eigentlich Phonemen Grapheme zugeordnet werden. Besonders ist beispielsweise die Verschriftlichung von Vokalen, denn diese können sowohl lang als auch kurz ausgesprochen werden (unterschiedliche Phoneme), werden aber mit dem gleichen Buchstaben/Graphem notiert. Daher haben sich verschiedene Ansätze etabliert, die die Schreibweise erklären: Der Wortansatz und Silbenansatz. Mehr dazu findest du hier in den Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0b604afb-1848-4f3f-945e-2d23cc2034c4","notation":["0b604afb-1848-4f3f-945e-2d23cc2034c4"],"prefLabel":{"de":"Doppelkonsonanten"},"definition":{"de":"Die Schreibung von Doppelkonsonanten oder auch Silbengelenkschreibung (z.B. “Wet-ter”) wird nicht gesprochen, sondern verdeutlicht die Aussprache des vorangegangenen Kurzvokals (gespannter Vokal). Für die Lesenden wird erkennbar, dass der Vokal kurz ausgesprochen wird und kennzeichnet die Grenze der beiden Silben - mit nur einem Konsonanten wäre dies nicht eindeutig zu erkennen. Es gibt im Unterricht verschiedene Möglichkeiten, dies zu thematisieren. Hier findest du eine Auswahl an tollen Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/27d33968-5dbe-480d-aa53-304dc33379bb","notation":["27d33968-5dbe-480d-aa53-304dc33379bb"],"prefLabel":{"de":"Vokale und Konsonanten"},"definition":{"de":"Die Unterscheidung zwischen Vokalen (Selbstlaute) und Konsonanten (Mitlaute) spielt in der Schule eine wichtige Rolle. Ersteres steht für die Buchstaben: A, E, I, O und U sowie die dadurch gebildeten Umlaute Ä, Ö und Ü. Alle weiteren Buchstaben werden als Konsonanten bezeichnet. Während bei der Artikulation der Vokale der Luftstrom frei entweichen kann, wird der Luftstrom bei der Artikulation von Konsonanten durch die sprechende Person behindert. In dieser Sammlung findest du einige tolle Materialien, die du zum Lehren und Lernen dieses Themas verwenden kannst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3f57de8c-866c-4c6b-a9cd-60b38c8e5dc4","notation":["3f57de8c-866c-4c6b-a9cd-60b38c8e5dc4"],"prefLabel":{"de":"Silbentrennendes h"},"definition":{"de":"Das silbentrennende h oder auch silbeninitiale h wird nicht gesprochen, es ist stumm. Es tritt auf, um in einem zweisilbigen Wort (z.B. “seen” -> “sehen”) die Silben eindeutig zu trennen, sodass diese nicht für einen Doppelvokal gehalten werden und das Lesen erleichtert wird. Du möchtest mehr über das Thema erfahren und Übungen dazu finden? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6187d849-c8cf-407a-aaf5-730cb98aad83","notation":["6187d849-c8cf-407a-aaf5-730cb98aad83"],"prefLabel":{"de":"ie-Schreibung"},"definition":{"de":"Die ie-Schreibung kennzeichnet die Aussprache eines langes Vokals (in einer offenen Silbe) im Gegensatz zur einfachen i-Schreibung, bei welcher der Vokal (in geschlossener Silbe) kurz gesprochen wird (z.B. “Winter” und “Wiese”). Um die ie-Schreibung zielführend zu lernen oder zu lehren, findest du hier einige tolle Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/676a0501-8e89-460e-8d9f-04c3dbaef4b7","notation":["676a0501-8e89-460e-8d9f-04c3dbaef4b7"],"prefLabel":{"de":"Umlaute und Diphthonge"},"definition":{"de":"Die Schreibung von Umlauten und Diphthongen spielt in der Schule ebenfalls eine wichtige Rolle. Ersteres beschreibt die Veränderung von den Buchstaben A, O, U zu den Umlauten Ä, Ö, Ü. Aus diesen Umlauten kann ein Diphthong gebildet werden. Der Dipthong/Doppellaut besteht aus zwei verschiedenen Vokalen innerhalb einer Silbe: z.B. “äu” (“Häuser”) sowie ei, au, eu usw. Im Unterricht werden diese Schreibungen “äu oder eu?” meist von verwandten Wörtern (“Haus”) abgeleitet. Du suchst hilfreiche Materialien für das Thema Umlaute und Diphthong? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8992d63e-6110-43f7-aa02-37647e6070d0","notation":["8992d63e-6110-43f7-aa02-37647e6070d0"],"prefLabel":{"de":"Doppelvokale"},"definition":{"de":"Unter dem Begriff Doppelvokale oder Doppelselbstlaute versteht man die Aneinanderreihung von zwei gleichen Vokalen. Verdoppelt werden können jedoch lediglich die Vokale: aa, ee und oo (z.B. “Haar”, “Boot”, “Beere”). Auch Umlaute werden nicht verdoppelt. In der Schule werden Wörter mit Doppelvokalen meist als Merkwörter thematisiert. Du brauchst ein paar Materialien für die Doppelvokale? Die findest du hier."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a6919031-0389-4ea3-8fd1-c42c5168aca7","notation":["a6919031-0389-4ea3-8fd1-c42c5168aca7"],"prefLabel":{"de":"Silben"},"definition":{"de":"Eine Silbe ist eine Einheit bestehend aus einem oder mehreren aufeinanderfolgenden Lauten. Jede Silbe verfügt über einen Silbenkern (Nukleus), in welchem ein Vokal oder Diphthong steht. Vor dem Silbenkern befindet sich der Anfangsrand (Onset), hinter dem Silbenkern der Endrand (Koda). Silbenkern und Endrand werden gemeinsam auch als Silbenreim bezeichnet. Ist der Anfangsrand unbesetzt, wird dies als nackte Silbe bezeichnet, ist er besetzt, spricht man von einer bedeckten Silbe. Ist der Endrand der Silbe leer, wird von einer offenen Silbe gesprochen, ist er besetzt, bezeichnet man dies als geschlossene Silbe. Viele deutsche zweisilbige Wörter folgen dem Muster betont-unbetont (Trochäus) und können daher in Voll- und Reduktionssilbe aufgeteilt werden. Die Einheit Silbe kann daher bei der richtigen Schreibung von Wörtern helfen. Wie? Hier findest du spannende Materialien, die beim Lernen und Lehren der Rechtschreibung helfen können."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e268ff6b-f8be-445a-aa2c-a57b021d9515","notation":["e268ff6b-f8be-445a-aa2c-a57b021d9515"],"prefLabel":{"de":"Dehnungs-h"},"definition":{"de":"Das Dehnungs-h oder auch silbenschließendes h ist stumm und dient als Dehnungszeichen zur (zusätzlichen) Verdeutlichung des langen/gedehnten Vokals in einer offenen Silbe. Oft stehen nach dem Dehnungs-h die Konsonanten l,m,n oder r (z.B. “wohnen”). In der Schule werden diese Wörter oft als Merkwörter thematisiert. Hier findest du viele hilfreiche Materialien zu dem Thema."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fa77844a-a904-47f4-80a9-579ca0d68edf","notation":["fa77844a-a904-47f4-80a9-579ca0d68edf"],"prefLabel":{"de":"s-Schreibung"},"definition":{"de":"Die s-Schreibung thematisiert die Schreibung der Buchstaben s, ss und ß. Wird der Vokal lang gesprochen (in offener Silbe) und der s-Laut ist stimmhaft, wird nur ein “s” (z.B. “Rose”) geschrieben. Ebenfalls kann bei langem Vokal (in offener Silbe) auch ein stimmloser s-Laut gesprochen werden, dann wird ein “ß” (z.B. “Maße”) verschriftlicht. Wird der Vokal hingegen kurz gesprochen (in geschlossener Silbe), ist der s-Laut stimmlos und wird “ss” (z.B. “fassen”) geschrieben. In einigen, meist südlichen Regionen kann die Unterscheidung zwischen stimmhaftem und stimmlosem s-Laut schwer fallen. Um dies zu üben, findest du hier viele tolle Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fe782ff4-e339-4492-99e1-5ac2317193cc","notation":["fe782ff4-e339-4492-99e1-5ac2317193cc"],"prefLabel":{"de":"Auslaute"},"definition":{"de":"Als Auslaut wird derjenige Laut bezeichnet, auf den ein Wort oder eine Silbe endet. Die Auslautverhärtung bezeichnet den Vorgang, dass stimmhafte Konsonanten am Ende der Silbe/des Wortes ihre Stimmhaftigkeit verlieren und somit stimmlos ausgesprochen werden (z.B. “Hund”). Um die richtige Schreibung herleiten zu können, werden im Unterricht oft Verlängerungen vorgenommen (“Hunde”), um den Konsonanten sicher identifizieren zu können: “d/t” , “b/p” , “g/k”. Um dieses Thema zu üben, gibt es viele tolle Materialien, die du hier finden kannst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/32799b02-159f-4ce2-a358-12ffa3d0dab9","notation":["32799b02-159f-4ce2-a358-12ffa3d0dab9"],"prefLabel":{"de":"Worttrennung am Zeilenende"},"definition":{"de":"Ist das Ende einer Zeile erreicht, kann ein mehrsilbiges Wort getrennt werden. So lässt sich der Platz für den selbstgeschrieben Text am besten nutzen. Doch wo genau wird getrennt? Gewöhnlich stimmen die Grenzen der Silben auch mit den Trennstellen überein. Du möchtest die Worttrennung am Zeilenende lernen oder lehren? Dann findest du hier viele hilfreiche Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3c3d51a5-fc8f-4ca6-926d-ae37d8a3f518","notation":["3c3d51a5-fc8f-4ca6-926d-ae37d8a3f518"],"prefLabel":{"de":"Bindestrichschreibung"},"definition":{"de":"Ein Bindestrich wird geschrieben, wenn Wortgruppen aneinandergereiht oder zusammengesetzt werden sowie bei der Zusammensetzung mit Buchstaben, Ziffern und Abkürzungen. Sollen bei der Zusammenfassung mehrerer Wörter Wortteile erspart werden, dann kann er auch als Ergänzungsstrich eingesetzt werden. Am Ende einer Zeile fungiert der Bindestrich außerdem auch als Trennstrich. Die Bindestrichschreibung kann den Lesefluss erheblich erleichtern. Du brauchst dafür Unterrichtsmaterial? Dann findest du hier ganz bestimmt etwas."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6660c406-e1e6-42e9-8113-d52bbea320b0","notation":["6660c406-e1e6-42e9-8113-d52bbea320b0"],"prefLabel":{"de":"Rechtschreibstrategien"},"definition":{"de":"Im Rechtschreibunterricht ist es nicht nur wichtig, die Regeln der Rechtschreibung zu wissen, sondern auch Strategien zur Herleitung einer richtigen Schreibung zu üben. Exemplarische Strategien sind u.a. das Verlängern \"Hund - Hunde\" oder das Ableiten \"Blatt - Blätter\"."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/98412501-86ec-44b2-8920-cb63fa29f1c3","notation":["98412501-86ec-44b2-8920-cb63fa29f1c3"],"prefLabel":{"de":"Getrennt- und Zusammenschreibung"},"definition":{"de":"Bei der Getrennt- und Zusammenschreibung gilt grundsätzlich die Frage, ob zwei oder mehr wortfähige Stämme gemeinsam ein Wort bilden oder ob es sich um mehrere Wörter (Bestandteile einer Wortgruppe) handelt. Eine Rolle spielt dabei beispielsweise die Kombination aus Substantiv und Substantiv. Entscheidend ist, ob das Substantiv eine eigene Bedeutung und syntaktische Funktion hat: “Er spielt Fußball.” und “Er spielt mit seinem Fuß Ball.” So kann die Frage nach der Getrennt- oder Zusammenschreibung auch bei Substantiv und Verb, Verb und Verb, Verb und Adjektiv u.v.m. auftauchen. Du brauchst Erklärungen und Übungen zu diesem Thema? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e126a8c7-fc46-4686-969b-5230f7732015","notation":["e126a8c7-fc46-4686-969b-5230f7732015"],"prefLabel":{"de":"Texte und Medien verstehen"},"definition":{"de":"In diesem Bereich soll es primär um die Fähigkeit gehen, Texte und weitere Medien zu lesen und zu verstehen. Damit ist in erster Linie nicht das Erkennen von Buchstaben, Wörtern und Sätzen oder das Vorlesen gemeint, sondern der erfolgreiche Umgang mit Texten und Medien. Also die Fähigkeiten, Inhalte zu verstehen, zu verarbeiten, kreativ weiterzuentwickeln und in anderen Handlungsfeldern anzuwenden. Insofern finden sich hier nicht nur Kategorien zu verschiedenen Textsorten oder zur Literaturgeschichte, sondern auch einzelne Bereiche zu Lesestrategien oder zum Umgang mit digitalen Medien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0eab78b5-747f-4b87-a861-7670990e0b92","notation":["0eab78b5-747f-4b87-a861-7670990e0b92"],"prefLabel":{"de":"Rhetorische Stilmittel"},"definition":{"de":"Rhetorische (Stil-)Mittel werden in Texten genutzt, um Inhalte auf besondere Weise zu präsentieren. Es geht also hierbei nicht darum, WAS dargestellt wird, sondern WIE etwas kommuniziert wird. Man findet Rhetorische Figuren, wie sie auch genannt werden, in allen möglichen Texten: Im Journalismus, in Gedichten, in Theaterstücken etc. Ihren Ursprung haben sie allerdings in der gesprochenen Sprache, denn sie wurden und werden genutzt, um Reden und Vorträge lebendig und anschaulich zu gestalten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2c8cd907-78b5-46f0-b3e7-7fe171633e44","notation":["2c8cd907-78b5-46f0-b3e7-7fe171633e44"],"prefLabel":{"de":"Lyrische Texte"},"definition":{"de":"Lyrische Texte sind in Verse und Strophen gegliedert und unterscheiden sich somit durch die äußerliche Form von Dramen und erzählenden Texten. Strophen bestehen aus Zeilen, die bestimmte Rhythmen aufweisen. Der jeweilige Rhythmus bedeutet eine besondere Betonung der Wörter. Lyrische Texte weisen oft eine bildhafte Sprache auf. Zur Lyrik gehören zum Beispiel das Gedicht oder die Ballade, aber auch das Lied. Informationen zum Lernen und Üben findest du hier."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/17991b28-2c5c-4a94-9db7-7c38278436b9","notation":["17991b28-2c5c-4a94-9db7-7c38278436b9"],"prefLabel":{"de":"Gedichtanalyse"},"definition":{"de":"Ein lyrischer Text kann vor dem Hintergrundwissen zu der entsprechenden Epoche analysiert werden und dies in Zusammenhang mit der Thematik in der Einleitung aufgreifen. Auch der Bezug zum Autor, zu weiteren geschichtlichen Ereignissen und die Bestimmung der Gedichtart gehören an den Anfang. Im Hauptteil geht es um den Inhalt und den Aufbau des Gedichts, um die Form und die Sprache des Gedichts gehen. Aus der Analyse von Inhalt, Form und Sprache kann dann die Aussage (Intention) des Gedichts erschlossen werden. Zentrale Aspekte wie beispielsweise das Lyrische Ich, Verse/ Reimschema/ Metrum, sprachliche Mittel etc. spielen hierbei eine wichtige Rolle. Zum Schluss sollten die Analyseergebnisse noch einmal grob zusammengefasst werden. Um noch mehr darüber zu erfahren und zu üben, kannst du auf dieser Seite nach hilfreichen Materialien stöbern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cc217ac0-ca5c-4b29-9824-24c9fcf20aa5","notation":["cc217ac0-ca5c-4b29-9824-24c9fcf20aa5"],"prefLabel":{"de":"Erzählende Texte"},"definition":{"de":"Unter erzählende oder narrative Texte, Erzähltexte oder auch Erzählungen fallen alle Texte, die von einer Erzählinstanz präsentiert werden. Sie sind in der Regel in Prosa geschrieben und grenzen sich so von der Versstruktur der Lyrik und der Dialogstruktur der Dramen ab. Erzählungen sind zum Beispiel Romane, Kurzgeschichten oder Novellen. Erzählungen können danach betrachtet werden, was sie präsentieren, die Histoire, oder wie präsentiert wird, der Discours. Die Forschung, die sich mit Erzähltexten beschäftigt, nennt sich Narrationstheorie. - Für \"Kinder- und Jugendliteratur\" gibt es eine eigene Untersammlung in der Sammlung \"Texte lesen und verstehen\"."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1d392eab-48b9-44ca-b126-fdab86fd14de","notation":["1d392eab-48b9-44ca-b126-fdab86fd14de"],"prefLabel":{"de":"Novelle"},"definition":{"de":"Die Novelle (lateinisch novus - neu) ist ein kurzer Prosatext, der sich vor allem durch ein inhaltliches Merkmal auszeichnet: Der zentrale Stoff des Textes ist eine Neuheit. Als Begründer der Novellentradition, die bis auf die italienische Renaissance zurückgeht, gilt Giovanni Boccaccio aufgrund des von ihm verfassten Decamerone."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/43514950-88f9-4eb7-a86a-e4544162e3b9","notation":["43514950-88f9-4eb7-a86a-e4544162e3b9"],"prefLabel":{"de":"Fabel"},"definition":{"de":"Die Fabel zeichnet sich als kurze Prosaerzählung dadurch aus, dass vor allem Tiere, aber auch Pflanzen oder Dinge personifiziert (vermenschlicht) werden und menschliche Konflikte erleben. Meist ergibt sich aus dem Schluss der Fabelhandlung eine allgemeingültige Moral, weswegen der Fabel ihre belehrende Absicht zugesprochen wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5cebb7bf-af1b-4915-bf50-64ebf761156e","notation":["5cebb7bf-af1b-4915-bf50-64ebf761156e"],"prefLabel":{"de":"Sage"},"definition":{"de":"Die kurze Erzählung Sage (ahd. saga - Gesagtes) basiert auf zunächst oral überlieferten Geschichten fantastischer Ereignisse. Da diese allerdings mit realen Begebenheiten, Personen- und Ortsangaben verbunden werden, kann der Eindruck eines wahrhaftigen Berichts entstehen. Verfasser*innen von Sagen sind in der Regel unbekannt. Sagen haben einen hohen kulturellen Wert, denn sie sind häufig mit dem Selbstverständnis von Völkern und Kulturen verbunden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/76e5e8b1-39a9-40df-bf97-315d05bd1949","notation":["76e5e8b1-39a9-40df-bf97-315d05bd1949"],"prefLabel":{"de":"Märchen"},"definition":{"de":"Prosatexte, die von wundersamen Begebenheiten erzählen, nennen wir Märchen. Es handelt sich dabei um eine alte kulturelle Tradition der mündlichen Überlieferung, weswegen Märchenerzählen als immatrielles Kulturerbe von der UNESCO-Kommission geführt wird. Im deutschsprachigen Raum stellt die Sammlung der Gebrüder Grimm ein einschneidendes Ereignis dar, da damit häufig ein festgeschriebener mono-medialer Text die oralen, vielfältigen Versionen ablöste. Märchen zeichnen sich durch phantastische Elemente wir Zauberer oder Fabeltiere aus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7fc339e6-95cb-4007-a405-2d559b45d1ac","notation":["7fc339e6-95cb-4007-a405-2d559b45d1ac"],"prefLabel":{"de":"Erzähltextanalyse"},"definition":{"de":"Erzählende/ Epische Texte lassen sich nicht nur wunderbar lesen, sondern in der Schule auch sehr gut analysieren. Meistens lässt sich gut eine bestimmte Textpassage oder ein übersichtliches Kapitel analysieren. Dabei können verschiedene Kriterien eine Rolle spielen, wie bspw. die Erzählperspektive, die Erzähltechnik, die Zeitform, die Figuren, die Handlung u.v.m. In dieser Sammlung kannst du noch viel mehr darüber erfahren und konkrete Materialien finden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b32b361d-2897-431b-841a-59a2a0a208ad","notation":["b32b361d-2897-431b-841a-59a2a0a208ad"],"prefLabel":{"de":"Roman"},"definition":{"de":"Was einen erzählenden Text zu einem Roman macht, ist nicht ganz unumstritten. Meist wird ein erzählender Text dann zu einem Roman, wenn es eine lange, ausführliche und fiktive Erzählung ist. Es gibt einen Erzähler und verschiedene Kapitel. Der Roman ist meist leicht verständlich und thematisiert Figuren in verschiedenen Handlungssträngen. Du möchtest mehr über den Roman und die Arbeit mit Romanen in der Schule erfahren? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b667f7a7-aae6-486e-b810-eb00cd93c3fe","notation":["b667f7a7-aae6-486e-b810-eb00cd93c3fe"],"prefLabel":{"de":"Kurzgeschichte"},"definition":{"de":"Als moderner Prosatext liegt das Hauptmerkmal der Kurzgeschichte in der Kürze. Der Begriff ist eine Lehnübersetzung der englischen Short Story. Im deutschsprachigen Raum erfuhr diese Textgattung vor allem durch die Bemühungen nach 1945 auf der Suche nach einer literarischen Stunde Null Aufmerksamkeit."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2a3fb08a-fcc3-47d9-bfb3-2bf367cb7a87","notation":["2a3fb08a-fcc3-47d9-bfb3-2bf367cb7a87"],"prefLabel":{"de":"Umgang mit Medien"},"definition":{"de":"Texte werden meist nicht mündlich, also oral, sondern medial vermittelt dargestellt, das heißt über ein Buch als literarischer Text oder auch über den Bildschirm als Film (weiter Text-Begriff). Medien sind dabei nicht neutral, sondern üben durch ihren bestimmten Aufbau Einfluss auf die Inhalte aus. In diesem Bereich findest du Material, das das Medium im Gegensatz zum Text oder Inhalt in den Mittelpunkt stellt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/131b383f-5c17-48a0-9530-76a6a16116ad","notation":["131b383f-5c17-48a0-9530-76a6a16116ad"],"prefLabel":{"de":"Dramatische Texte"},"definition":{"de":"Die Dramatik ist die dritte literarische Gattung neben der Epik und der Lyrik. Ein dramatischer Text ist in der Regel für die Aufführung auf der Bühne konzipiert. Er zeichnet sich vornehmlich durch die Erzähler*innenleerstelle aus - es gibt also keine*n Erzähler*in der Handlung, sondern die Handlung wird als Schauspiel präsentiert. Ein Drama muss nicht tragisch, sondern kann auch komisch oder absurd sein."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3951d005-03c0-4887-961d-b006e07db53f","notation":["3951d005-03c0-4887-961d-b006e07db53f"],"prefLabel":{"de":"Dramentheorie"},"definition":{"de":"Hier findest du viele hilfreiche Informationen und Materialien rund um Theorien über Form und Funktion des Dramas. Es gibt verschiedene Dramentheorien, wie die von Lessing, Brecht oder Dürrenmatt. Im Vordergrund steht immer die Frage, was für ein Drama charakteristisch ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ea224fa1-b2cc-4083-a81a-3ee8aa6d2db3","notation":["ea224fa1-b2cc-4083-a81a-3ee8aa6d2db3"],"prefLabel":{"de":"Dramenanalyse"},"definition":{"de":"Dramatische Texte eignen sich in der Schule sehr gut für eine Analyse. Analysiert wird meistens eine konkrete Szene hinsichtlich verschiedener Kriterien. Auf welche Art und in welchem Umfang sprechen die einzelnen Figuren, wie verläuft das Gespräch, was gibt es für eine Raumstruktur? Wie lassen sich die Figuren charakterisieren, was gibt es für Regieanweisungen u.v.m. Was alles in dramatischen Texten analysiert werden kann und wie das am besten funktioniert, kannst du gut mit den Materialien aus unserer Sammlung üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/58e66666-162e-4673-804f-a9961136311d","notation":["58e66666-162e-4673-804f-a9961136311d"],"prefLabel":{"de":"Literaturgeschichte"},"definition":{"de":"Literatur meint alle sprachlich fixierten Überlieferungen, wobei künstlerisch gestaltete Werke maßgeblich sind. In der Literaturgeschichte wird die Historie der Texte näher betrachtet und gemeinsame Merkmale den jeweiligen Entstehungszeiten in Epochen zugeordnet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4c5a5c49-00e5-4a6d-adde-a5f7a2deebe7","notation":["4c5a5c49-00e5-4a6d-adde-a5f7a2deebe7"],"prefLabel":{"de":"Literatur von 1949-1990"},"definition":{"de":"In der Bundesrepublik Deutschland thematisierte die direkte Nachkriegsliteratur noch die Schrecken des Krieges (siehe Trümmerliteratur). In den 1950er Jahren konzentrierte sich der Roman auf die aktuelle Zeit des Wirtschaftswunders. Neben dem Roman waren lyrische Texte, aber auch andere Medien wie Hörspiele, populär. In den 1960er Jahren verstärkte sich als Reaktion auf den Vietnamkrieg und die 68er-Bewegung die thematische Auseinandersetzung mit politischen Themen. In der DDR war die literarische Landschaft deutlich homogener als in der BRD und stark antifaschistisch orientiert. Besonders die direkte Nachkriegsliteratur strebte nach Auseinandersetzung und Aufarbeitung der vorhergehenden Zeit. Die literarische Auseinandersetzung in der DDR wurde zentral organisiert und verfolgte das Konzept des “Sozialistischen Realismus”, hatte also einen dezidierten Erziehungsauftrag hin zum Sozialismus.\nIn dieser Sammlung findest du Materialien der Literatur der BRD, der DDR und der deutschen Literatur aus der Schweiz und Österreich."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4ffe8231-7bc8-4e3e-a0f2-e444af38e932","notation":["4ffe8231-7bc8-4e3e-a0f2-e444af38e932"],"prefLabel":{"de":"Trümmerliteratur"},"definition":{"de":"Mit dem Ende des Zweiten Weltkrieges sprach man von einer literarischen “Stunde Null”, einem angestrebten Neuanfang, obwohl viele Strukturen fortbestanden. Die Strömung der “Trümmerliteratur” setzte sich direkt mit den Folgen des Weltkrieges auseinander und wird mit dem Zeitraum 1945-1950 beziffert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/61fb59cb-ff81-4e7f-9f23-af8da90d6b93","notation":["61fb59cb-ff81-4e7f-9f23-af8da90d6b93"],"prefLabel":{"de":"Naturalismus"},"definition":{"de":"Als Naturalismus bezeichnet man in der deutschen Literaturgeschichte eine Strömung in Literatur und Theater von etwa 1880 bis ins 20. Jahrhundert. Ziel des Schaffens ist eine naturgetreue Beobachtung und Darstellung auch gesellschaftlicher Phänomene. Der Naturalismus wendet sich von verklärender und interpretierender Dichtung ab und bemüht sich, auch gesellschaftlich Nichtbeachtetem Raum zu geben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/730b9a46-99dd-4f2a-a70d-64ff2ecdbc00","notation":["730b9a46-99dd-4f2a-a70d-64ff2ecdbc00"],"prefLabel":{"de":"Impressionismus"},"definition":{"de":"Die Literaturepoche des Impressionismus ist fachwissenschaftlich umstritten. Als Überbegriff sammelt der Impressionismus in der Literatur ebenso wie in der Malerei und Musik Werke, die von etwa 1890-1910 entstanden und die Momente subjektiv wiedergeben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/795706d6-cc43-4fd9-8c58-7132fd8927df","notation":["795706d6-cc43-4fd9-8c58-7132fd8927df"],"prefLabel":{"de":"Neue Sachlichkeit"},"definition":{"de":"Eine literarische Strömung der Weimarer Republik ist die Neue Sachlichkeit, deren zugerechnete Werke hauptsächlich zwischen den Weltkriegen entstanden. Als Reaktion auf den literarischen Expressionismus ist diese Strömung eine Hinwendung zur nüchternen Darstellung. Die Werke der Neuen Sachlichkeit greifen auf Tendenzen des Naturalismus zurück, wenn auch der natürlich “Anspruch” der Objektivität aufgegeben wurde."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8707ce13-3f56-4162-9456-66730dd2d422","notation":["8707ce13-3f56-4162-9456-66730dd2d422"],"prefLabel":{"de":"Barock"},"definition":{"de":"Als Barock wird in der deutschen Literaturgeschichte die Kunst und Literatur im Zeitraum zwischen etwa 1600 und 1720 bezeichnet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/90e5c9e1-7885-4c5f-95aa-0e5b71842f6b","notation":["90e5c9e1-7885-4c5f-95aa-0e5b71842f6b"],"prefLabel":{"de":"Klassik"},"definition":{"de":"Die (Weimarer) Klassik bezeichnet den Zeitraum, in dem die Schriftsteller Wieland, Goethe, Herder und Schiller in Weimar (und Jena) lebten. Der Anfang wird in der Regel mit Goethes Italienreise für diese Epoche angenommen - 1786. Goethes Tod 1805 gilt als Ende. Eine engere Definition der “Klassik” bezieht nur Goethe und Schiller und deren Schaffen in Beziehung zueinander von 1794-1805 ein."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/952647e6-793b-4f87-9357-6db0f7a2309c","notation":["952647e6-793b-4f87-9357-6db0f7a2309c"],"prefLabel":{"de":"Literatur der Gegenwart"},"definition":{"de":"Die Sammlung Literatur der Gegenwart beinhaltet Materialien der Gegenwartsliteratur. Damit ist demnach keine Epoche gemeint, sondern ein Sammelbegriff für Literatur seit der deutschen Wiedervereinigung. Inhaltlich werden größtenteils aktuelle Themen aufgegriffen. Die Gegenwartsliteratur will unterhalten und bilden und probiert verschiedene Stile aus. Hier findest du die Materialien für die Literatur ab den 1990-er Jahren sowie für die Literatur des 21. Jahrhunderts."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9622d8f9-5ea9-4be7-a455-a5976c7654fd","notation":["9622d8f9-5ea9-4be7-a455-a5976c7654fd"],"prefLabel":{"de":"Romantik"},"definition":{"de":"Die Romantik als Literaturepoche ging von etwa 1795-1848 und teilt sich in Frühromantik (bis 1804), Hochromantik (bis 1815) und Spätromantik (bis 1835). Die Romantiker*innen wendeten sich bewusst von der Antike ab und hin zu ihrer eigenen Kultur und Geschichte. Dies bedeutet formal häufig ein fragmentarisches Schreiben und inhaltlich ein Orientieren an Sagen und Mythen des Mittelalters."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9c279c09-c97f-41fe-8125-907a3c992a7d","notation":["9c279c09-c97f-41fe-8125-907a3c992a7d"],"prefLabel":{"de":"Realismus"},"definition":{"de":"Die Epoche des Realismus wird in Deutschland zwischen 1848 und 1890 angesiedelt. In abgewandelter Form wird auch von “bürgerlichem” oder “poetischem” Realismus gesprochen. Für die Epoche des Realismus ist besonders der Roman bedeutsam. Die Epoche ist vom Stil und der Norm des Realistischen, so objektiv wie möglich zu beschreiben, zu unterscheiden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aca968d3-4835-4581-b253-3f752372116d","notation":["aca968d3-4835-4581-b253-3f752372116d"],"prefLabel":{"de":"Biedermeier/ Vormärz und Junges Deutschland"},"definition":{"de":"Die Jahrzehnte vor der deutschen Märzrevolution sind durch unterschiedliche literarische Strömungen gekennzeichnet. So können Vormärz, Biedermeier und Junges Deutschland unterschieden werden. Unter Biedermeier wird die konservative und restaurative Literatur der Zeit gesammelt, der die Bewegung des Jungen Deutschland und des Vormärz entgegensteht. Besonders die Autor*innen des Vormärz strebten nach (Prosa-)Literatur, die sich an den gesellschaftlichen Gegebenheiten orientierte und von der Literatur der Klassik abwandte. Den Fortschritt forcierten auch die Autor*innen des Vormärz, wenn auch vor allem mit lyrischen Texten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c4dd8943-7b57-4e96-90b7-0ee91433a079","notation":["c4dd8943-7b57-4e96-90b7-0ee91433a079"],"prefLabel":{"de":"Weimarer Republik"},"definition":{"de":"Die Literatur der Weimarer Republik zeichnet sich durch Varianz aus. Der Expressionismus des Jahrhundertanfangs zog sich noch bis in die 1920er Jahre hinein. Die darauffolgende Zeit ist geprägt von realitätsbezogenen Darstellungen, die unter dem Namen der Neuen Sachlichkeit gesammelt werden. Gleichzeitig lässt sich die Literatur der Weimarer Republik nicht auf diese beiden Strömungen reduzieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c9333323-15bb-49fa-bac9-cd5bc3562a17","notation":["c9333323-15bb-49fa-bac9-cd5bc3562a17"],"prefLabel":{"de":"Exilliteratur"},"definition":{"de":"Unter Deutscher Exilliteratur wird die Literatur zur Zeit des Nationalsozialismus verstanden, die im Exil, also außerhalb Deutschlands oder in innerer Emigration, entstand. Zahlreiche Autor*innen wurden ins Ausland gedrängt oder flohen und litten sowohl psychisch als auch materiell unter der Situation. Im Ausland wurden Emigrantenzentren und Verlage gegründet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cf1b568b-112c-4253-9a64-7fec10887735","notation":["cf1b568b-112c-4253-9a64-7fec10887735"],"prefLabel":{"de":"Expressionismus"},"definition":{"de":"Der literarische Expressionismus (ca. 1905-1925) ist eine Abkehr vom reinen Beobachten und genauem Beschreiben, wie es für den Naturalismus noch galt. Das lateinische “ex” und “premere”, das “Ausdrücken” bedeutet, ist namengebend für die Strömung, in der es hauptsächlich darum geht, Gefühle auszudrücken. Diese können allerdings sehr wohl auf existentielle und gesellschaftsrelevante Themen ausgerichtet sein."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dd385d8b-fca3-47bc-ad64-214fcbbca272","notation":["dd385d8b-fca3-47bc-ad64-214fcbbca272"],"prefLabel":{"de":"Renaissance"},"definition":{"de":"Die Renaissance bezeichnet eine von Italien im 15. Jhd. ausgehende Hinwendung zur sinnlich erfahrbaren Wirklichkeit, die sich an der “Wiedergeburt” der Antike orientiert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e434ef05-e1b5-4730-ab17-26bec189a418","notation":["e434ef05-e1b5-4730-ab17-26bec189a418"],"prefLabel":{"de":"Postmoderne"},"definition":{"de":"Die literarische Strömung der Postmoderne beginnt in den 1950er Jahren. Ob sie bereits beendet ist, oder noch andauert, ist umstritten. Ebenso ist unklar, was genau unter Postmoderne zu verstehen ist, da es keine genau theoretische Bestimmung gibt. Die Postmoderne ist gekennzeichnet durch den Rückgriff auf die Moderne (namengebend), die Abkehr von Sinnhaftigkeit (Zeichen verweisen auf Zeichen), fragmentarische Formen und Multiperspektivität."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f5ddcca1-5868-40cc-a4f5-29fd1f8e1450","notation":["f5ddcca1-5868-40cc-a4f5-29fd1f8e1450"],"prefLabel":{"de":"Sturm und Drang"},"definition":{"de":"Der Sturm und Drang bezeichnet eine revolutionäre Strömung der deutschen Literatur in der Epoche der Aufklärung von ca 1765 bis 1785. Die Strömung ist benannt nach dem gleichnamigen Drama von Friedrich Maximilian Klinger."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f7b13ebc-e389-497a-accf-3a19a1bee6dd","notation":["f7b13ebc-e389-497a-accf-3a19a1bee6dd"],"prefLabel":{"de":"Moderne"},"definition":{"de":"In der Jahrhundertwende 1800/1900 wird der Beginn der literarischen Moderne gesehen, in deren Zentrum das Experimentieren mit neuen literarischen Techniken steht. Kennzeichen moderner Romane sind beispielsweise erlebte Rede, Perspektivenwechsel oder das Zurücktreten einer Erzählinstanz. Die Romantik gilt partiell als inhaltliche Vorgängerin der Moderne, da hier bereits Anzeichen wie die Abkehr von antiken Stoffen zu finden sind. Als moderne Bewegungen verstehen sich insbesondere die Naturalist*innen, die Expressionist*innen, die Wiener Moderne sowie die Dekadenz. Die Moderne endete etwa 1920."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fa711361-4df7-4dd8-bb1e-3cf07710cca7","notation":["fa711361-4df7-4dd8-bb1e-3cf07710cca7"],"prefLabel":{"de":"Aufklärung"},"definition":{"de":"Zur Aufklärungsliteratur werden allgemein Texte zugeordnet, die zwischen 1720 und 1800 entstanden sind und die Ideen des Zeitalters der Aufklärung, der Vernunft, vertreten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/60696691-6305-4c19-98dd-a52027093134","notation":["60696691-6305-4c19-98dd-a52027093134"],"prefLabel":{"de":"Lesestrategien"},"definition":{"de":"Lesestrategien helfen dabei, dass sich die Lesenden einen Text erfolgreich erschließen können. Unterschieden werden können die Strategien des “überfliegenden Lesens”, wobei es nur um das selektive Erfassen der wichtigsten Daten geht, und Strategien zur intensiveren Aneignung der Inhalte. Hier findest Du Informationen und Übungen zum Lesen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8818e408-0278-4e7d-874e-b38d6b5039b6","notation":["8818e408-0278-4e7d-874e-b38d6b5039b6"],"prefLabel":{"de":"Sachtexte"},"definition":{"de":"Unter einem pragmatischen oder auch logischen, funktionalen, expositorischen, Gebrauchs- oder Sachtext wird jeder Text verstanden, der primär Fakten liefert. Diese Informationstexte sind objektiv und neutral, um nicht vom Zweck des Textes abzulenken. Häufig wird Fachsprache verwendet. Die Bandbreite pragmatischer Texte reicht vom wissenschaftlichen Aufsatz über den Zeitungsbericht bis zum journalistischen Kommentar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/93efc136-a10c-44d0-bf86-22c8ffdb0197","notation":["93efc136-a10c-44d0-bf86-22c8ffdb0197"],"prefLabel":{"de":"Sachtextanalyse"},"definition":{"de":"Einen Sachtext zu analysieren bedeutet, sich die Hauptaussage des Textes, die Struktur des Textes, den Inhalt des Textes und die Sprache des Textes anzuschauen. Eine Sachtextanalyse sollte kohärent und stringent formuliert sein, d.h. über eine eigene Einleitung verfügen, einen Hauptteil mit der Analyse der wichtigsten Aspekte und ein Fazit mit einer abschließenden Zusammenfassung. Dabei gibt es verschiedene Strategien, wie eine Sachtextanalyse am besten gelingen kann. In dieser Sammlung erfährst du alles wichtige über die Sachtextanalyse und erhältst verschiedene Tipps, um eine gelungene Sachtextanalyse zu verfassen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/62c39719-86e6-4d8e-9974-81ec6eba0457","notation":["62c39719-86e6-4d8e-9974-81ec6eba0457"],"prefLabel":{"de":"Kinder- und Jugendliteratur"},"definition":{"de":"Diese Sammlung umfasst Materialien zu Texten, die im Deutschunterricht der Primarstufe oder der Sekundarstufe I unter dem Begriff \"Ganzschrift\" oder dergleichen behandelt werden. Es geht um Materialien sowohl zu den Texten, zu deren Behandlung im Unterricht als auch um Verfilmungen und vielem mehr."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f40fd390-607f-4a9b-9f80-f0c262dc61f7","notation":["f40fd390-607f-4a9b-9f80-f0c262dc61f7"],"prefLabel":{"de":"Lesepfade"},"definition":{"de":"Wird in der Schule gemeinsam ein Buch gelesen, werden dazu oft verschiedene Übungen durchgeführt. Lesepfade bieten eine interessante und moderne Möglichkeit für Kinder, sich eigenständig mit einem Buch auseinanderzusetzen. Sie können dabei in ihrem eigenen Tempo verschiedene interaktive Übungen bearbeiten und selbstständig kontrollieren. Viel Spaß beim Stöbern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f244026d-aceb-4d24-abd6-60d9d4c91b4c","notation":["f244026d-aceb-4d24-abd6-60d9d4c91b4c"],"prefLabel":{"de":"Kommunizieren"},"definition":{"de":"“Wir können nicht nicht kommunizieren” - damit meinte der Philosoph Paul Watzlawick, dass wir immer eine Botschaft senden, mit allem, was wir tun. Unter “Kommunikation” wird die Verständigung auf Basis von Zeichen verstanden. Wir kommunizieren mit Worten in der gesprochenen Sprache, können uns aber auch mit Mimik, Gestik oder Körpersprache verständlich machen. In diesem Bereich geht es um diese verschiedenen zeichenbasierten Kommunikationsformen, aber auch allgemein um Kommunikationsmodelle und unsere Zeichen Benutzung in der Kultur. Außerdem kannst du Arbeitsmaterialien zum Präsentieren und Vorlesen hier finden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1006c4ea-c5d1-4429-94bd-e389c9d48ad9","notation":["1006c4ea-c5d1-4429-94bd-e389c9d48ad9"],"prefLabel":{"de":"Mimik, Gestik und Körpersprache"},"definition":{"de":"Schriftsprache und gesprochene Sprache machen zwar einen Großteil unserer Kommunikation aus, jedoch darf der Einfluss der nonverbalen Kommunikation auf unsere Umwelt nicht unterschätzt werden. Unter “nonverbaler” oder “adverbialer Kommunikation” verstehen wir die Zeichen, die wir über unser Gesicht (“Mimik”), unsere Hände (“Gestik”) und unsere Körpersprache senden. Gestik ist dabei abzugrenzen von Gebärdensprachen, die als eigenständige, vollwertige Sprachen gelten und anders als Gesten keine gesprochenen Worte unterstützen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/599a6873-09f6-4d7b-bd83-77efe3d72b10","notation":["599a6873-09f6-4d7b-bd83-77efe3d72b10"],"prefLabel":{"de":"Kommunikationsmodelle"},"definition":{"de":"Kommunikationsmodelle stellen Verständigungsprozesse abstrahiert dar. Sie versuchen allgemein zu verdeutlichen, wie Kommunikation abläuft oder auch ablaufen kann. Zu den bekannteren Kommunikationsmodellen gehören zum Beispiel das Sender-Empfänger-Modell oder auch das 4-Ohren-Modell. In diesem Bereich werden gleichzeitig auch Informationen zur bedürfnisorientierten Lehrerschaft wie der Gewaltfreien Kommunikation gesammelt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8fb3b39d-9e22-4c7d-9261-563c053ff28d","notation":["8fb3b39d-9e22-4c7d-9261-563c053ff28d"],"prefLabel":{"de":"Präsentieren und Vorlesen"},"definition":{"de":"Ein Referat in der Schule zu halten, eine Rede auf dem Geburtstag der Oma oder einen Vortrag in der Uni, hängt nicht nur von dem Inhalt ab. Für das erfolgreiche Präsentieren oder Vorlesen gibt es nicht nur rhetorische - also “redekünstlerische”- Tipps, sondern auch Tricks, Handouts anschaulich zu gestalten oder die Powerpointpräsentation nicht zu überfrachten. Hier findest du Material rund um Vorträge und Präsentationen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fe116741-e33a-45ad-8b21-2320f93306e5","notation":["fe116741-e33a-45ad-8b21-2320f93306e5"],"prefLabel":{"de":"Gesprächskultur"},"definition":{"de":"Wie führen wir Gespräche? An das Bewerbungsgespräch haben wir andere Erwartungen als an das Gespräch mit dem besten Freund. Diese und andere Kommunikationssituationen unterliegen allerdings gesellschaftlichen Codes - also unausgesprochenen Verhaltensregeln-, denen wir hier auf den Grund gehen wollen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/55b795c1-50ac-483e-a707-e387a57e3661","notation":["55b795c1-50ac-483e-a707-e387a57e3661"],"prefLabel":{"de":"Anfangsunterricht"},"definition":{"de":"Um Kindern zu Beginn der Schulzeit den Zugang zur Schriftsprache zu ermöglichen, beginnen viele Lehrende mit dem Schreiben und Lesen erster Buchstaben, Wörter und kurzer Sätze. Hier gibt es viele verschiedene Herangehensweisen wie beispielsweise das Arbeiten mit Fibeln, Anlauttabellen sowie mit weiteren offenen Methoden. Hier findest du einige hilfreiche Informationen und Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2defd71f-3bf0-4dac-9303-720c602f0b20","notation":["2defd71f-3bf0-4dac-9303-720c602f0b20"],"prefLabel":{"de":"Erstes Schreiben"},"definition":{"de":"Um das Schreiben erster Buchstaben, Wörter und Sätze zu lernen, spielen viele wichtige Aspekte eine Rolle, wie u.a. die Motorik und Kognition. Hier findest du viele tolle Materialien, um das Schreiben erster Buchstaben, Wörter und Sätze zu üben. Das Üben des Lesens hängt in den meisten Fällen unmittelbar damit zusammen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f7b82856-226a-4766-826b-11e33bde8468","notation":["f7b82856-226a-4766-826b-11e33bde8468"],"prefLabel":{"de":"Erstes Lesen"},"definition":{"de":"Um das Lesen erster Wörter und Sätze zu lernen, spielen viele wichtige Aspekte eine Rolle, wie u.a. die Kognition sowie verschiedene erste Lesestrategien, wie z.B. das Lautlesen. Hier findest du viele tolle Materialien, um das Lesen erster Wörter und Sätze zu üben. Das Üben des Schreibens hängt in den meisten Fällen unmittelbar damit zusammen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/69756a85-a71e-4790-8ed0-6c11bb56aa1d","notation":["69756a85-a71e-4790-8ed0-6c11bb56aa1d"],"prefLabel":{"de":"Zeichen setzen"},"definition":{"de":"Die Zeichensetzung wird auch Interpunktion genannt und thematisiert das Setzen bestimmter Satzzeichen in Sätzen (z.B. Komma oder Doppelpunkt), um syntaktische Strukturen zu veranschaulichen. Auch das Setzen von Wortzeichen in Wörtern (z.B. Apostroph), um die morphologischen Strukturen zu verdeutlichen, wird dazugezählt. \nDiese Grenz- und Gliederungszeichen helfen, einen Text übersichtlich zu gestalten. In dieser Sammlung findest du viele tolle Materialien, mit denen das Thema Zeichensetzung gut gelehrt und gelernt werden kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3324512a-dabe-4a9e-a2d6-4bb0bceaac1a","notation":["3324512a-dabe-4a9e-a2d6-4bb0bceaac1a"],"prefLabel":{"de":"Apostroph"},"definition":{"de":"Das Apostroph ist ein Satzzeichen, das in erster Linie in einem Wort bestimmte Auslassungen markiert (“das ist ‘ne gute Idee”) sowie in Ausnahmefällen den Genitiv eines Substantivs (“Jan Keiss’ Schuhe”) und Ableitungen von Namen, die mit -sch gebildet werden (“Schubert’sche Lieder”) kennzeichnet. Hier gibt es viele spannende Materialien zum Thema Apostroph."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/43555deb-5e17-4932-bcde-8b184fe9c8a2","notation":["43555deb-5e17-4932-bcde-8b184fe9c8a2"],"prefLabel":{"de":"Gedankenstrich"},"definition":{"de":"Ein Gedankenstrich steht häufig dort, wo in der gesprochenen Sprache eine deutliche Pause gemacht wird. Manchmal taucht der Gedankenstrich auch zweifach auf (als Paar), um einen Zusatz in den Satz einzuschieben. Anstelle des Gedankenstrichs kann manchmal alternativ auch ein Komma oder eine Klammer stehen. Du brauchst für dieses Thema ausgewählte Materialien? Dann bist du hier genau richtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7460be5c-2fd5-4644-b782-3e28a23d310e","notation":["7460be5c-2fd5-4644-b782-3e28a23d310e"],"prefLabel":{"de":"Doppelpunkt und Semikolon"},"definition":{"de":"Der Doppelpunkt steht beispielsweise vor Aufzählungen, einem Zitat oder der wörtlichen Rede und kann auch eine Ausführung oder Zusammenfassung des vorher Geschriebenen einleiten. Das Semikolon, oder der “Strichpunkt”, hingegen verbindet zwei gleichrangige Sätze oder Wortgruppen miteinander. Es ist somit schwächer als ein Punkt, trennt jedoch die Bestandteile stärker als ein Komma. Zum Lernen und Lehren dieses Themas findest du hier einige tolle Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/97e2fa84-ca92-4ba2-911c-7e318c635c69","notation":["97e2fa84-ca92-4ba2-911c-7e318c635c69"],"prefLabel":{"de":"Komma"},"definition":{"de":"Als Satzzeichen hat das Komma die Funktion, einen orthographischen Satz zu strukturieren und somit einzelne Bestandteile voneinander abzugrenzen. So können beispielsweise Haupt- und Nebensätze abgegrenzt werden sowie Aufzählungen und besondere Herausstellungen. Auch das Abgrenzen von Verbalgruppen kann bei der Vermittlung der Kommasetzung eine wichtige Rolle spielen. Das Komma hilft vor allem dabei, einen Satz besser zu verstehen: “Die Kinder spielen jeden Tag im Turnraum entdecken sie zahlreiche Spielmöglichkeiten.” Manchmal rettet das Komma auch Leben: “Wir essen jetzt Oma.” Du möchtest ausgewählte Übungen durchstöbern? Los geht’s!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b1117a88-226d-416a-a6b9-4e4982823bf0","notation":["b1117a88-226d-416a-a6b9-4e4982823bf0"],"prefLabel":{"de":"Direkte und indirekte Rede"},"definition":{"de":"Um beim Schreiben und Lesen von Texten die direkte von der indirekten Rede unterscheiden zu können, wird die direkte oder auch wörtliche Rede in Anführungszeichen gesetzt. Auch der Doppelpunkt und das Komma spielen dabei eine Rolle. Es wird also ein Gedanke oder eine Rede direkt im Wortlaut wiedergegeben. Der Begleitsatz der wörtlichen Rede kann vorangestellt oder nachgestellt sein: 1. Mohammed sagt: “Mir geht es gut.” oder 2. “Mir geht es gut”, sagt Mohammed. Hier findest du einige hilfreiche Materialien, die zu diesem Thema bearbeitet werden können."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dec1042f-ca5e-4b26-a2f3-423160464946","notation":["dec1042f-ca5e-4b26-a2f3-423160464946"],"prefLabel":{"de":"Satzschlusszeichen"},"definition":{"de":"Satzschlusszeichen kennzeichnen das Ende eines Satzes. Das kann ein Punkt, ein Ausrufezeichen oder ein Fragezeichen sein. Neben dem “normalen” Punkt, der bei Aussagesätzen verwendet wird, wird ein Fragezeichen bei Fragewörtern und Fragesätzen und ein Ausrufezeichen beispielsweise bei Aufforderungssätzen, Warnungen, Wünschen oder Grüßen eingesetzt. Um das Lernen oder Lehren zu erleichtern, findest du hier einige Materialien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8b3b171d-11be-4a50-9042-b435ee50251e","notation":["8b3b171d-11be-4a50-9042-b435ee50251e"],"prefLabel":{"de":"Projektideen"},"definition":{"de":"Hier sammeln wir Ideen für Projekte, die sich im Fach Deutsch oder fächerübergreifend anbieten. Die hier zu findenden Materialien wurden von der Fachredaktion Deutsch von WirlernenOnline ursprünglich für die Portalseite zusammengetragen. Suchst Du Inspiration für ein Projekt? Schau dich gern um!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e5f2fbcf-0609-49fc-9025-b882a06e72ef","notation":["e5f2fbcf-0609-49fc-9025-b882a06e72ef"],"prefLabel":{"de":"Jahreszeiten"},"definition":{"de":"Tolle Materialien zu den Jahreszeiten gesucht? Vor allem zum Herbst wirst du hier fündig. Gern kannst du uns auch passendes Material von dir einsenden, um unsere Sammlung zu erweitern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/18c4b29c-2062-4d7f-b4d7-0ddabd05d150","notation":["18c4b29c-2062-4d7f-b4d7-0ddabd05d150"],"prefLabel":{"de":"Winter"},"definition":{"de":"Es ist wieder Winter und du suchst dafür passende Materialien für das Fach Deutsch? Alles zum Thema Winter, auch fächerübergreifend, findest du hier. Viel Spaß beim Stöbern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1b52e630-567c-420f-869b-1b31822fa620","notation":["1b52e630-567c-420f-869b-1b31822fa620"],"prefLabel":{"de":"Sommer"},"definition":{"de":"Die Sommerferien stehen vor der Tür und die Sonne scheint schon jetzt bis in das Klassenzimmer hinein? Bringe das Thema Sommer in den Deutschunterricht deiner Klasse."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8dfb199e-af0c-46df-9798-a6755603cab7","notation":["8dfb199e-af0c-46df-9798-a6755603cab7"],"prefLabel":{"de":"Frühling"},"definition":{"de":"Du merkst, dass es Frühling wird, wenn es draußen langsam wieder sonnig und warm wird. Die Tage werden länger, es bleibt hell - es wird bunt, die Blumen beginnen zu blühen und die Bäume werden wieder grün. Die Menschen werden aktiv und die Natur wacht auf. Du möchtest diese Frühlingsgefühle auch in der Schule erleben und suchst dafür passende Unterrichtsmaterialien? Dann bist du hier genau richtig! Für den Deutschunterricht gibt es viele tolle Unterrichtsmaterialien zum Thema Frühling, die sich auch sehr gut für eine fächerübergreifende Projektarbeit eignen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/da684986-4dbc-42ff-87e5-e3d724e26cb1","notation":["da684986-4dbc-42ff-87e5-e3d724e26cb1"],"prefLabel":{"de":"Herbst"},"definition":{"de":"Du suchst Unterrichtsmaterialien passend zur Jahreszeit Herbst? Dann bist du hier genau richtig! Für den Deutschunterricht gibt es beispielsweise tolle Arbeitsblätter und Übungen sowie auch Bastelvorlagen, die sich darüber hinaus auch hervorragend für eine fächerübergreifende Projektarbeit einsetzen lassen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3f978e12-4641-44c4-aa26-707b6cf079c0","notation":["3f978e12-4641-44c4-aa26-707b6cf079c0"],"prefLabel":{"de":"Lesetipps"},"definition":{"de":"Die Fachredaktion Deutsch hat Lesetipps gesammelt. Ursprünglich gedacht für den anstehenden Sommerurlaub, finden sich hier nun Materialien, in denen es nicht unbedingt um Sommer oder um Urlaub geht, die aber von uns den Stempel \"lesenswert\" bekommen haben. Viel Spaß beim Stöbern!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4f4c59c9-813c-4c2f-8715-ac5902c8a1a1","notation":["4f4c59c9-813c-4c2f-8715-ac5902c8a1a1"],"prefLabel":{"de":"Feste"},"definition":{"de":"Auf dieser Seite sammeln wir Unterrichtsmaterialien zu verschiedenen Festen wie Ostern oder Weihnachten. Diese können im Deutschunterricht, aber auch fachübergreifend eingesetzt werden. Sie dienen der Information, eignen sich zum Lesen, Üben, für Projekte etc. und können bei Bedarf zur Einstimmung auf ein Fest oder zur Auseinandersetzung damit genutzt werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6a5862d3-87bd-46b0-ac68-a10faa880e58","notation":["6a5862d3-87bd-46b0-ac68-a10faa880e58"],"prefLabel":{"de":"Weihnachten"},"definition":{"de":"Die Adventszeit, der Nikolaustag, das Weihnachtsfest und der Jahreswechsel machen den Monat Dezember zu einem ganz besonderen. Diese besinnliche Stimmung wirkt sich natürlich auch auf den Deutschunterricht in der Schule aus. Hier findest du viele tolle Materialien rund um das Thema Weihnachten im Deutschunterricht, die sich auch großartig für eine fächerübergreifende Projektarbeit einsetzen lassen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ed0e4894-b494-430f-bb7b-f62031d3c4e9","notation":["ed0e4894-b494-430f-bb7b-f62031d3c4e9"],"prefLabel":{"de":"Ostern"},"definition":{"de":"Ostern steht vor der Tür und du möchtest in der Schule ein Projekt dazu starten? Dann bist du hier genau richtig. Hier gibt es viele tolle Materialien zum Osterfest, die sich auch für den fachübergreifenden Unterricht wunderbar eignen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cffaadfb-5c07-4e9c-919c-0a53b862c89b","notation":["cffaadfb-5c07-4e9c-919c-0a53b862c89b"],"prefLabel":{"de":"Grammatik und Sprache untersuchen"},"definition":{"de":"Das Wort Grammatik kommt aus dem Altgriechischen  [gramma] und bedeutet übersetzt Buchstabe oder Geschriebenes.  Heute verstehen wir unter Grammatik die Lehre von Wörtern und Sätzen, und wie man Wörter und Wortformen in einer Reihenfolge im Satz verwendet. Die Grammatik ist das Regelsystem einer Sprache. In dieser Sammlung findet ihr Materialien zu Wortbildung und zum Satzbau."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/96aca699-ebc0-418c-bfc6-e08f44d388f4","notation":["96aca699-ebc0-418c-bfc6-e08f44d388f4"],"prefLabel":{"de":"Satzglieder"},"definition":{"de":"Die wesentlichen Satzglieder im Deutschen sind Subjekt, Objekt  und Prädikat. Es gibt aber noch viele andere Komponenten, die in einem Satz stehen können, wie z.B. Pronomen oder Adverbialbestimmung. Im Deutschen ist außerdem wichtig, wie diese Satzglieder in einem Satz angeordnet werden. Diese Materialien helfen dabei, Satzglieder und deren Funktionen zu lernen und zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/34a155be-df78-491e-a552-66edf64a3f1a","notation":["34a155be-df78-491e-a552-66edf64a3f1a"],"prefLabel":{"de":"Attribut"},"definition":{"de":"Ein Nomen lässt sich mit einem Attribut oder einem Attributsatz näher bestimmen. Ein Attribut kann verschiedene Formen haben, es kann ein Adjektiv sein: “der glänzende Blindenstock”, ein Genitivattribut: “der Rollstuhl der Frau”, ein Präpositionalattribut: “die Brille von dem Mann”, oder als Apposition:  “Emily hat einen Hund, einen vierjährigen Hund.” Ein Attribut kann auch in Form eines Attributsatzes sein z.B. als Relativsatz: “Der Hund, der der Frau gehört.” Dieses Material beinhaltet Erklärungen, Arbeitsblätter und Videos zu Attributen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/379190a4-2eb3-4f51-ab1c-a80a9a261cc7","notation":["379190a4-2eb3-4f51-ab1c-a80a9a261cc7"],"prefLabel":{"de":"Prädikat"},"definition":{"de":"Das Prädikat ist der Kernbestandteil eines Satzes. Es macht eine Aussage über das Subjekt des Satzes, wie z.B. “Die Katze schläft.” Vom Prädikat hängen mehrere Satzglieder ab, wie. z.B das Objekt: “Die Katze schläft auf dem Sofa”.  Das Prädikat wird aus einem Verb mit einer Endung, einem konjugierten Verb, gebildet und steht im Deutschen immer auf Platz zwei eines Hauptsatzes. Diese Materialien helfen dir, die verschiedenen Verbarten und ihre Anwendung im Satz kennenzulernen und zu vertiefen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a35a695a-402d-4ba8-92be-b7807e4cc535","notation":["a35a695a-402d-4ba8-92be-b7807e4cc535"],"prefLabel":{"de":"Objekt"},"definition":{"de":"Ein Objekt im Satz ist eine Ergänzung zum Prädikat. Das Objekt kann ein Akkusativ-,Dativobjekt, Genitivobjekt  oder eine präpositionale Ergänzung sein. Das Objekt bezeichnet einen Gegenstand, das Ziel einer Handlung oder Denkens, z.B. “Ich denke an dich. Ich esse einen Salat”, oder das Objekt als präpositionale Ergänzung: “Der Zug fährt um 15 Uhr ab.”\nHier findest du Materialien mit Erklärungen und Übungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c232ca9e-4dba-4972-a350-70140d36c72b","notation":["c232ca9e-4dba-4972-a350-70140d36c72b"],"prefLabel":{"de":"Adverbiale Bestimmung"},"definition":{"de":"Adverbiale Bestimmungen sind Adverbien, die als Satzglieder zusätzliche Angaben im Satz machen. Adverbiale Bestimmungen geben Antwort auf die Fragen: \nWann? --> Angabe der Zeit --> Temporaladverbiale\nWo? --> Angabe des Ortes --> Lokaladverbiale\nWarum? --> Angabe des Grundes --> Kausaladverbiale\nWie? --> Angabe der Art und Weise --> Modaladverbiale\nIm Deutschen muss man auf die Satzstellung dieser Adverbien achten, weil es besondere Regeln gibt. Diese Materialien helfen dir für den Unterricht mit Erklärungen und Übungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fee5b560-4390-46c5-abf4-379ef5572cb7","notation":["fee5b560-4390-46c5-abf4-379ef5572cb7"],"prefLabel":{"de":"Subjekt"},"definition":{"de":"Das Subjekt ist ein Satzglied;  kann eine Person, ein Ort, eine Sache oder eine Idee sein, die tut oder ist. Das Subjekt wird manchmal als \"Benennungsteil\" eines Satzes bezeichnet. Es macht eine Angabe darüber, worum es in dem Satz geht oder wer oder was eine Aktion im Satz ausführt. Das Subjekt ist meistens Kern einer Nominalgruppe."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6a03cfed-26f8-4936-8ca1-5ef36a81e795","notation":["6a03cfed-26f8-4936-8ca1-5ef36a81e795"],"prefLabel":{"de":"Wortarten"},"definition":{"de":"Jedes Wort, das du in einem Satz benutzt, ist einer besonderen Wortart zugeordnet. Es gibt z.B. Nomen, Verben, Präpositionen und Adverbien. In diesem Themenbereich findest du Materialien für verschiedene Wortarten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4344259b-f594-454f-aea3-1d183560cdcc","notation":["4344259b-f594-454f-aea3-1d183560cdcc"],"prefLabel":{"de":"Adverbien"},"definition":{"de":"Adverbien bestimmen eine Tätigkeit, einen Vorgang oder einen Umstand und beziehen sich somit auf Verben. Adjektive hingegen beziehen sich auf Substantive oder Pronomen. Adverbien sind unveränderliche Wörter und bleiben immer in der Grundform. Beispiele für Adverbien sind z.B. “ der Blinde läuft sicher auf der Straße” oder “die Leute bleiben abends zuhause”. Diese Materialien helfen dir den Unterschied zwischen Adverbien und Adjektiven zu verstehen und diese Satzglieder zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6c9f12c8-350b-41fd-b864-8d19e3565852","notation":["6c9f12c8-350b-41fd-b864-8d19e3565852"],"prefLabel":{"de":"Nomen"},"definition":{"de":"Ein Nomen (pl. Nomina) ist ein Substantiv und bezeichnet ein Ding, ein Lebewesen oder einen Sachverhalt. Ein Nomen hat einen Artikel im Deutschen. Auf dieser Seite findest du Erklärungen und Aufgaben zu Nomina bzw. Substantiven."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6d696588-0740-4767-9fb2-66476af6af84","notation":["6d696588-0740-4767-9fb2-66476af6af84"],"prefLabel":{"de":"Konjunktionen"},"definition":{"de":"Konjunktionen heißen auch Bindewörter, weil sie Wörter, Wortteile oder ganze Sätze miteinander verbinden. Das häufigste Bindewort im Deutschen ist ‘und’, man kann aber auch Satzteile mit ‘aber’, ‘oder’, ‘sondern’, ‘wenn’, ‘weil’ und vielen anderen Konjunktionen verbinden. Wenn du mehr darüber lernen willst, kannst du mit diesen Materialien arbeiten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8c1c5d9c-0b32-4f51-a9b2-70fb05ea1df6","notation":["8c1c5d9c-0b32-4f51-a9b2-70fb05ea1df6"],"prefLabel":{"de":"Adjektive"},"definition":{"de":"Adjektive nennt man auch Eigenschaftswörter, weil sie Eigenschaften von Substantiven oder Pronomen beschreiben, sie geben Antwort auf die Frage “wie?”,  z.B. “Kennst du das kranke Kind?”. Adjektive tragen Endungen, die vom Substantiv oder Pronomen bestimmt werden, z.B. “das alte Essen, der gute Mensch”. Hier gibt es viele Arbeitsblätter mit Erklärungen und Übungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9add0b24-6832-4a84-877e-338e098c16ce","notation":["9add0b24-6832-4a84-877e-338e098c16ce"],"prefLabel":{"de":"Artikel"},"definition":{"de":"Artikel sind Begleiter von Substantiven, sie können nie alleine stehen. Das Wort Artikel kommt aus dem Lateinischen und bedeutet articulus= Gelenk). Es gibt bestimmte und unbestimmte Artikel, die sich dem Genus, Numerus und Kasus des Substantivs anpassen. In diesem Beispielsatz stehen ein bestimmter und ein unbestimmter Artikel: “Sie hilft der Freundin in einen Rollstuhl.”"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c94daaa3-ab49-43b4-86f1-b3efeeeded39","notation":["c94daaa3-ab49-43b4-86f1-b3efeeeded39"],"prefLabel":{"de":"Pronomen"},"definition":{"de":"Pronomen stehen - wie der Name pro Nomen schon sagt - für Nomen, oder anstelle von Nomen. Es gibt verschiedene Möglichkeiten, wie Pronomen Nomen ersetzen können. Es können Possessivpronomen sein, die Zugehörigkeit bzw. Besitz ausdrücken, z.B. mein Rollstuhl, ‘mein’ ist das Pronomen. Es gibt aber auch Relativpronomen, die einen Nebensatz einleiten, wie z.B. “ Der blinde Mann, der seinem Hund folgt”. ‘der’ ist das Pronomen, das eine Relation zwischen dem Nomen “Mann” und der nachfolgenden Information macht. Diese Materialien helfen dir die verschiedenen Pronomen zu lernen und zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ccc981bf-85e2-41f5-b8fb-e7cca9958e2e","notation":["ccc981bf-85e2-41f5-b8fb-e7cca9958e2e"],"prefLabel":{"de":"Verben"},"definition":{"de":"Wir brauchen Verben, auch  Tu- oder Tätigkeitswörter genannt, um Sätze zu bilden. Verben sind die wichtigsten Bausteine eines Satzes. Sie beschreiben eine Handlung, einen Vorgang oder einen Zustand und werden fast  immer klein geschrieben. Die Grundform der Verben nennt man Infinitiv z.B. “hinken”. Wenn man ein Verb in einem Satz benutzt, passt man die Verbform dem Satz an, man benutzt die konjugierte Form, z.B. “Die Frau bezahlt”."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fe05a651-7158-45ff-8664-7faa4bbca5be","notation":["fe05a651-7158-45ff-8664-7faa4bbca5be"],"prefLabel":{"de":"Präpositionen"},"definition":{"de":"Präpositionen werden auch Verhältniswörter genannt, weil sie das Verhältnis zu den anderen Satzgliedern im Satz bestimmen. Es gibt vier Gruppen von Präpositionen je nach Verhältnis im Satz: lokal, temporal, modal und kausal. Präpositionen stehen zusammen mit einem anderen Wort, meistens einem Nomen oder Personalpronomen. Die Präpositionen sind unveränderlich und werden immer klein geschrieben. Sie bestimmen den Kasus des nachfolgenden Nomens, das im Akkusativ, Dativ oder Genitiv stehen kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4d593519-13c1-4e66-bae2-ec97ade836fa","notation":["4d593519-13c1-4e66-bae2-ec97ade836fa"],"prefLabel":{"de":"Sprachwandel"},"definition":{"de":"Die deutsche Sprache ist (wie andere Sprachen) auch in einem stetigen Wandel. Hier geht es um Formen des  Sprachwandels und die Frage, was diesen Wandel auslöst und beeinflusst. Und natürlich geht es auch um Standpunkte hierzu."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4dbf1b75-c9e1-4bc5-b1f3-9684895dbdcd","notation":["4dbf1b75-c9e1-4bc5-b1f3-9684895dbdcd"],"prefLabel":{"de":"Sprachvarietäten"},"definition":{"de":"Innerhalb einer Sprache gibt es zahlreichen Varietäten dieser Sprache, die sich regional unterschieden (Dialekte) oder nach sozialen Gruppen (Soziolekte), nach Altersgruppen (Jugendsprache) oder Berufsgruppen (Fachsprachen)."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b34588ea-e38a-4e2a-b37e-309bfb2e803e","notation":["b34588ea-e38a-4e2a-b37e-309bfb2e803e"],"prefLabel":{"de":"Wortformen"},"definition":{"de":"Diese Sammlung beinhaltet Materialien zu Satzteilen, wenn sie in einem Satz benutzt werden. In einem Satz werden den Wortarten jeweils eine besondere Funktion zugeteilt, die durch eine Wortform gekennzeichnet wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/59df9859-d8db-4126-ad98-f67ba46a0e1b","notation":["59df9859-d8db-4126-ad98-f67ba46a0e1b"],"prefLabel":{"de":"Kasus"},"definition":{"de":"Was ist ein Kasus? Das ist ein lateinisches Wort, das auf Deutsch “Fall” heißt. Ein Kasus ist eine grammatische Kategorie, die zu den Wortarten der Nomen, Adjektiv, Artikel und Pronomen gehört. Im Deutschen gibt es vier Kasus: Nominativ, Akkusativ, Dativ und Genitiv. Ein Kasus markiert syntaktische Relationen oder Funktionen im Satz. In dieser Sammlung  findest du Erklärungen und Übungen zu den deutschen Kasus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/77164cea-de28-49d4-b3ad-d83a36771bde","notation":["77164cea-de28-49d4-b3ad-d83a36771bde"],"prefLabel":{"de":"Tempus"},"definition":{"de":"Tempus (pl. tempora) ist die lateinische Bezeichnung für die Zeitformen von Verben. Im Deutschen gibt es insgesamt sechs Zeitformen: Präsens oder Gegenwartsform, drei Vergangenheitsformen, Präteritum, Perfekt und Plusquamperfekt und zwei Futurformen, Futur I und Futur II. Die Gegenwartsform im Deutschen ist z.B. “der Junge hinkt” und die Vergangenheitsformen dazu sind im Präteritum “der Junge hinkte”, im Perfekt “der Junge hat gehinkt”, im Plusquamperfekt “der Junge hatte gehinkt, im Futur I “der Junge wird hinken” und im Futur II “der Junge wird gehinkt haben”. \nIn dieser Sammlung findest du Materialien um diese Zeitformen zu lernen und zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/85e89fe5-c387-4016-8863-abee1d044182","notation":["85e89fe5-c387-4016-8863-abee1d044182"],"prefLabel":{"de":"Numerus"},"definition":{"de":"Der Numerus bezeichnet in der Grammatik eine Form, die eine Menge angibt. Es gibt die Einzahl (Singular) und die Mehrzahl (Plural). Die Wortarten Substantiv, Adjektiv, Verben, Artikel und Pronomen haben eigene Numerusformen, z.B. “der Blindenstock”, “die Blindenstöcke”, “der hilfsbereite Pfleger”, die hilfsbereiten Leute”. Hier gibt es Material um diese Numerusformen zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c2c5e0a8-195d-4625-acd3-40306cbeea9b","notation":["c2c5e0a8-195d-4625-acd3-40306cbeea9b"],"prefLabel":{"de":"Genus"},"definition":{"de":"Als Genus wird das grammatische Geschlecht eines Nomens bezeichnet. Im Deutschen gibt es drei Genera: Maskulinum (männlich), Femininum (weiblich) und Neutrum (sächlich). In dieser Sammlung findet ihr Erklärvideos und weitere Materialien zu Genera im Deutschen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cca34a40-241e-4e5e-bf41-0e7427a7f6a5","notation":["cca34a40-241e-4e5e-bf41-0e7427a7f6a5"],"prefLabel":{"de":"Genus Verbi (Aktiv und Passiv)"},"definition":{"de":"Als Aktiv und Passiv wird die Verhaltensrichtung des Verbs bezeichnet und auch Genus Verbi genannt. \"Ich suche jemanden.\" und \"Ich werde von jemandem gesucht.\" sind demnach Beispiele für die unterschiedliche Verhaltensrichtung des Verbs \"suchen\" in der 1. Person Singular."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d3aff232-07ed-4bdd-b87c-b8e46e193d96","notation":["d3aff232-07ed-4bdd-b87c-b8e46e193d96"],"prefLabel":{"de":"Modus (Indikativ und Konjunktiv)"},"definition":{"de":"Ein Modus ist eine besondere Form des Verbs. Im Deutschen gibt es drei Modi, den Indikativ, den Konjunktiv I und den Konjunktiv II. Diesen drei Modi sind folgende Funktionen zugeschrieben: Indikativ wird für reale Beschreibungen benutzt, z.B. “er ist behindert”; Konjunktiv I steht für indirekte Rede, z.B. “Ich habe gehört, er sei behindert”;  und Konjunktiv II wird benutzt, wenn man über irrealer Begebenheiten spricht oder schreibt, z.B. “Wenn er doch bald käme.” Hier findest du Materialien mit Erklärungen und Übungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f5d7932f-d500-4141-a4be-ef10bd5292ae","notation":["f5d7932f-d500-4141-a4be-ef10bd5292ae"],"prefLabel":{"de":"Satzarten"},"definition":{"de":"Eine Reihe von Wörtern werden dann zu einem Satz, wenn diese eine in sich geschlossene sprachliche Einheit darstellen. Ein Satz kann sich in Haupt- und Nebensatz untergliedern und dann hinsichtlich seiner kommunikativen und semantischen Funktion in verschiedene Arten unterteilt werden. Hier findest du Materialien mit Übungen und Erklärungen zu den verschiedenen Satzarten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/89a2b7d1-5065-40f7-95f5-a7e01e63a9b6","notation":["89a2b7d1-5065-40f7-95f5-a7e01e63a9b6"],"prefLabel":{"de":"Hauptsätze"},"definition":{"de":"Als ein Hauptsatz gilt ein grammatisch korrekter eigenständiger Satz. Der Hauptsatz ist außerdem der Gegenbegriff zum Nebensatz und gilt somit als der syntaktisch übergeordnete Matrixsatz zum Nebensatz. Der Hauptsatz lässt sich in verschiedene Satzarten unterteilen, den Aussagesatz, den Aufforderungssatz und den Fragesatz. Mehr darüber lernen kannst du durch die Materialien aus unserer Sammlung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9f31ebe4-fcd5-4236-bb02-c6cf7680c1d7","notation":["9f31ebe4-fcd5-4236-bb02-c6cf7680c1d7"],"prefLabel":{"de":"Nebensätze"},"definition":{"de":"Ein Nebensatz oder auch ein Teilsatz gilt nicht als eigenständiger grammatisch korrekter Satz, sondern ist immer einem Matrixsatz - also einem Hauptsatz oder einem anderen übergeordneten Nebensatz - untergeordnet. Der Nebensatz kann weiter klassifiziert werden, so kann er eingeleitet oder uneingeleitet sein, und beispielsweise als Relativsatz, Adverbialsatz oder erweiterter Infinitiv bestimmt werden. In dieser Sammlung lernst du die verschiedenen Nebensätze kennen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dcd35083-8050-4767-a890-e64e3b624322","notation":["dcd35083-8050-4767-a890-e64e3b624322"],"prefLabel":{"de":"Wortbildung"},"definition":{"de":"Wie werden Wörter im Deutschen gebildet, z.B. “Liebeskummer” oder “Handtuch”? In diesem Themenbereich gibt es Arbeitsblätter, die dir die Wortbildung erklären und Übungen dazu."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f3089575-2c0c-4538-bc80-dcdfd4592d92","notation":["f3089575-2c0c-4538-bc80-dcdfd4592d92"],"prefLabel":{"de":"Wortschatz und Wortkunde"},"definition":{"de":"Was ist das eigentlich: Wortschatz? Schatz der Wörter? Was für einen Wortschatz habt ihr? Hier gibt es Materialien um euren Wortschatz zu vergrößern, zu verbessern und zu üben. Und wenn ihr mehr Information zu Herkunft und Bedeutung der Wörter eures Wortschatzes wissen wollt, dann nennt man das Wortkunde. Auch dafür findet ihr Materialien in diesem Themenbereich."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/38154eec-b4d4-4889-b33e-9a66a2b2c81d","notation":["38154eec-b4d4-4889-b33e-9a66a2b2c81d"],"prefLabel":{"de":"Texte schreiben"},"definition":{"de":"Wenn du einen Text schreibst, gibt es viele Dinge zu beachten, z.B. wie man einen Text plant, wie man den Text formuliert oder wie du ihn überarbeiten kannst. Zu welcher Textsorte gehört der Text? In dieser Sammlung findest du Materialien zu diesen verschiedenen Aspekten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/56cd6ac3-849e-4540-8701-afba2ef0bdaf","notation":["56cd6ac3-849e-4540-8701-afba2ef0bdaf"],"prefLabel":{"de":"Erzählen und Gestalten"},"definition":{"de":"Hier findest du Materialien, die dir helfen, Erzähltexte alleine oder zusammen zu schreiben, z.B. Märchen. Es gibt auch viele Anregungen, wie du im digitalen Unterricht Ideen sammeln und spannende Texte gestalten kannst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5d86eba1-bf6f-487d-a73d-783ab166bc02","notation":["5d86eba1-bf6f-487d-a73d-783ab166bc02"],"prefLabel":{"de":"Formelle und informelle Texte"},"definition":{"de":"Es ist wichtig zu unterscheiden, welche Bausteine man für formelle und informelle Texte verwendet. Diese Sammlung hilft euch den Unterschied zwischen diesen Texttypen zu entschlüsseln und diese verschiedenen Texttypen zu üben. Diese Sammlung hat außerdem Materialien zur Erstellung eines Lebenslaufs und Information darüber, wie man sich bei einem Vorstellungsgespräch verhält."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7ab6f94f-a971-4016-834e-cfbbe35f5e01","notation":["7ab6f94f-a971-4016-834e-cfbbe35f5e01"],"prefLabel":{"de":"Wörtliche Rede und indirekte Redewiedergabe"},"definition":{"de":"Die wörtliche Rede und indirekte Redewiedergabe sind zwar einerseits ein Aspekt der Zeichensetzung und daher auch in dieser Sammlung zu finden, andererseits aber auch ein wichtiger Aspekt beim Schreiben von eigenen Texten. Die wörtliche Rede bezeichnet die exakte, direkte Wiedergabe des Gesprochenen einer anderen Person. Bei der indirekten Redewiedergabe wird die Formulierung mit eigenen Worten wiedergegeben, hierbei spielen vor allem kommunikative und inhaltliche Aspekte eine große Rolle. Hier findest du Materialien zu diesen zwei Bereichen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8fe04278-c880-43c6-813a-36d25a0ecbdf","notation":["8fe04278-c880-43c6-813a-36d25a0ecbdf"],"prefLabel":{"de":"Zusammenfassen"},"definition":{"de":"In dieser Sammlung gibt es Materialien mit Erklärungen und Aufgaben, die dir helfen, den Inhalt eines Textes zusammenzufassen und eine Inhaltsangabe zu schreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/abf484fc-6b87-4775-9940-8a71dd46c19a","notation":["abf484fc-6b87-4775-9940-8a71dd46c19a"],"prefLabel":{"de":"Schreibprozess"},"definition":{"de":"Diese Sammlung bietet Materialien mit Ideen und Anregungen, wie man das Schreiben von Texten plant, Sätze formuliert und überarbeitet. Und natürlich, wie man als Lehrkraft diese verschiedenen Texttypen dann unterrichtet und damit arbeitet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b02a62fb-73f5-471b-9f75-205d71b3cf8a","notation":["b02a62fb-73f5-471b-9f75-205d71b3cf8a"],"prefLabel":{"de":"Argumentieren"},"definition":{"de":"In dieser Sammlung findest du sowohl Videos als auch Unterrichtsbausteine mit Erklärungen und Aufgaben, die dir helfen, effektiv das Argumentieren in Erörterungen zu lernen und zu üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f099652a-b4ab-4713-b741-a1ad01478ee1","notation":["f099652a-b4ab-4713-b741-a1ad01478ee1"],"prefLabel":{"de":"Beschreiben, Berichten und Informieren"},"definition":{"de":"Musst du einen Bericht schreiben oder eine Beschreibung wie z.B. eine Wegbeschreibung? In dieser Sammlung findest du Anleitungen zum Verfassen dieser informativen, sachlichen Texttypen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/120"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/94f22c9b-0d3a-4c1c-8987-4c8e83f3a92e","notation":["94f22c9b-0d3a-4c1c-8987-4c8e83f3a92e"],"prefLabel":{"de":"Physik"},"definition":{"de":"Fachportal Physik"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/65652844-fe24-4ab0-9374-02af5a0b63e3","notation":["65652844-fe24-4ab0-9374-02af5a0b63e3"],"prefLabel":{"de":"Atomphysik"},"definition":{"de":"Die Physik der Elektronenhülle, die Atomphysik, beschreibt den grundlegenden Aufbau der uns umgebenden Materie. Mit Hilfe von Erkenntnissen der Atomphysik lässt sich erklären, weshalb bestimmte Materialien farbig sind (und andere nicht), weshalb sich Atome zu Molekülen oder noch größeren Festkörpern zusammenfinden und welche Eigenschaften diese haben. Die Atomphysik nutzt hierzu meist Methoden und Ideen der Quantenphysik."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0524ce0e-dbdb-48a3-80cd-249c3e769359","notation":["0524ce0e-dbdb-48a3-80cd-249c3e769359"],"prefLabel":{"de":"Röntgen-Strahlung"},"definition":{"de":"Wilhelm Konrad Röntgen machte Ende des 19. Jahrhunderts eine interessante Entdeckung: Mit von ihm erzeugter Strahlung konnte er das innere seiner Hand sichtbar machen. Diese zufällige Entdeckung wird heute in Medizin und Technik vielfältig genutzt - vielleicht wurde auch von deinem Körper schon einmal eine Röntgenaufnahme angefertigt. Bei Röntgenstrahlung handelt es sich, wie wir heute wissen, um eine Form der elektromagnetischen Strahlung. Sie ist sehr energiereich und entsteht immer dann, wenn elektrische Ladungen abrupt abgebremst werden. Dies ist in Röntgenröhren der Fall - aber auch in vielen astrophysikalischen Prozessen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e67d03cd-9408-45de-9894-d6a47705fc38","notation":["e67d03cd-9408-45de-9894-d6a47705fc38"],"prefLabel":{"de":"Atomarer Energieaustausch"},"definition":{"de":"Atome sind in der Lage, Strahlungsenergie zu absorbieren und zu emitieren. Anders als makroskopische Gegenstände erfolgt diese Absorption und Emission jedoch nicht in beliebigen Portionsgrößen. Vielmehr sind Atome nur in der Lage, Photonen mit ganz bestimmter Energie aufzunehmen oder abzugeben. Dieses Verhalten der Atome lässt sich mit Mitteln der Quantenphysik genauer erklären - und führt zu einer Vielzahl technischer Anwendungen: Der Laser basiert genauso auf dieser grundlegenden Eigenschaft eines Atoms wie sämtliche Halbleiterbauelemente."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3e7609a6-9a7e-4189-b143-73ffd3ab85a3","notation":["3e7609a6-9a7e-4189-b143-73ffd3ab85a3"],"prefLabel":{"de":"Festkörperphysik"},"definition":{"de":"Die Festkörperphysik befasst sich mit dem Aufbau und den Eigenschaften fester Körper. Mit ihren Modellen lassen sich Phänomene wie die Farbe von Festkörpern oder der Glanz von Metallen genauer erklären. Die Festkörperphysik nutzt hierzu Methoden der Quantenphysik, die sie auf größere Atomverbünde anwendet, wie sie für Festkörper kennzeichnend sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3f7a600a-5b88-4976-9f34-1ed52c32dca6","notation":["3f7a600a-5b88-4976-9f34-1ed52c32dca6"],"prefLabel":{"de":"Halbleiterphysik"},"definition":{"de":"Halbleiterbauelemente sind aus unserem Alltag nicht mehr wegzudenken: Die gesamte Computertechnik basiert auf Halbleitern. Ein Halbleiter kann als Festkörper mit besonderen Eigenschaften verstanden werden: Anders als ein Metall leitet er den elektrischen Strom je besser, desto wärmer er ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ecb0a81c-f2ed-4d93-a2b2-1b1fb7748849","notation":["ecb0a81c-f2ed-4d93-a2b2-1b1fb7748849"],"prefLabel":{"de":"Laser"},"definition":{"de":"Laser sind in der Lage, Licht sehr hoher Helligkeit, definierter Polarisation zu erzeugen. Auch hat das Licht eines Lasers eine genau festgelegte Wellenlänge oder Farbe. Diese besonderen Eigenschaften machen Lasern zu dem, was sie heute sind. Neben Laserpointern kommen sie beispielsweise in Laufwerken für DVDs und in einer Vielzahl von Messgeräten zum Einsatz. Nicht zuletzt sind Laser auch optisch hübsch, wie du vielleicht bei einer Lasershow schon einmal selbst beobachten konntest."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7eb26d5f-e0a9-46c1-99bc-a76943d5f6a8","notation":["7eb26d5f-e0a9-46c1-99bc-a76943d5f6a8"],"prefLabel":{"de":"Historische Atommodelle"},"definition":{"de":"Schon seit dem Altertum denken Menschen über die Struktur der Materie nach. Bereits im antiken Griechenland prägte der Philosoph Demokrit den Begriff des Atoms. In der Neuzeit wurde das Bild der Physiker:innen vom Atom immer detailreicher und komplexer: Dalton beschrieb sie als kleine Kugeln, Thomson spekulierte über subatomare Rosinenkuchen und Rutherford entwickelte das Kern-Hülle-Modell des Atoms. Auf dem Weg zum modernen quantenmechanischen Atommodell stellt schließlich das Atommodell von Bohr einen wichtigen Zwischenschritt dar. Erstaunlich ist, dass fast alle diese Modelle heute in vielfältigen Zusammenhängen von Physik und Chemie noch immer genutzt werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a6800a2e-ce72-406f-8d08-b9f3b92ae8fd","notation":["a6800a2e-ce72-406f-8d08-b9f3b92ae8fd"],"prefLabel":{"de":"Quantenmechanisches Atommodell"},"definition":{"de":"Das quantenmechanische Atommodell, oft auch als Orbitalmodell bezeichnet, ist die genaueste Beschreibung der Struktur der Elektronenhülle von Atomen, die Physiker:innen bislang entwickelt haben. Es basiert auf der Schrödingergleichung und leitet aus dieser quantisierte Energieniveaus und charakteristische Aufenthaltsbereiche von Elektronen in der Atomhülle ab. Das Orbitalmodell ist so in der Lage, alle bislang beobachtbaren Eigenschaften von Atomen von der Wechselwirkung mit Licht bis hin zur Ausbildung chemischer Bindungen zu erklären."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cacbc7bc-833e-4360-90f0-6f828db27508","notation":["cacbc7bc-833e-4360-90f0-6f828db27508"],"prefLabel":{"de":"Wärmelehre / Thermodynamik"},"definition":{"de":"Das physikalische Teilgebiet der Thermodynamik versucht, dem Phänomen der Wärme und Wärmeübertragung auf die Spur zu kommen. Untersucht werden Phänomene wie Temperaturveränderung, der Wärmetransport von einem Ort zum anderen, Kreisprozesse wie der Stirling-Kreisprozess. Ebenso wird die Ursache dieser Prozesse auf mikroskopischer Ebene betrachtet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/09dbf6d9-4492-4871-87a7-6397404fec20","notation":["09dbf6d9-4492-4871-87a7-6397404fec20"],"prefLabel":{"de":"Allgemeines Gasgesetz"},"definition":{"de":"Gase bestehen aus Atomen und Molekülen unterschiedlicher Elemente. Experimente zeigen jedoch: Aus physikalischer Perspektive verhalten sich alle Gase so, als würden sie aus kleinen, festen Kugeln bestehen. Diese wechselwirken miteinander allein über Stöße. Und da alle Gase sich näherungsweise so verhalten, lassen sie sich auch gut mit einer und der selben Gleichung beschreiben: Dem idealen Gasgesetz."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/14c45a72-8439-4309-946e-f762bc9feeba","notation":["14c45a72-8439-4309-946e-f762bc9feeba"],"prefLabel":{"de":"Temperatur im Teilchenmodell"},"definition":{"de":"Verschiedene Temperaturen kennt jeder von uns. Zu entscheiden, ob etwas heiß oder kalt ist, gehört zu unserem Alltag. Ein Blick in die Struktur der Materie eröffnet einen weniger alltäglichen Blick: Eine hohe Temperatur ist gleichbedeutend mit einer schnellen Bewegung der Teilchen, aus denen ein Gegenstand besteht. Eine niedrige Temperatur ist hingegen verbunden mit der langsamen Bewegung dieser Teilchen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6eae60ff-2ff6-4759-a2a6-2c75dea749f4","notation":["6eae60ff-2ff6-4759-a2a6-2c75dea749f4"],"prefLabel":{"de":"Ausdehnung"},"definition":{"de":"Gegenstände dehnen sich bei der Erwärmung aus, wie du gut an großen Straßenbrücken beobachten kannst. Diese verfügen über spezielle Dehnungsfugen die verhindern, dass bei der Ausdehnung der gesamte Asphalt aufreißt und Wellen schlägt. Erklären lässt sich diese Längenausdehnung über das Teilchenmodell der Temperatur: Eine größere Temperatur entspricht einer schnelleren Bewegung der Teilchen. Die Teilchen benötigen hierzu dann entsprechend mehr Platz, der Gegenstand dehnt sich aus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/81be8023-7faa-4c16-ad09-d9600d3f21ec","notation":["81be8023-7faa-4c16-ad09-d9600d3f21ec"],"prefLabel":{"de":"Wetter / Klima"},"definition":{"de":"Das aktuelle Wetter und das langfristige Klima wird vor allem durch aufwändige Modellrechnungen beschrieben. Diese Modellrechnungen nutzen vielfältige physikalische Gesetzmäßigkeiten und benötigen Messdaten aus realen Messstationen. Darüber hinaus lassen sich zur langfristigen Vorhersage des Klimas wichtige Methoden der Physik auch für die Erstellung immer besserer und genauerer Prognosen nutzen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/85b4a93e-cecc-4165-a931-fa11a50504ba","notation":["85b4a93e-cecc-4165-a931-fa11a50504ba"],"prefLabel":{"de":"Wärmekraftmaschinen"},"definition":{"de":"Motoren sind aus physikalischer Sicht Wärmekraftmaschinen. Sie nutzen sehr unterschiedliche Prozesse. Allen gemeinsam ist jedoch, dass durch das geschickte Zusammenspiel von Temperatur, Druck und Volumen in einem geeignet konstruierten Gerät durch sich wiederholende Schritte von Erwärmung und Abkühlung Bewegung entsteht. Typische Beispiele sind der Otto- und Dieselmotoren, genauso wie der Stirlingmotor. Jeder dieser Motortypen realisiert einen anderen thermodynamischen Kreisprozess."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a8e71db1-acaf-4e63-a906-5c104b91c231","notation":["a8e71db1-acaf-4e63-a906-5c104b91c231"],"prefLabel":{"de":"Kinetische Gastheorie"},"definition":{"de":"Die kinetische Gastheorie beschreibt Gase als bewegte Teilchen unterschiedlicher Energie. Sie ist in der Lage, statistische Aussagen zur Verteilung der Geschwindigkeit von Gasmolekülen bei einer bestimmten Temperatur zu treffen. So wird klar, dass es auch bei Raumtemperatur bereits Wassermoleküle gibt, die sich so schnell bewegen, dass sie als Gas vorliegen. Sie stellt somit eine Erweiterung des Modell des idealen Gases um statistische Überlegungen dar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b8c4150a-81b9-49ed-941d-039bed225281","notation":["b8c4150a-81b9-49ed-941d-039bed225281"],"prefLabel":{"de":"Wärmetransport"},"definition":{"de":"Wärme strömt immer von einem warmen Gegenstand zu einem kälteren. Der Transport der Wärme erfolgt dabei jedoch auf sehr unterschiedliche Weise: Durch Wärmestrahlung, Wärmeleitung und Konvektion. Den Transport durch Wärmestrahlung bemerken wir vor allem im Fall der Sonne. Die Wärme der Sonne gelangt durch Infrarotstrahlung durch das Weltall zu uns. Wärmeleitung bemerkst du vor allem beim Kochen: Der Löffel aus Metall leitet Wärme aus dem Kochtopf an deine Hand. Konvektion ist vor allem in Gasen und Flüssigkeiten relevant. Die Wärme wird mit einem Trägermedium mitgeführt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c5addb17-2acd-41e9-9d97-b60f5a64adfa","notation":["c5addb17-2acd-41e9-9d97-b60f5a64adfa"],"prefLabel":{"de":"innere Energie / Wärmekapazität"},"definition":{"de":"Die in einem Gegenstand gespeicherte Energie liegt in der Regel als thermische Energie oder als potentielle Energie der Bestandteile des Gegenstandes vor. Die innere Energie eines Gegenstandes kann sich verändern, indem Arbeit an dem oder durch den Gegenstand verrichtet wird - oder Wärme mit der Umgebung ausgetauscht wird. Ansonsten behält ein Gegenstand seine innere Energie bei. Dies ist die zentrale Aussage des ersten Hauptsatzes der Thermodynamik. Die Wärmekapazität eines Gegenstandes beschreibt, wie stark sich die innere Energie eines Gegenstandes bei Erwärmung verändert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ebe18f72-ec94-407e-b700-a852c74ffd9d","notation":["ebe18f72-ec94-407e-b700-a852c74ffd9d"],"prefLabel":{"de":"Deterministisches Chaos"},"definition":{"de":"Systeme, die auf den ersten Blick völlig chaotisch wirken, sind es oft auf den zweiten Blick gar nicht. Vielmehr folgen die dort ablaufenden Prozesse grundlegenden physikalischen Regeln, die grundsätzlich berechenbar wären - wenn nicht alles so schnell und in großer Menge ablaufen würde. Das Verhalten eines solchen Systems lässt sich nur schwer voraussagen, weil es von den (nie vollständig gleichen) Startbedingungen abhängt. Das vielleicht berühmteste Beispiel ist der sogenannte Schmetterlingseffekt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f1fa8806-5e67-4e6b-8140-2d52ee2cb71e","notation":["f1fa8806-5e67-4e6b-8140-2d52ee2cb71e"],"prefLabel":{"de":"Entropie"},"definition":{"de":"Die Entropie ist ein Maß für die Unordnung eines physikalischen Systems. Betrachtet man das Universum als ganzes, so nimmt die Unordnung immer weiter zu. Dies hat zur unmittelbaren Folge, dass thermische Energie nicht unbegrenzt in andere Energieformen umwandelbar ist - und die Entwicklung einer ewig laufenden Maschine, eines Perpetuum Mobiles - grundsätzlich unmöglich ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8841e193-e8f6-410b-a315-52935506b962","notation":["8841e193-e8f6-410b-a315-52935506b962"],"prefLabel":{"de":"Physik im Kontext"},"definition":{"de":"Physik wird nicht im luftleeren Raum betrieben, sondern ist in vielfältige Kontexte eingebettet. Diese reichen von interdisziplinären Forschungsprojekten über große Forscherpersönlichkeiten bis hin zum Wesen der Naturwissenschaften. Lass dich durch unsere Materialien zu Physik im Kontext inspirieren, mehr als nur den physikalischen Inhalt zu betrachten!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a9ea529-efbb-4bbe-ad48-d2a1ff6cebe1","notation":["0a9ea529-efbb-4bbe-ad48-d2a1ff6cebe1"],"prefLabel":{"de":"Wesen der Naturwissenschaften"},"definition":{"de":"Die \"Natur der Naturwissenschaft\" - oder auch \"Nature of Science\" bezeichnet den Blick auf zentrale Ideen, Methoden und Eigenschaften der Naturwissenschaften, des naturwissenschaftlichen Forschungsprozesses sowie die daran beteiligten Personen. Die Naturwissenschaft, insbesondere die Physik, wird somit unter historischen, soziologischen und psychologischen Blickwinkeln genauer unter die Lupe genommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7716f7b8-7830-484a-b5c6-d4747bc1a877","notation":["7716f7b8-7830-484a-b5c6-d4747bc1a877"],"prefLabel":{"de":"Geschichte der Physik"},"definition":{"de":"Die Geschichte der Physik ist voll von Irrungen und Wirrungen, schillernden Persönlichkeiten und erbitterten Streitigkeiten. Manche Theorien und Konzepte haben sich bewährt, andere sind überholt und gingen verloren. Zeitweise stellten sich Physiker:innen, wie im amerikanischen Manhattan-Projekt, in den Dienst der Waffenindustrie und entwickelten Waffen die die Atombombe. Zu anderen Zeiten setzten sich Physiker:innen für die Beilegung bewaffneter Konflikte ein. Und als wäre das nicht genug gibt es viele Geschichten über das Wechselspiel von Kirche, Gesellschaft und Wissenschaft zu erzählen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/996d136f-ae10-4db1-924c-96842271e0fb","notation":["996d136f-ae10-4db1-924c-96842271e0fb"],"prefLabel":{"de":"Physik interdisziplinär"},"definition":{"de":"Nicht erst in der Corona-Pandemie zeigte sich, dass Physiker:innen in einer Vielzahl benachbarter Disziplinen eine Rolle spielen: Viele epidemiologische Modellierer:innen sind studierte Physiker:innen. Physikalische Methoden und Ideen sind also auch außerhalb der Physik gefragt und nützlich, um wichtige Fragestellungen zu beantworten. Auch Erfindungen wie beispielsweise das WWW sind im Zuge physikalischer Forschung entstanden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bed07e06-7db9-402a-b7f1-32b26c647286","notation":["bed07e06-7db9-402a-b7f1-32b26c647286"],"prefLabel":{"de":"Wissenschaft und Mythen"},"definition":{"de":"Hier findet man Inhalte rund um wissenschaftliche Methoden in Abgrenzung zu nichtwissenschaftlichem / pseudowissenschaftlichem / verschwörungstheoretischem Vorgehen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"},{"id":"http://w3id.org/openeduhub/vocabs/discipline/04003"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c3bc1dc2-89d6-40ef-820e-473738862de7","notation":["c3bc1dc2-89d6-40ef-820e-473738862de7"],"prefLabel":{"de":"Persönlichkeiten der Physik"},"definition":{"de":"Die Physik ist voller schillernder, skurriler und auch weniger auffälligen Persönlichkeiten. Viele berühmte Physiker waren Männer. Frauen hatten es nicht immer leicht, in der patriarchalen Wissenschaft Physik zu bestehen. Trotz aller Widrigkeiten haben es aber auch berühmte Physikerinnen geschafft und sich ihren verdienten Platz in der Geschichte der Physik gesichert. Namen wie Einstein, Heisenberg, Pauling, Curie, Newton und Galilei sind weltberühmt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fa24ecae-4390-49a0-b2ad-52a80c7b3e77","notation":["fa24ecae-4390-49a0-b2ad-52a80c7b3e77"],"prefLabel":{"de":"aktuelle Projekte"},"definition":{"de":"Aktuelle Forschungsprojekte sind spannend - und inhaltlich oft komplex. Sie erfordern in vielen Fällen aufwändige Messapparaturen wie Teilchenbeschleuniger, Satelliten oder Fusionsreaktoren - und oft internationale Zusammenarbeit hunderter Wissenschaftler:innen. Sie sind deshalb faszinierend und spannend zugleich. Physiker:innen suchen in aktuellen Projekten Antworten auf aktuelle wissenschaftliche Fragestellungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3c044300-e8da-4f49-a693-eddf5f3ef73f","notation":["3c044300-e8da-4f49-a693-eddf5f3ef73f"],"prefLabel":{"de":"Astronomie"},"definition":{"de":"Die Astronomie befasst sich mit der Physik der Himmelskörper. Diese umfasst die Beobachtung von Sternen und Planeten, die Berechnung ihrer Bahnen und die Untersuchung ihrer Eigenschaften. Hierbei kommen neben theoretischen Berechnungen vor allem spektroskopische Methoden zum Einsatz. So lassen sich aus dem Licht der Sterne Rückschlüsse auf die chemischen und physikalischen Eigenschaften weit entfernter Sterne ziehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0b1181fe-a29b-4930-903a-9cbdf2c77edb","notation":["0b1181fe-a29b-4930-903a-9cbdf2c77edb"],"prefLabel":{"de":"Sternbeobachtung"},"definition":{"de":"Auch du hast sicherlich schon einmal in den Sternenhimmel geblickt und warst fasziniert von diesem Anblick. Unsere Galaxie, die Milchstraße, ist eine von Milliarden Galaxien im Universum - und in jeder Galaxie existieren wiederum Milliarden von Sternen. Die Menschen haben diese seit der Antike in Sternbilder gruppiert. Heute nutzen Astronomen vielfältige Methoden zur genaueren Beobachtung von Sternen: Radioteleskope, Neutrinodetektoren und optische Teleskope sind die wichtigsten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5f0754ed-10d1-4845-9e4f-57f35f7f787d","notation":["5f0754ed-10d1-4845-9e4f-57f35f7f787d"],"prefLabel":{"de":"Sonne / Fixsterne"},"definition":{"de":"Die Sonne ist ein Stern unter vielen. Jedoch ist sie - natürlich - für das Leben auf der Erde von ganz besonderer Bedeutung. Ohne die Sonne könnte Leben auf der Erde nicht existieren. Als Fixsterne bezeichnet man andere Sterne, die an vergleichsweise festen Positionen von der Erde aus sichtbar sind. Sie können deshalb zur Navigation, zum Beispiel auf hoher See, genutzt werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6481eff0-eb1d-45b7-b099-ddc9f670db0a","notation":["6481eff0-eb1d-45b7-b099-ddc9f670db0a"],"prefLabel":{"de":"Einführung / Grundlagen"},"definition":{"de":"Die Astronomie untersucht unglaublich viele, unglaublich unterschiedliche Phänomene. Astronomen nutzen hierzu oft Begriffe und Ideen aus der Physik, Mathematik und Chemie. Eine “Einführung” in die Astronomie versucht, grundlegende Phänomene zu klären, Techniken und Arbeitsweisen der Astronomie zu beschreiben und die geschichtliche Entwicklung des astronomischen Wissens deutlich zu machen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/99db3d61-6683-42e9-8ea0-c8e14534c96e","notation":["99db3d61-6683-42e9-8ea0-c8e14534c96e"],"prefLabel":{"de":"Kosmologie"},"definition":{"de":"Die Kosmologie fragt nach dem großen Ganzen: Wie ist das Universum eigentlich entstanden? Wird es irgendwann auch wieder verschwinden? Gibt es weitere Paralleluniversen? Kosmologische Fragestellungen haben, wie du siehst, ihre ganz eigene Faszination und auch Auswirkungen auf die Philosophie."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/df13c01a-dd5e-4fb5-b570-845fed00fbd0","notation":["df13c01a-dd5e-4fb5-b570-845fed00fbd0"],"prefLabel":{"de":"Planetensystem"},"definition":{"de":"Das Planetensystem unserer Sonne besteht aus acht Planeten: Merkur, Venus, Erde, Mars, Jupiter, Saturn, Uranus und Neptun. Daneben gibt es unzählige weitere Monde, Kleinplaneten und Asteroiden. Die grundlegenden Gesetzmäßigkeiten, nach denen sich diese Planeten bewegen, wurden bereits zu Beginn der Neuzeit von Kepler beschrieben: Die Planeten bewegen sich auf Ellipsenbahnen um die Sonne."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/998f0a83-f928-400b-be4a-ac609b2dc878","notation":["998f0a83-f928-400b-be4a-ac609b2dc878"],"prefLabel":{"de":"Teilchenphysik"},"definition":{"de":"Die Teilchenphysik ist eines der aktuellsten und spannendsten Teilgebiete der Physik. Um die Welt der allerkleinsten Teilchen, der Leptonen, Quarks und Austauschteilchen und die ihnen zugrunde liegenden physikalischen Wechselwirkungen zu untersuchen, werden riesige Teilchenbeschleuniger betrieben. Am bekanntesten ist wohl der LHC am europäischen Kernforschungszentrum CERN in der Schweiz. Auftretende Wechselwirkungen sind die starke und schwache Kraft, die elektromagnetische Kraft und die Gravitation. Diese Forschungen haben entscheidend zu unserem heutigen Bild des Aufbaus der Materie geführt: Dem Standardmodell der Teilchenphysik."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0fdcd162-d01b-40f0-922b-bbd2fe1ac420","notation":["0fdcd162-d01b-40f0-922b-bbd2fe1ac420"],"prefLabel":{"de":"Standardmodell"},"definition":{"de":"Das Standardmodell der Teilchenphysik ist eine der größten Errungenschaften der Physik. Es beschreibt den Aufbau der Materie und die auf diese Materie wirkenden physikalischen Wechselwirkungen vollumfänglich - und benötigt dafür nur eine gute handvoll elementarer Teilchen. Es erklärt alle bislang beobachtbaren Eigenschaften der Materie. Und doch benötigt es Randbedingungen, die sich nicht aus theoretischen Annahmen begründen lassen. Physiker:innen sind deshalb seit Jahren auf der Suche auch nach einer Physik \"jenseits des Standardmodells.\""},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/386bb7fe-c14f-4327-b27b-7053e8258f89","notation":["386bb7fe-c14f-4327-b27b-7053e8258f89"],"prefLabel":{"de":"Beschleuniger"},"definition":{"de":"Beschleuniger sind die vermutlich größten technisch-experimentellen Aufbauten der Menschheitsgeschichte. Sie bestehen oft aus unterirdischen, kreisförmigen Tunneln mit teils mehreren Kilometern Radius und dienen nur zu einem Zweck: Winzige Teilchen zu beschleunigen und kontrolliert zur Kollision zu bringen. Bei der Kollission werden unvorstellbare Energiemengen auf kleinstem Raum freigesetzt - und es entstehen neue Teilchen. Diese lassen Rückschlüsse auf die ablaufenden, subatomaren Prozesse zu. Beschleuniger dienen also dazu, noch genauer zu verstehen, \"was die Welt im innersten zusammenhält\"."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dcef1170-e592-4925-8298-603baefa07d4","notation":["dcef1170-e592-4925-8298-603baefa07d4"],"prefLabel":{"de":"Elementarteilchen"},"definition":{"de":"Elementarteilchen sind die kleinsten bekannten Bausteine der Materie und haben selbst keine innere Struktur. Es wurden in den letzten Jahrzehnten eine unglaubliche Vielzahl von Elementarteilchen beobachtet: Teilchen wie Quarks und Leptonen, aus denen die Materie besteht, genauso wie Austauschteilchen wie W- und Z-Bosonen, Photonen und viele mehr. Sie dienen dazu, Prozesse im Kosmos über die Beobachtung von Photonen und Neutrinos zu verstehen. Und sie eröffnen ein erstes Verständnis, weshalb Materie überhaupt so etwas wie \"Masse\" besitzt: Das Higgs-Teilchen trägt hierfür die Hauptverantwortung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/43cd5309-9d1c-4e05-80e6-a5ef6e40a0ca","notation":["43cd5309-9d1c-4e05-80e6-a5ef6e40a0ca"],"prefLabel":{"de":"Schwingungen und Wellen"},"definition":{"de":"Schwingungen sind ein allgegenwärtiges Phänomen in der Physik: Es handelt sich um periodische Vorgänge, die sich mit sehr ähnlichen mathematischen Formalismen beschreiben lassen. Breiten sich Schwingungen im Raum aus, so entstehen Wellen. Als solche Welle lassen sich Phänomene wie Wasserwellen und La-Ola-Wellen, aber auch Licht, Radiowellen und Ultraschallwellen beschreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/13922d17-57c7-44f6-994c-6c35811752d1","notation":["13922d17-57c7-44f6-994c-6c35811752d1"],"prefLabel":{"de":"Mechanische Wellen"},"definition":{"de":"Breiten sich mechanische Schwingungen im Raum aus, so entstehen mechanische Wellen. Typische Beispiele für derartige Wellen sind Wasserwellen. Mit solchen mechanischen Wellen lassen sich bereits für Wellen typische Eigenschaften wie Interferenz und Beugung beobachten und analysieren. Die Grundlage solcher mechanischer Wellen ist die Kopplung benachbarter Atome beziehungsweise Moleküle: Wird ein Molekül in Schwingung versetzt, so beeinflusst es benachbarte Moleküle und versetzt diese ebenfalls in Schwingung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/13c03c9b-220d-4821-a2b9-5d6ebb86e08b","notation":["13c03c9b-220d-4821-a2b9-5d6ebb86e08b"],"prefLabel":{"de":"Elektromagnetische Wellen (s. Wellenoptik)"},"definition":{"de":"Licht kann als elektromagnetische Welle beschrieben werden. Sichtbares Licht ist allerdings nur ein kleiner Ausschnitt aus dem breiten Spektrum elektromagnetischer Wellen. Auch Mikrowellen, Radiowellen und Gammastrahlung sind elektromagnetische Wellen. Bei einer solchen Welle schwingen ein elektrisches und ein magnetisches Feld periodisch und breiten sich dabei im Raum aus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/87390ea6-69c7-465c-9efb-13f7ba23acc9","notation":["87390ea6-69c7-465c-9efb-13f7ba23acc9"],"prefLabel":{"de":"Elektromagnetische Schwingungen"},"definition":{"de":"Elektromagnetische Schwingungen entstehen in der Regel dann, wenn elektrische und magnetische Felder sich periodisch verändern. Die naheliegende Realisierung einer solchen Schwingung ist der elektrische Schwingkreis. Pflanzen sich solche Schwingungen im Raum fort, so sprechen wir von einer elektromagnetischen Welle."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/61c4ebff-a9d7-4001-8f14-bce0cf28e927","notation":["61c4ebff-a9d7-4001-8f14-bce0cf28e927"],"prefLabel":{"de":"Schwingkreis (s. E-Lehre)"},"definition":{"de":"Der Schwingkreis bildet die Grundlage der gesamten Nachrichtentechnik. Mit Hilfe eines Schwingkreises, der aus einem Kondensator und einer Spule besteht, lassen sich gezielt elektromagnetische Wellen erzeugen. Zu diesem Zweck muss der Schwingkreis durch eine äußere Wechselspannung zum Schwingen angeregt werden. Die Eigenfrequenz eines solchen Schwingkreises bestimmt sich durch die Art des verwendeten Kondensators und der genutzten Spule."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/09dc6bd8-03dd-4bb4-864c-c224f86740af","notation":["09dc6bd8-03dd-4bb4-864c-c224f86740af"],"prefLabel":{"de":"Mechanische Schwingungen"},"definition":{"de":"Mechanische Schwingungen werden nicht nur in Pendeluhren genutzt. Oft machen sie sich auch unangenehm bemerkbar, beispielsweise als Vibration des Autos, wenn zufällig die Eigenfrequenz einer mechanischen Schwingung angeregt wurde. Zur physikalischen Untersuchung derartiges mechanischer Schwingungen nutzen Physiker:innen oftmals ein Feder- oder ein Fadenpendel, da diese gut beobachtbar sind und sich wichtige Eigenschaften leicht berechnen lassen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/694c7437-e98a-4810-bc72-7842c62de8b2","notation":["694c7437-e98a-4810-bc72-7842c62de8b2"],"prefLabel":{"de":"Fadenpendel"},"definition":{"de":"Ein Fadenpendel besteht aus einem Faden, an den ein Massestück gehängt wird. Das Massestück wird nun leicht zur Seite bewegt und beginnt, hin und her zu pendeln. Die Frequenz dieser Pendelbewegung hängt von der Masse sowie der Länge des Fadens ab - nicht aber von der anfänglichen Auslenkung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8af28342-37d8-465a-b6a3-e4b5c4946499","notation":["8af28342-37d8-465a-b6a3-e4b5c4946499"],"prefLabel":{"de":"Federpendel"},"definition":{"de":"Ein Federpendel besteht im einfachsten Fall aus einer aufgehängten Feder, an der ein Massestück befestigt ist. Dieses Massestück wird aus der Ruhelage ausgelenkt und beginnt eine periodische Schwingung zu beschreiben. Die Frequenz eines solchen Federpendels hängt von der Federkonstanten sowie der angehängten Masse ab - nicht aber von der Auslenkung zu Beginn der Schwingung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fb7d5747-a5bd-437f-bb16-7c2b8ea89af0","notation":["fb7d5747-a5bd-437f-bb16-7c2b8ea89af0"],"prefLabel":{"de":"Methoden"},"definition":{"de":"Es gibt einige grundlegende Methoden der Physik. Am wichtigsten zu Nennen ist hier vermutlich das Experiment, neben den oft mathematischen Überlegungen in der theoretischen Physik. Neben diesen “Makro-Methoden” gibt es aber auch eine Vielzahl kleinerer Methodenwerkzeuge: Videoanalysen, digitale Messwerterfassungssysteme, Regressionsanalysen und vieles mehr."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2117677f-dc71-4d55-93bb-3b509760c3aa","notation":["2117677f-dc71-4d55-93bb-3b509760c3aa"],"prefLabel":{"de":"Modellieren"},"definition":{"de":"Das Modellieren ist eine zentrale Tätigkeit der Physik. In Modellen werden Aspekte der Wirklichkeit in idealisierter Form dargestellt, um Zusammenhänge und Abhängigkeiten einzelner physikalischer Parameter genauer zu untersuchen. Die Art der verschiedenen Modelle ist oft sehr unterschiedlich: Graphische Modelle, Anschauungsmodelle und nicht zuletzt mathematische Modelle der Wirklichkeit werden in der Physik sehr oft genutzt. Das Modellieren ist eine Grundfertigkeit von Physiker:innen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/28f3a4bf-4ece-4631-9ea7-284fb9b39c28","notation":["28f3a4bf-4ece-4631-9ea7-284fb9b39c28"],"prefLabel":{"de":"SI-Einheiten"},"definition":{"de":"Historisch haben Physiker:innen ganz unterschiedliche Einheiten für physikalische Größen benutzt. Mittlerweile haben sie sich aber auf ein international anerkanntes System geeinigt, das lediglich von sieben physikalischen Basis-Größen abhängt. Diese Basis-Größen sind Meter, Sekunde, Mol, Ampere, Kelvin, Candela und das Kilogramm. Alle anderen Einheiten lassen sich eindeutig auf diese Größen zurückführen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b0d2a7bb-3fdf-44da-8302-f9414ebd5ff0","notation":["b0d2a7bb-3fdf-44da-8302-f9414ebd5ff0"],"prefLabel":{"de":"Dokumentieren"},"definition":{"de":"Wissenschaftler:innen dokumentieren ihre Arbeit in wissenschaftlichen Fachartikeln. Diese werden nur veröffentlicht, wenn die dargestellten experimentellen Ergebnisse auch nachvollziehbar dokumentiert wurden. Oft werden zur Veranschaulichung Tabellen und Graphen genutzt, insbesondere in der wissenschaftlichen Forschung aber auch Computerprogramme, die Messwerte automatisiert dokumentieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b1ededc7-bee3-4fa4-bf98-8e661ceb527e","notation":["b1ededc7-bee3-4fa4-bf98-8e661ceb527e"],"prefLabel":{"de":"Experimentieren"},"definition":{"de":"Neben der theoretischen Physik bildet die Experimentalphysik die zweite Säule der physikalischen Forschung. Dem entsprechend kommt dem Experimentieren auch im Physik-Unterricht in der Schule eine große Bedeutung zu. In Experimenten lassen sich physikalische Theorien und Überlegungen prüfen und neue Zusammenhänge entdecken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bc3958a8-9398-49e3-a790-be05e298c312","notation":["bc3958a8-9398-49e3-a790-be05e298c312"],"prefLabel":{"de":"Mathematik"},"definition":{"de":"Viele physikalische Phänomene lassen sich auch ohne Mathematik verstehen und erklären. Allerdings stößt das intuitive Erfassen von Physik schnell an seine Grenzen und mathematische Formalismen werden notwendig, um physikalische Sachverhalte sinnvoll beschreiben zu können. Es gibt eine ganze Reihe mathematischer Fertigkeiten, wie die Auswertung von Messwerten, der Umgang mit Gleichungen und Größen oder die Nutzung der Differential- und Integralrechnung, die zu einem Verständnis der Physik notwendig sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c7d08fcb-568f-4757-80f5-22c7299dc3c8","notation":["c7d08fcb-568f-4757-80f5-22c7299dc3c8"],"prefLabel":{"de":"Physikalische Konstanten"},"definition":{"de":"In vielen physikalischen Formeln tauchen grundlegende Konstanten immer wieder auf. Typische Beispiele sind die Gravitationskonstante oder die elektrische Feldkonstante. Im Schulunterricht wird auch oft die von der Gravitationskonstante abgeleitete Erdbeschleunigung g benötigt. Die Werte dieser Konstanten sind oft mit hoher Genauigkeit bekannt und in Formelsammlungen oder Tabellenwerken dokumentiert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/62dbc371-2e8e-4c86-90cd-a2682153b16d","notation":["62dbc371-2e8e-4c86-90cd-a2682153b16d"],"prefLabel":{"de":"Relativitätstheorie"},"definition":{"de":"Albert Einstein gilt gemeinhin als Begründer der Relativitätstheorie. Relativistische Phänomene wie die Veränderung der Masse von Objekten oder die Zeitdillatation sind in der Regel erst bei Geschwindigkeiten in der Größenordnung der Lichtgeschwindigkeit zu beobachten. Viele merkwürdige Voraussagen der speziellen und allgemeinen Relativitätstheorie wie das Zwillingsparadoxon oder Krümmung der Raumzeit sind mittlerweile mit großer Genauigkeit durch Beobachtungen und Experimente bestätigt worden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/21266a5f-0e9f-40c3-baef-78db5375b242","notation":["21266a5f-0e9f-40c3-baef-78db5375b242"],"prefLabel":{"de":"Spezielle Relativitätstheorie (SRT)"},"definition":{"de":"Die spezielle Relativitätstheorie wurde von Albert Einstein in seinem Wunderjahr, seinem annus mirabilis, entwickelt. Die spezielle Relativitätstheorie beschreibt die Bewegung von Körpern in Raum und Zeit und wird insbesondere dann bedeutsam, wenn sich Körper mit Geschwindigkeiten nahe der Lichtgeschwindigkeit bewegen. Bei diesen Geschwindigkeiten treten Phänomene wie die Längenkontraktion und die Zeitdilatation auf - und die Vorhersagen der speziellen Relativitätstheorie stimmen nicht mehr mit den Vorhersagen der klassischen Mechanik überein."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5e6028f0-ca13-460a-973f-0a052c3aa4f5","notation":["5e6028f0-ca13-460a-973f-0a052c3aa4f5"],"prefLabel":{"de":"Allgemeine Relativitätstheorie (ART)"},"definition":{"de":"Anders als die spezielle Relativitätstheorie beschreibt die allgemeine Relativitätstheorie auch das Zustandekommen der Gravitation. Im Rahmen der allgemeinen Relativitätstheorie wird Gravitation als Eigenschaft der vierdimensionalen Raumzeit gedeutet: Massen krümmen diese Raumzeit und verursachen so die gravitative Wechselwirkung. Bis heute ist es nicht gelungen, allgemeine Relativitätstheorie und Quantenmechanik miteinander zu einer gemeinsamen Theorie zu vereinen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a37552ec-78c4-44f2-8564-8e84e4ca26b3","notation":["a37552ec-78c4-44f2-8564-8e84e4ca26b3"],"prefLabel":{"de":"Quantenphysik"},"definition":{"de":"In der Größendimension von Elementarteilchen, Atomen und Molekülen finden sich verschiedene, merkwürdige Effekte. Die klassischen Konzepte Welle und Teilchen tragen nicht mehr und werden durch den Welle-Teilchen-Dualismus ersetzt. Submikroskopische Objekte gehorchen den Gesetzen der Statistik und es lassen sich für den Aufenthaltsort, die Energie und viele weitere Größen nur Wahrscheinlichkeiten angeben. Die Welt der Quantenphysik, die all diese Merkwürdigkeiten beschreibt, entzieht sich oft jeder anschaulichen Vorstellung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/28c4cee1-a987-4bfb-b227-8f36f57e33d3","notation":["28c4cee1-a987-4bfb-b227-8f36f57e33d3"],"prefLabel":{"de":"Elektronen"},"definition":{"de":"Elektronen spielten in der Entwicklung der Quantenmechanik eine wichtige Rolle. An ihnen können wichtige Eigenschaften und Prinzipien der Quantenphysik beobachtet werden wie die Interferenz an dreidimensionalen Gitterstrukturen und der daraus ableitbare Dualismus von Wellen- und Teilcheneigenschaft. Elektronen sind aus Sicht der Quantenphysik weder Welle, noch Teilchen. Als Quantenobjekt zeigen sie je nach experimentellem Aufbau entweder eher Teilchen- oder Welleneigenschaften."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5c8fc12e-1ff9-4223-b920-5ea8cec7c65b","notation":["5c8fc12e-1ff9-4223-b920-5ea8cec7c65b"],"prefLabel":{"de":"Photonen"},"definition":{"de":"Licht wird von der klassischen Wellenoptik, der Elektrodynamik, als elektromagnetische Welle beschrieben. Phänomene wie die Interferenz am Doppelspalt lassen sich mit Hilfe dieser Beschreibung hervorragend erklären. Aus Sicht der Quantenphysik wird Licht demgegenüber als Teilchenstrahlung beschrieben. Licht besteht aus Photonen definierter Energie. Diese Sichtweise auf das Licht ist in der Lage, vielfältige Phänomene wie den Photoeffekt oder die Absorption von Licht durch Atome zu erklären."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6f5c9f2e-f187-4a4b-9252-9d63521b3ccc","notation":["6f5c9f2e-f187-4a4b-9252-9d63521b3ccc"],"prefLabel":{"de":"Grundlagen"},"definition":{"de":"Die Quantenphysik stellt viele Grundlagen der klassischen Physik in Frage. Liefert die klassische Physik eindeutige Vorhersagen, so sind in der Quantenmechanik nur statistische Aussagen zum Beispiel über den Aufenthaltsort eines Teilchens möglich. Trotz dieser Eigenschaft liefern physikalische Messungen an Quantenobjekten eindeutige Ergebnisse. Quantenobjekte sind zudem zur Interferenz fähig, oft sogar mit sich selbst. Versucht man hingegen, die genaue Bahnkurve eines Quantenobjekts  durch experimentelle Kniffe festzustellen, so verschwinden die zuvor beobachtbaren Interferenzmuster. Kurzum: Die Natur ist merkwürdig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/88654ade-1707-4f14-9e1d-b25e97c6c071","notation":["88654ade-1707-4f14-9e1d-b25e97c6c071"],"prefLabel":{"de":"Quanteninformation"},"definition":{"de":"Ideen der Quantenmechanik werden in Wissenschaft und mittlerweile auch Technik zur Informationsübermittlung und - insbesondere - zur Verschlüsselung von Informationen genutzt. Zu diesem Thema der modernen Physik gibt es eine Vielzahl von Simulationen und Unterrichtsideen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0b3a729c-b006-499c-8da9-251657fbe52b","notation":["0b3a729c-b006-499c-8da9-251657fbe52b"],"prefLabel":{"de":"Kernphysik"},"definition":{"de":"Die Kernphysik untersucht Phänomene, die sich auf der Ebene der Atomkerne abspielen. Drei Prozesse sind hierbei von besonderer Bedeutung: Der Zerfall, die Spaltung und die Fusion von Atomkernen. Radioaktive Zerfälle sind verantwortlich für die Freisetzung ionisierender Strahlung. Kernspaltungen werden technisch in Kernkraftwerken zur Stromerzeugung wie auch in Atombomben genutzt. Es lässt sich allerdings nicht nur durch die Spaltung von Atomkernen eine unglaubliche Menge an Energie gewinnen. Auch die Verschmelzung leichter Atomkerne zu schwereren, die Kernfusion, setzt gewaltige Energiemengen frei: Zum Beispiel in unserer Sonne."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/47e6b1d9-8bb6-411c-be11-ece5736c739a","notation":["47e6b1d9-8bb6-411c-be11-ece5736c739a"],"prefLabel":{"de":"Grundlagen"},"definition":{"de":"Die Kernphysik befasst sich mit dem Aufbau und den möglichen Umwandlungsprozessen von Atomkernen. Die Kernphysik baut hierbei einerseits auf grundlegenden experimentellen Befunden auf, andererseits auf immer komplexer werdenden Modellen. Zur Beschreibung sind zudem einige Grundbegriffe notwendig: Nuklide, Isotope und Kernreaktionsgleichung sind einige davon."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/64d917fc-a952-4a4f-be24-28870fb69408","notation":["64d917fc-a952-4a4f-be24-28870fb69408"],"prefLabel":{"de":"Kernspaltung"},"definition":{"de":"Sehr schwere Atomkerne wie Uran lassen sich durch Beschuss mit Neutronen in leichtere Bruchstücke spalten. Bei diesem Prozess wird Energie freigesetzt. Diese freigesetzte Energie kann einerseits in Atombomben ihre zerstörerische Kraft zeigen. Andererseits kann die freigesetzte Energie auch zur Erzeugung elektrischer Energie in Atomkraftwerken genutzt werden - mit allen Gefahren und Chancen, die hiermit verbunden sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6870879c-315c-4a6c-b8df-41da285fb4cd","notation":["6870879c-315c-4a6c-b8df-41da285fb4cd"],"prefLabel":{"de":"Kernfusion"},"definition":{"de":"Sehr leichte Atomkerne können unter unvorstellbar hohem Druck zu schwereren Atomkernen fusionieren. Bei diesem Prozess entsteht Energie. In der Natur läuft die Kernfusion vor allem innerhalb von Sternen ab, auch unsere Sonne gewinnt ihre Energie durch die Fusion von Wasserstoffatomkernen zu Heliumkernen. Technisch wird seid vielen Jahren versucht, die Kernfusion zu nutzen. Erste Anwendung war die verheerende Wasserstoffbombe. Reaktoren zur Erzeugung von elektrischer Energie durch Kernfusion befinden sich in der Entwicklung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/930b370e-9129-4b82-a1af-ba1cf34a014a","notation":["930b370e-9129-4b82-a1af-ba1cf34a014a"],"prefLabel":{"de":"Radioaktivität"},"definition":{"de":"Radioaktivität ist ein allgegenwärtiges Phänomen. Doch Menschen besitzen für radioaktive Strahlung, die bei Zerfallsprozessen von Atomkernen entsteht, kein spezielles Sinnesorgan. Du bemerkst also gar nicht, dass um dich herum radioaktive Zerfallsprozesse ablaufen. Mit Hilfe spezieller Messgeräte lässt sich dieser Nulleffekt jedoch sichtbar machen und quantifizieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a3e2c6f1-b534-4fef-bb17-eee42c6111af","notation":["a3e2c6f1-b534-4fef-bb17-eee42c6111af"],"prefLabel":{"de":"Kernmodelle"},"definition":{"de":"Atomkerne lassen sich durch unterschiedliche Modelle beschreiben. Grundlegend für die Entdeckung des Atomkerns war zunächst der Rutherford-Versuch, der zu einem einfachen Kern-Hülle-Modell führte. Für die meisten Anwendungen der Schulphysik hat sich das Tropfenmodell bewährt. In diesem Modell bestehen Atomkerne aus Protonen und Neutronen. Die Elektronen in der Atomhülle sind für kernphysikalische Prozesse in der Regel nicht relevant. Neben diesem eher einfachen Kernmodell lässt sich der Atomkern auch quantenphysikalisch zum Beispiel mit Hilfe eines linearen Potentialtopf-Modells beschreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ddcb52c7-5ae3-4ff7-9cf4-8dfdc14c647d","notation":["ddcb52c7-5ae3-4ff7-9cf4-8dfdc14c647d"],"prefLabel":{"de":"Kernreaktionen"},"definition":{"de":"Atomkerne können sich verändern und einzelne Bestandteile abgeben, mit anderen Atomkernen fusionieren und in energetisch verschiedene Zustände übergehen. Diese Kernreaktionen äußern sich nach außen meist durch die Abgabe radioaktiver Strahlung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c0a79a8d-9f29-4056-8e1e-c0cec3f41f34","notation":["c0a79a8d-9f29-4056-8e1e-c0cec3f41f34"],"prefLabel":{"de":"Akustik"},"definition":{"de":"Musik, Töne und Geräusche lassen sich physikalisch als Wellenphänomene beschreiben. Dieses spannende Teilgebiet der Physik bezeichnen Physiker:innen auch als Akustik. Schallwellen entstehen durch schwingende Systeme - wie beispeilsweise Saiten und Membranen - und breiten sich in Zeit und Raum aus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f427eb5f-6d9b-4133-aae7-2ad9f6911a68","notation":["f427eb5f-6d9b-4133-aae7-2ad9f6911a68"],"prefLabel":{"de":"Akustische Schwingungen & Wellen"},"definition":{"de":"Schall kann als eine sich im Raum ausbreitende Schwingung der Trägerteilchen verstanden werden - mit anderen Worten: Schall lässt sich als mechanische Welle, als Schallwelle beschreiben. Die Beschreibung akustischer Phänomene im Wellenmodell ermöglicht es, eine Vielzahl akustischer Phänomene zu modellieren und Vorhersagen zu treffen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3e025be8-88ef-4b14-b991-16211d36eefa","notation":["3e025be8-88ef-4b14-b991-16211d36eefa"],"prefLabel":{"de":"Quellen & Empfänger"},"definition":{"de":"Schallwellen breiten sich von Schallquellen wie Lautsprechern zu Empfänger wie den menschlichen Ohren oder Mikrophonen aus."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5de85c18-6451-47fd-a9f0-4e32108959ee","notation":["5de85c18-6451-47fd-a9f0-4e32108959ee"],"prefLabel":{"de":"Schallgeschwindigkeit"},"definition":{"de":"Der Schall breitet sich mit einer weitgehend gleichbleibenden Geschwindigkeit im Raum aus. Wie schnell genau hängt jedoch vom Medium ab: In Wasser ist diese Schallgeschwindigkeit eine andere als in Metall oder in Luft. Unterschiedliche Schallgeschwindigkeiten führen oft zu spannenden Phänomenen. Und natürlich lässt sich die Geschwindigkeit des Schalles auch experimentell bestimmen - viele Versuche hierzu sind überraschend einfach."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/89eb5d3e-58bb-4b2a-94a1-12bc18071737","notation":["89eb5d3e-58bb-4b2a-94a1-12bc18071737"],"prefLabel":{"de":"menschl. Stimme & Gehör (s. Biologie)"},"definition":{"de":"Du hast jeden Tag mit einem Schallsender und einem Schallempfänger zu tun. Die menschliche Stimme ist nichts anderes als ein Schallsender, der durch schnelle Schwingungen der Stimmlippen und Verstärkung des Schalls durch Resonanzphänomene eine akustische Welle erzeugt. Die Ohren werden demgegenüber durch akustische Wellen in Schwingung versetzt und wandeln diese Schwingung in Nervenimpulse um. Diese Nervenimpulse erzeugen dann im Gehirn den Höreindruck."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f35980cd-1ee9-426b-923d-422e60b6bccb","notation":["f35980cd-1ee9-426b-923d-422e60b6bccb"],"prefLabel":{"de":"Phänomene"},"definition":{"de":"Die Akustik ist voller spannender Phänomene. Wie entsteht ein Echo? Was genau ist eigentlich der Dopplereffekt? Weshalb klingt die eigene Stimme ganz anders, wenn man Helium einatmet? Warum kann ich jemand hinter einem Baum sprechen hören, obwohl ich die Person nicht sehen kann? Alle diese Effekte lassen sich mit dem Wellenbild von Schall sehr gut erklären und begründen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/640ad31f-0a11-44a1-bdfe-4931b992622c","notation":["640ad31f-0a11-44a1-bdfe-4931b992622c"],"prefLabel":{"de":"Energie"},"definition":{"de":"Die Energie ist eine der wichtigsten und gleichzeitig schwierigsten Größen der Physik: Ihre Gesamtmenge ändert sich nie, und doch lässt sie sich nicht beliebig nutzen. Sie tritt in vielen verschiedenen Formen auf, die sich ineinander umwandeln lassen. Und sie vereinfacht die Berechnung einer Vielzahl von praktischen und physikalischen Problemen. Gleichzeitig weiß niemand so genau, was Energie letzten Endes eigentlich genau ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5f6f53a0-b4f8-4f5a-a0a4-f92eecccffa7","notation":["5f6f53a0-b4f8-4f5a-a0a4-f92eecccffa7"],"prefLabel":{"de":"Energieumwandlung"},"definition":{"de":"Energie kann nicht vernichtet werden, wohl aber von einer Energieform in eine andere umgewandelt werden. Diese Umwandlungen sind zwar nicht direkt mit Verlusten verbunden, doch tritt bei jeder praktisch möglichen Energieumwandlung die thermische Energie als (weitere) Energieform in Erscheinung. Diese ist nicht unbegrenzt in anderen Energieformen umwandelbar. Energieumwandlungen können mit Hilfe von Bilanzgleichungen, Energieflussdiagrammen und Energiekontomodellen beschrieben werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a164dcce-1332-4a69-ad68-db91954cb86f","notation":["a164dcce-1332-4a69-ad68-db91954cb86f"],"prefLabel":{"de":"Energiespeicher"},"definition":{"de":"Energie lässt sich speichern. Alltäglich begleitet dich dieses Phänomen bei elektronischen Geräten wie Smartphones: Die elektrische Energie ist hier in Form von chemischer Energie im Akku des Gerätes gespeichert und kann bei Bedarf wieder in elektrische Energie umgewandelt werden. Für viele andere Energieformen existieren andere Speichermöglichkeiten wie Pumpspeicherkraftwerke, thermische Speicher und viele mehr."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ed4516c8-c848-4b31-8c4d-2dc7f01229ef","notation":["ed4516c8-c848-4b31-8c4d-2dc7f01229ef"],"prefLabel":{"de":"Energieformen"},"definition":{"de":"Energie ist eine physikalische Größe, die in verschiedenen Formen auftritt. Zu diesen verschiedenen Energieformen zählen unter anderem Höhenenergie, Kernenergie, Strahlungsenergie, Spannenergie, elektrische Energie, thermische Energie, chemische Energie und Bewegungsenergie. Die Veränderung der Menge einer bestimmten Energieform lässt sich mathematisch beschreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ed9c23b8-9d3f-4911-93f3-fac55ac379c3","notation":["ed9c23b8-9d3f-4911-93f3-fac55ac379c3"],"prefLabel":{"de":"Leistung / Energieübertragung"},"definition":{"de":"Leistung beschreibt die pro Zeit umgewandelte Energie bei einer Energieumwandlung. Sie ist daher eine wichtige Größe zur Beschreibung der Stärke einer Energieübertragung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9e7ae956-e9df-430f-bace-f3db4b910013","notation":["9e7ae956-e9df-430f-bace-f3db4b910013"],"prefLabel":{"de":"Optik"},"definition":{"de":"Die Physik des Lichts, die Optik, hält eine ganze Reihe eindrucksvoller Phänomene bereit. Bereits mit Hilfe einfacher Modelle lassen sich die physikalischen Vorgänge beim Sehvorgang, die Funktionweise von Mikroskopen, Fotoapparaten und Lochkameras sowie die Zusammensetzung weißen Lichts aus verschiedenen Spektralfarben verstehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bf729405-18ff-440b-9d5b-dd466ff563c3","notation":["bf729405-18ff-440b-9d5b-dd466ff563c3"],"prefLabel":{"de":"Wellenoptik"},"definition":{"de":"In der Wellenoptik wird Licht als elektromagnetische Welle beschrieben, die sich geradlinig im Raum ausbreitet. Durch diese Beschreibung von Licht können Interferenzphänomene erklärt werden wie die Beugung am Einfach- und Doppelspalt oder die Aufspaltung von Licht in seine Spektralfarben an einer CD-Rom. Die Wellenoptik nutzt dabei mathematisches Handwerkzeug, das auch zur Beschriebung anderer Wellenarten genutzt wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1f04a569-4e3f-4f9d-aa77-4262f0a4abce","notation":["1f04a569-4e3f-4f9d-aa77-4262f0a4abce"],"prefLabel":{"de":"Interferenz"},"definition":{"de":"Treffen Lichtwellen an einem bestimmten Ort aufeinander, so können ihre jeweiligen Zeigerstellungen vektoriell addiert werden. Dies führt zu ausgeprägten Interferenzmustern: Minima und Maxima wechseln sich in bestimmten Abständen ab - und lassen zum Beispiel Rückschlüsse auf die Wellenlänge des interferierenden Lichtes zu."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9419a69a-ae66-49fd-814e-8115560c5eeb","notation":["9419a69a-ae66-49fd-814e-8115560c5eeb"],"prefLabel":{"de":"Elektromagnetisches Spektrum"},"definition":{"de":"Sichtbares Licht umfasst nur einen kleinen Bereich des elektromagnetischen Spektrums: Neben Licht sind auch Infrarotstrahlung, Mikrowellen, Gammastrahlung, Radiowellen und viele weitere Wellenarten Teil des umfassenden Spektrums elektromagnetischer Wellen. Der Hauptunterschied dieser Wellenarten ist ihr Wellenlänge,"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c8820c6a-54a3-4490-960b-2fa6345d0cc1","notation":["c8820c6a-54a3-4490-960b-2fa6345d0cc1"],"prefLabel":{"de":"Polarisation"},"definition":{"de":"Beschreibt man Licht als elektromagnetische Welle, so schwingen die elektrischen und magnetischen Feldvektoren in bestimmten Ebenen senkrecht zur Ausbreitungsrichtung. In manchen Fällen ist diese Schwingungsebene fest oder rotiert mit konstanter Geschwindigkeit. Physiker*innen sprechen hier von polarisiertem Licht. Es wird technisch zum Beispiel bei 3D-Brillen im Kino genutzt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d309bab9-50e5-4a95-aeb4-d83c4ecea632","notation":["d309bab9-50e5-4a95-aeb4-d83c4ecea632"],"prefLabel":{"de":"Beugung"},"definition":{"de":"Wechselwirkt Licht mit einem Gegenstand in der Größenordnung der Wellenlänge des Lichts, so entstehen nach dem Prinzip von Huygens kreisförmige Elementarwellen. Licht wird an diesen Gegenständen gebeugt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/95f2002f-1745-4b47-889d-8376af38fe41","notation":["95f2002f-1745-4b47-889d-8376af38fe41"],"prefLabel":{"de":"Farben"},"definition":{"de":"Die Welt um uns herum besteht aus Farben. Wie kommt das eigentlich? Das Licht selbst kann aus unterschiedlichen Farben bestehen. Immer dann, wenn “alle” Farben gleichzeitig von einer Lichtquelle ausgesandt werden, wirkt das Licht für uns weiß. Aus diesem Grund lässt weißes Licht sich auch mit Hilfe eines Prismas in die Spektralfarben zerlegen - den gleichen Effekt kannst du bei einem Regenbogen beobachten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f35c17d1-a29e-4b26-9d22-802682fad43d","notation":["f35c17d1-a29e-4b26-9d22-802682fad43d"],"prefLabel":{"de":"Geometrische Optik"},"definition":{"de":"Die geometrische Optik nutzt das physikalische Modell des Lichtstrahls: Licht besteht aus unendlich vielen, unendlich feinen Lichtstrahlen, die sich geradlinig im Raum ausbreiten. In den meisten Fällen ist es völlig ausreichend, nur einzelne dieser Lichtstrahlen zu zeichnen. Mit der geometrischen Optik lassen sich grundlegende Phänomene aus dem Alltag wie die Reflexion und Brechung von Licht oder die Entstehung von Schatten gut erklären."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9b289ccc-472b-43f3-a846-0a1c2f2e215f","notation":["9b289ccc-472b-43f3-a846-0a1c2f2e215f"],"prefLabel":{"de":"Licht und Schatten"},"definition":{"de":"Schatten ist das Fehlen von Licht. Er entsteht immer dann, wenn ein Gegenstand die Ausbreitung von Licht verhindert. Schatten sind im Alltag allgegenwärtig, spielen darüber hinaus aber auch bei Phänomenen wie Sonnen- und Mondfinsternissen eine wichtige Rolle."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9e2ecea3-f6e1-4e87-a154-d4fd7c0bf6d0","notation":["9e2ecea3-f6e1-4e87-a154-d4fd7c0bf6d0"],"prefLabel":{"de":"Linsen / optische Geräte"},"definition":{"de":"Linsen aus Glas oder Kunststoff sind Grundlage einer Vielzahl optischer Geräte: Fernrohre, Brillen, Kameras und Mikroskope sind historisch wie auch technisch und wirtschaftlich bedeutsame Beispiele für derartige Geräte."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b104e2f9-5ceb-4e65-b234-43dc9130a2bb","notation":["b104e2f9-5ceb-4e65-b234-43dc9130a2bb"],"prefLabel":{"de":"Reflexion"},"definition":{"de":"Licht kann an spiegelnden Oberflächen reflektiert, also zurückgeworfen werden. Dieses Phänomen begegnet dir im Alltag zum Beispiel an Glasscheiben, Wasseroberflächen oder Spiegeln. Grundlegend für die Erklärung solcher Spiegelbilder ist das Reflexionsgesetz."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c6200d4c-443c-4f32-b9d3-e6ff2f362e71","notation":["c6200d4c-443c-4f32-b9d3-e6ff2f362e71"],"prefLabel":{"de":"Brechung"},"definition":{"de":"Trifft Licht auf eine Grenzfläche zwischen zwei unterschiedlichen Stoffen, beispielsweise zwischen Luft und Glas oder zwischen Wasser und Luft, so ändert sich die Ausbreitungsrichtung des Lichtes. Physiker*innen sagen: Es wird gebrochen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c9f069ae-33b5-474e-b8f7-9840175a7c3a","notation":["c9f069ae-33b5-474e-b8f7-9840175a7c3a"],"prefLabel":{"de":"Ausbreitung"},"definition":{"de":"Wie breitet sich Licht im Raum aus? Mit Hilfe der geometrischen Optik lassen sich hierzu bereits grundlegende Aussagen treffen: Licht breitet sich geradlinig aus, solange es nicht auf Grenzflächen trifft. Lichtsender senden Licht aus, Lichtempfänger nutzen das Licht."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f7a11ee6-86a5-4d5f-a033-7d46efc99b94","notation":["f7a11ee6-86a5-4d5f-a033-7d46efc99b94"],"prefLabel":{"de":"Abbildungen"},"definition":{"de":"Linsen können genutzt werden, um Bilder von Gegenständen auf einem Schirm zu erzeugen. Diesen Vorgang nennen Physiker*innen auch Abbildung. Eine genaue Kenntnis der dabei auftretenden Gesetzmäßigkeiten ist zum Beispiel wichtig, um richtig korrigierende Brillen herzustellen oder Beamer passend zu justieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d8b1849b-26a2-4d08-8dd3-0e0cb733113b","notation":["d8b1849b-26a2-4d08-8dd3-0e0cb733113b"],"prefLabel":{"de":"Menschliches Auge"},"definition":{"de":"Das menschliche Auge ist unser wahrscheinlich wichtigstes Sinnesorgan: Über das Auge nehmen wir die Welt wahr, erkennen Formen und Farben. Das Auge erfüllt also wichtige biologische Funktionen. Unter physikalischen Gesichtspunkten betrachtet besteht das Auge im Wesentlichen aus einer Sammellinse, die einen entfernten Gegenstand auf die Netzhaut abbildet. Mit diesem Wissen lässt sich der Prozess des Sehens physikalisch erklären."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/256377d9-ca30-4280-9cfb-64fb4c45a998","notation":["256377d9-ca30-4280-9cfb-64fb4c45a998"],"prefLabel":{"de":"Mechanik"},"definition":{"de":"Das Gebiet der Mechanik ist vielfältig und voller Alltagsbezüge. Mit Hilfe des Wissens aus der Mechanik lässt sich beschreiben, wie und warum sich Objekte bewegen, wie Apparaturen wie Flaschenzüge oder Hebel arbeiten und welchen Einfluss die Schwerkraft auf die Bewegung von Himmelskörpern hat. Gleichzeitig stellt die Mechanik vielfältige Konzepte und Methoden bereit, die auch in anderen physikalischen Gebieten wichtig sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/877c65b8-78c3-46b3-9eb1-86f1083cbfbe","notation":["877c65b8-78c3-46b3-9eb1-86f1083cbfbe"],"prefLabel":{"de":"Bewegungen"},"definition":{"de":"Die Untersuchung von Bewegungen hat eine sehr lange Tradition in der Physik. Berühmte Wissenschaftler:innen wie Galileo Galilei und Aristoteles in der griechischen Antike beschäftigten sich mit der experimentellen und theoretischen Untersuchung von Bewegungsvorgängen. Grundsätzlich lassen sich (mindestens) drei Typen von Bewegungen unterscheiden: Die geradlinig gleichförmige Bewegung, die gleichmäßig beschleunigte Bewegung sowie die Kreisbewegung. Jeder dieser Bewegungstypen lässt sich experimentell und theoretisch genauer untersuchen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a81707d-fd92-4190-9c7b-2b931fb84f8b","notation":["0a81707d-fd92-4190-9c7b-2b931fb84f8b"],"prefLabel":{"de":"Gleichförmige Bewegung"},"definition":{"de":"Die gleichförmige Bewegung ist auf der Erde vermutlich nie in Reinform zu beobachten. Sie beschreibt die Bewegung eines Gegenstandes mit gleichbleibender Geschwindigkeit und Bewegungsrichtung. Idealtypisch liegt diese Art der Bewegung im Weltall vor, da dort Effekte wie Reibung und Luftwiderstand keine wesentliche Rolle spielen. Diese gilt es bei der Untersuchung gleichförmiger Bewegungen auf der Erde zu minimieren - oder die Reibungsverluste durch einen Motor auszugleichen, um eine gleichbleibende Geschwindigkeit zu erreichen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1f910d7d-37ed-4eb5-934e-003984f04028","notation":["1f910d7d-37ed-4eb5-934e-003984f04028"],"prefLabel":{"de":"Wurfbewegungen"},"definition":{"de":"Wurfbewegungen stellen eine Überlagerung mehrerer Bewegungen dar. Sie erfordern also in der Regel eine zweidimensionale Beschreibung der Bewegung. Typische Beispiele für solche Wurfbewegungen sind der vertikale Wurf, der horizontale Wurf und - als Verallgemeinerung - der schiefe Wurf."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/88733fb3-e348-417b-9883-4ac2b9aa2d41","notation":["88733fb3-e348-417b-9883-4ac2b9aa2d41"],"prefLabel":{"de":"Kreis- / Drehbewegungen"},"definition":{"de":"Wieso wirst du beim Autofahren in der Kurve nach außen gedrückt? Und weshalb kann eine Salatschleuder Wasser von deinem Salat trennen? An diesen Beispiel wird schnell deutlich, dass die Beschreibung von Kreisbewegungen nicht immer ganz einfach ist, da rotierende Bezugssysteme eine wesentliche Rolle spielen. Neben den auch für andere Bewegungsarten wichtigen physikalischen Größen wie Ort und Geschwindigkeit sind zur Beschreibung von Kreisbewegungen weitere physikalische Größen wie die Frequenz oder die Umlaufdauer wichtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8fc65ca7-d21c-445c-80f2-91d7f64f1689","notation":["8fc65ca7-d21c-445c-80f2-91d7f64f1689"],"prefLabel":{"de":"Beschleunigte Bewegungen"},"definition":{"de":"Bei beschleunigten Bewegungen verändert der bewegte Gegenstand seine Geschwindigkeit. Derartige Bewegungen stellen in unserem Alltag wohl den Normalfall dar. Eine besondere Form der beschleunigten Bewegung ist die gleichmäßig beschleunigte Bewegung. Bei diesem Bewegungstyp ändert sich die Geschwindigkeit gleichmäßig mit der Zeit. Eines der bekanntesten Beispiele für eine gleichmäßig beschleunigte Bewegung ist der freie Fall ohne Luftwiderstand."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b520e573-9c67-45f2-a335-63c584b6bb05","notation":["b520e573-9c67-45f2-a335-63c584b6bb05"],"prefLabel":{"de":"s-t-Diagramme von Bewegungen"},"definition":{"de":"Bewegungen lassen sich auf ganz verschiedene Arten physikalisch beschreiben. Eine sinnvolle Möglichkeit stellen Zeit-Orts-Diagramme dar. Die vorliegenden Materialien thematisieren die Aufnahme und Analyse derartiger Diagramme."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5eb63223-6b0d-4b3f-9266-155d1aa7ab01","notation":["5eb63223-6b0d-4b3f-9266-155d1aa7ab01"],"prefLabel":{"de":"Newtonsche Gesetze"},"definition":{"de":"Isaac Newton gilt als einer der Begründer der modernen Physik. Auf seinen Untersuchungen von Bewegungsvorgängen basieren die Newton'schen Axiome: Das Trägheitsprinzip, die Grundgleichung der Mechanik sowie das Wechselwirkungsprinzip. Aus diesen drei Prinzipien lassen sich alle Phänomene der klassischen Physik herleiten. Mit ihrer Hilfe lassen sich alle Phänomene begründen - zumindest solange du nicht über sehr schnelle Prozesse nachdenkst oder sehr kleine Größenordnungen untersuchst. Die Newtonschen Gesetze stellen eine der größten Errungenschaften der Physik dar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0bfde870-03c2-4706-86ec-fe5f19f4a64f","notation":["0bfde870-03c2-4706-86ec-fe5f19f4a64f"],"prefLabel":{"de":"Wechselwirkungsprinzip"},"definition":{"de":"Läufts du gegen einen LKW, so übst du eine Kraft auf den LKW aus. Aufgrund seiner sehr großen Masse wird diese kaum zu einer nennenswerten Beschleunigung des LKW führen. Dir geht es vermutlich anders: Du wirst abrupt abgebremst und hast Schmerzen. Dieses kleine Gedankenexperiment verdeutlicht eine grundlegende Einsicht von Newton: Übt ein Gegenstand auf einen anderen eine Kraft aus, so existiert immer eine gleich große, aber entgegengesetzt gerichtere Kraft."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/21cfa69d-98d0-4152-90e3-5ed3fd9b4f21","notation":["21cfa69d-98d0-4152-90e3-5ed3fd9b4f21"],"prefLabel":{"de":"Trägheitsprinzip"},"definition":{"de":"Ein Gegenstand, auf den keine Kraft einwirkt, bewegt sich mit gleichbleibender Geschwindigkeit immer in die selbe Richtung. Leider ist das Trägheitsprinzip auf der Erde nicht beobachtbar, da ein Gegenstand auf der Erde nie völlig kräftefrei ist. Ein Asteroid im Weltraum ist hier eine gute Näherung: Ohne dass er Reibungskräften oder gravitativen Einflüssen ausgesetzt ist, bewegt er sich ewig geradeaus weiter."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9f34bee1-9cff-4222-8697-06135d2e264d","notation":["9f34bee1-9cff-4222-8697-06135d2e264d"],"prefLabel":{"de":"Grundgleichung der Mechanik"},"definition":{"de":"Die Grundgleichung der Mechanik beschreibt die Auswirkung von Kräften auf den Impuls eines Gegenstandes. Wirkt auf einen Gegenstand eine Kraft ein, so ändert sich sein Impuls. Letztlich erfährt ein Gegenstand dadurch eine Beschleunigung und verändert seine Geschwindigkeit oder seine Bewegungsrichtung. Anders herum lässt sich auch feststellen: Immer dann, wenn ein Gegenstand seine Geschwindigkeit oder Bewegungsrichtung verändert, muss eine Kraft gewirkt haben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/50d93eab-83e8-4f62-8887-5b90d9488324","notation":["50d93eab-83e8-4f62-8887-5b90d9488324"],"prefLabel":{"de":"Kräfte"},"definition":{"de":"Kräfte sind allgegegnwärtig und doch unsichtbar. Sie lassen sich nur anhand ihrer Wirkung beobachten. Kurz gesagt: Kräfte sind in der Lage, die Energie eines Gegenstandes entlang eines Weges zu verändern. Konkret bedeutet dies: Kräfte können Gegenstände beschleunigen oder abbremsen, die Bewegungsrichtung verändern, Gegenstände verformen und vieles mehr."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1bff4d1d-3ce4-49d5-935b-9f6deaddb1c9","notation":["1bff4d1d-3ce4-49d5-935b-9f6deaddb1c9"],"prefLabel":{"de":"Arten"},"definition":{"de":"Kraft und Energie sind nicht das gleiche. Und doch gibt es, ähnlich wie es verschiedene Energieformen gibt, auch verschiedene Kräfte. So unterscheiden Physiker:innen die Grundkräfte elektromagnetische Kraft, schwache Kraft, starke Kraft und Gravitationskraft. Daraus abgeleitet lassen sich auch Reibungskräfte, Zentripetalkräfte und viele mehr beobachten und berechnen. Sie alle sind zur Erklärung jeweils ganz bestimmter physikalischer Phänomene wichtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/65424653-539f-48df-a30d-c27b6097ca07","notation":["65424653-539f-48df-a30d-c27b6097ca07"],"prefLabel":{"de":"Addition & Zerlegung"},"definition":{"de":"Kräfte lassen sich durch Pfeile beschreiben: Die Richtung des Pfeils zeigt in die selbe Richtung, in die Kraft wirkt, während die Länge Auskunft darüber gibt, wie stark die Kraft ist. Mathematisch gesehen handelt es sich bei Kräften also um Vektoren. Diese lassen sich addieren, um zum Beispiel zu entscheiden, in welche Richtung mehrere Schlittenhunde einen Schlitten ziehen. Sie lassen sich aber auch zerlegen, um beispielsweise zu verstehen, wie groß die Kraft einen Abhang hinunter ist - und wie diese mit der Gewichtskraft eines Gegenstandes zusammenhängt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d9cc463a-9619-4ead-9437-9592abf00229","notation":["d9cc463a-9619-4ead-9437-9592abf00229"],"prefLabel":{"de":"Hooksches Gesetz"},"definition":{"de":"Federn verhalten sich ähnlich wie Gummibänder: Sie lassen sich dehnen und streben anschließend zu ihrer ursprünglichen Form zurück. Dieses verhalten lässt sich mathematisch durch das Hookesche Gesetz genauer beschreiben. Ein zentrales Ergebnis der Überlegungen von Hooke, nach dem dieses Gesetz benannt wurde, ist: Je weiter eine Feder ausgelengt wird, desto stärker wird die Kraft, die die Feder wieder zusammenzieht. Dieses Phänomen lässt sich sowohl theoretisch herleiten als auch experimentell mit Federn und Gewichten überprüfen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b16a309b-0edf-4b0a-83f3-2ca765cad0f2","notation":["b16a309b-0edf-4b0a-83f3-2ca765cad0f2"],"prefLabel":{"de":"Masse, Volumen, Dichte"},"definition":{"de":"Alle Gegenstände besitzen Masse: Sie sorgt dafür, dass Gegenstände \"schwer\" sind - und gleichzeitig träge. Je größer die Masse eines Gegenstandes ist, desto schwerer lässt er sich beschleunigen und abbremsen und desto schwerer ist er auf der Erde. Jeder Gegenstand füllt zudem ein bestimmtes Volumen aus. Aus dem Verhältnis dieser beiden Größen - Masse und Volumen - lässt sich schließlich eine für den jeweiligen Stoff, aus dem Gegenstände bestehen, spezifische Eigenschaft bestimmen: Die Dichte."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/656b3238-d2e4-4940-9cb5-e23bbe053005","notation":["656b3238-d2e4-4940-9cb5-e23bbe053005"],"prefLabel":{"de":"Druck & Auftrieb"},"definition":{"de":"Weshalb schwimmen Menschen auf dem toten Meer? Wieso gehen Schiffe nicht unter, obwohl sie aus Stahl sind? Derartige Fragen lassen sich mit dem Prinzip des Auftriebs beantworten. Jeder Gegenstand erfährt im Wasser (und auch in der Luft) eine Auftriebskraft. Wie groß diese ist, hängt sowohl von der Dichte des Gegenstandes als auch von der Dichte des umgebenden Mediums ab. Um das Auftriebsprinzip schließlich wirklich tiefgreifend zu verstehen, musst du dich mit dem in Gasen und Flüssigkeiten herrschenden Druck befassen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9bf51538-d670-4acb-89fa-1217ba12774f","notation":["9bf51538-d670-4acb-89fa-1217ba12774f"],"prefLabel":{"de":"Einfache Maschinen"},"definition":{"de":"Viele einfache Maschinen kommen ganz ohne elektrische oder chemische Energie aus. Dennoch erleichtern sie dir oft den Alltag. Wichtige Beispiele solcher Maschinen sind Hebel und Flaschenzüge. Beide lassen sich in Experimenten genauer untersuchen und durch physikalische Gleichungen auch mathematisch beschreiben. So lässt sich auch klären, warum ein Brecheisen geeignet ist, um Türen zu öffnen - und eine Türklinke immer an der den Scharnieren gegenüberliegenden Seite angebracht ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a188f17a-5705-4a30-bd42-ae39d6ed0432","notation":["a188f17a-5705-4a30-bd42-ae39d6ed0432"],"prefLabel":{"de":"Impuls"},"definition":{"de":"Der Impuls ist in der klassischen Physik eine wichtige Erhaltungsgröße. Betrachtet man ein in sich geschlossenes System wie (in guter Näherung) einen Billiardtisch, so bleibt der Gesamtimpuls in diesem System erhalten. Was auf den ersten Blick kompliziert klingt, hat ganz einfache Konsequenzen. Stößt eine Billiardkugel absolut mittig auf eine andere, so bleibt die erste stehen - und die zweite bewegt sich mit exakt der gleichen Geschwindigkeit wie zuvor die andere. In diesem Beispiel wurde der Impuls der ersten Billiardkugel auf die zweite übertragen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dcaade7e-4045-4494-99cc-0a28eb434683","notation":["dcaade7e-4045-4494-99cc-0a28eb434683"],"prefLabel":{"de":"Reibung"},"definition":{"de":"Reibung entsteht immer dann, wenn zwei Gegenstände sich berühren und mit unterschiedlicher Geschwindigkeit bewegen. Die so entstehende Reibungskraft kann nützlich sein um ein Auto in der Kurve zu halten. Oft ist sie aber auch störend: Luftreibung erhöht den Treibstoffverbrauch eines Fahrzeugs und macht die physikalische Beschreibung einer Vielzahl von Phänomenen deutlich komplizierter."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e8c79aa0-b438-476e-b863-a8a6ed5cae2c","notation":["e8c79aa0-b438-476e-b863-a8a6ed5cae2c"],"prefLabel":{"de":"Arbeit, Leistung, Wirkungsgrad"},"definition":{"de":"Mechanische Systeme können Arbeit verrichten, indem sie z.B. Gegenstände anheben, sie beschleunigen oder abbremsen. \"Arbeit\" ist somit neben der Wärme eine Form der Energieübertragung. Diese Energieübertragung erfolgt in realen Prozessen mit ganz unterschiedlichen Geschwindigkeiten. Dies wird durch die physikalische Größe Leistung beschrieben. Nicht nur die Leistung kann bei unterschiedlichen Prozessen ganz verschieden sein: Gerade für technische Anwendungen ist es zudem wichtig, den Wirkungsgrad im Auge zu behalten. Dieser beschreibt, wie effizient Verluste in Form von Reibung vermieden werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ffa05eb0-d428-4b2b-9b06-755bdbcbc0bd","notation":["ffa05eb0-d428-4b2b-9b06-755bdbcbc0bd"],"prefLabel":{"de":"Mechanische Energie"},"definition":{"de":"Energie ist eine komplexe Größe, die in verschiedenen Formen vorkommt. Sie kann in unterschiedlichen Prozessen von einer Form in die andere umgewandelt werden. Neben elektrischer Energie und Strahlungsenergie wird eine Vielzahl anderer Energieformen unter dem Oberbegriff der \"mechanischen Energie\" zusammengefasst. Die wichtigsten \"mechanischen\" Energieformen sind hierbei sicherlich die Höhenenergie und die Bewegungsenergie."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3d2e9d02-348d-4f51-bb5c-6ed0fdeddcb6","notation":["3d2e9d02-348d-4f51-bb5c-6ed0fdeddcb6"],"prefLabel":{"de":"Elektrizitätslehre / Magnetismus"},"definition":{"de":"Elektrizität und Magnetismus - zwei auf den ersten Blick sehr unterschiedliche Themen - hängen doch physikalisch betrachtet eng zusammen. Magneten haften an verschiedenen Materialien (und an anderen nicht) - und sind ein beliebtes Kinderspielzeug. Elektrische Geräte und Schaltungen umgeben uns täglich und erleichtern unseren Alltag. Die Physik hilft nicht nur, beide Themenbereiche besser zu verstehen, die elektromagnetische Wechselwirkung ist sogar eine der vier Grundkräfte der Physik."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c5d62d48-fbd3-4454-a9fe-bc37d141f548","notation":["c5d62d48-fbd3-4454-a9fe-bc37d141f548"],"prefLabel":{"de":"Grundlagen Magnetismus"},"definition":{"de":"Magnete sind nicht nur ein beliebtes Spielzeug für Kinder und praktische Helfer zur Befestigung von Zetteln am Kühlschrank. Solch einfache Permanentmagnete sind auch für den Betrieb von Elektromotoren und zur Stromerzeugung unverzichtbar. Das Phänomen des Elektromagnetismus zeigt darüber hinaus: Es besteht ein enger Zusammenhang elektrischer und magnetischer Phänomene. Elektromagnete lassen sich gezielt steuern und zur Trennung von Müll genauso wie zur Erzeugung von Elektrizität nutzen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0e841b54-4777-4b85-b9a4-198620def422","notation":["0e841b54-4777-4b85-b9a4-198620def422"],"prefLabel":{"de":"Permanentmagnetismus"},"definition":{"de":"Permanentmagnete sind immer magnetisch. Sie ziehen bestimmte Gegenstände an - und andere nicht. Solche Phänomene hast du sicherlich schon einmal beobachtet. Weniger offensichtlich ist, dass Permanentmagnete über ein unsichtbares Magnetfeld verfügen, das zum Beispiel mit feinen Eisenspänen sichtbar gemacht werden kann. Die Eigenschaften von Permanentmagneten lassen sich gut verstehen, wenn du dich genauer mit dem Modell der Elementarmagnete beschäftigst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/96a16444-a5c1-4bdb-a93e-ced1aa5498d8","notation":["96a16444-a5c1-4bdb-a93e-ced1aa5498d8"],"prefLabel":{"de":"Elektromagnetismus"},"definition":{"de":"Einen Draht, eine Spannungsquelle und einen Nagel aus Eisen. Mehr brauchst du nicht, um einen einfachen Elektromagneten zu bauen. Anders als Permanentmagnete kannst du diesen Magneten ganz einfach an- und wieder ausschalten. Elektromagnete eröffnen eine Vielzahl technischer Anwendungsmöglichkeiten des Magnetismus und sind aus unserem Alltag nicht mehr wegzudenken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/33ce727a-9dca-418d-ae11-624f71dcb05d","notation":["33ce727a-9dca-418d-ae11-624f71dcb05d"],"prefLabel":{"de":"Grundlagen E-Lehre"},"definition":{"de":"Die Elektrizitätslehre ist ein für den Alltag hochrelevantes Teilgebiet der Physik. Einige grundlegende Aspekte der Elektrizitätslehre gehören mittlerweile zum Allgemeinwissen: Welche Wirkungen hat elektrischer Strom? Wie sind Stromkreis grundlegend aufgebaut? Mit Hilfe welcher physikalischer Größen kann ich elektrische Ströme messen? Verbunden mit all diesen technisch bedeutsamen Fragestellungen ist natürlich auch die viel grundlegendere Frage: Was strömt eigentlich im elektrischen Stromkreis?"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2704741a-838d-40f0-9de2-b8d33116797e","notation":["2704741a-838d-40f0-9de2-b8d33116797e"],"prefLabel":{"de":"Elektrostatik"},"definition":{"de":"Die Elektrostatik untersucht die Eigenschaften nicht bewegter Ladungen. Mit Hilfe einfacher Versuche lässt sich zeigen, dass es zwei Arten elektrischer Ladung geben muss - und dass sich bestimmte Kombinationen dieser Ladungen gegenseitig anziehen oder abstoßen. Die Erkenntnisse der Elektrostatik können dazu genutzt werden zu erklären, was im elektrischen Stromkreis auf mikroskopischer Ebene eigentlich strömt: Es sind bewegte Ladungsträger, die Elektronen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3dbe8969-39f5-4f54-a50c-effbe49557dd","notation":["3dbe8969-39f5-4f54-a50c-effbe49557dd"],"prefLabel":{"de":"Stromwirkung"},"definition":{"de":"Elektrischer Strom ist nicht sichtbar, hat aber doch viele sichtbare Auswirkungen: Er kann elektrische Leiter erwärmen, Licht erzeugen, Motoren in Bewegung setzen und vieles mehr. Elektrischer Strom ist gefährlich. Kommst du mit zu starken elektrischen Strömen in Kontakt, so besteht akute Lebensgefahr."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/753c00d1-ab5b-4f41-b28b-337049a3172e","notation":["753c00d1-ab5b-4f41-b28b-337049a3172e"],"prefLabel":{"de":"Elektrische Grundgrößen"},"definition":{"de":"Zur Beschreibung und Untersuchung elektrischer Schaltungen sind verschiedene physikalische Größen notwendig. Die elektrische Stromstärke (oft auch: Elektronenstromstärke) beschriebt, wie viele Elektronen pro Sekunde durch den Querschnitt eines Drahtes strömen, während die elektrische Spannung die durch ein einzelnes Elektron übertragbare Energie beschreibt. Die Leistung (oft auch passender: Energiestromstärke) beschreibt schließlich die pro Sekunde übertragene Energiemenge. Die elektrische Stromstärke und die Spannung sind über den elektrischen Widerstand und das ohm'sche Gesetz miteinander verknüpft."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d0095c99-3d13-47e8-bcea-e57dfd4a8b56","notation":["d0095c99-3d13-47e8-bcea-e57dfd4a8b56"],"prefLabel":{"de":"Einfache Stromkreise"},"definition":{"de":"Der einfachste Stromkreis besteht aus einer Spannungsquelle wie zum Beispiel einer Batterie, zwei Kabeln und einem Verbraucher. Als Verbraucher kann beispielsweise eine Glühlampe genutzt werden. Stück für Stück lassen sich darauf aufbauend mehrere Verbraucher und Kabel in den Stromkreis integrieren. Mehrere Verbraucher lassen sich dann sowohl in Reihe als auch parallel schalten und dem entsprechend Reihenschaltungen und Parallelschaltungen voneinander unterscheiden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/11674b73-3b8a-4514-a4a0-a6e1a69d8298","notation":["11674b73-3b8a-4514-a4a0-a6e1a69d8298"],"prefLabel":{"de":"Vertiefung E-Lehre"},"definition":{"de":"Reale elektrische Schaltungen nutzen neben Glühlampen und Motoren oft auch weitere Bauteile und sind zu komplizierteren Schaltungstypen wie Und- beziehungsweise Oder-Schaltungen verbunden. Weitere Bauteile wie Transistoren, Spulen und Kondensatoren haben für die Anwendung in elektrischen Geräten wichtige Eigenschaften - und zeigen spannende Phänomene wie die Induktion. Diese bildet auch die Grundlage für die Erzeugung von elektrischem Strom in Kraftwerken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/709be54b-61b1-4b12-acf9-f0da564b5530","notation":["709be54b-61b1-4b12-acf9-f0da564b5530"],"prefLabel":{"de":"Induktion"},"definition":{"de":"Magnetfelder, die sich zeitlich verändern, sind in der Lage, innerhalb einer Spule eine Spannung zu induzieren. Dieses Phänomen bildet die Grundlage fast aller Kraftwerke: Von Kohlekraftwerken über Wind- und Kernkraftwerke bis hin zum Fahrraddynamo. Auch zum Kochen lässt sich das Phänomen der Induktion sinnvoll nutzen, genauso wie zum drahtlosen Aufladen von Smartphones. Kurzum: Aus deinem Leben ist die Induktion kaum wegzudenken - auch wenn du sie oft nicht bemerkst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c23e573e-343a-4ccc-a329-f59890ace180","notation":["c23e573e-343a-4ccc-a329-f59890ace180"],"prefLabel":{"de":"Spule"},"definition":{"de":"In einem elektromagnetischen Schwingkreis, mit dem zum Beispiel Radiowellen erzeugt werden können, stellen Spulen neben Kondensatoren das zweite wesentliche Bauteil dar. In ihnen finden Induktionsvorgänge statt - und werden zugleich magnetische Felder auf und abgebaut. Diese Eigenschaften von Spulen sorgen dafür, dass sie in Transformatoren zur Spannungswandlung und vielen weiteren Bereichen der Elektrotechnik zur Anwendung kommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c607fe99-6fff-4bce-a028-3fac5377a12b","notation":["c607fe99-6fff-4bce-a028-3fac5377a12b"],"prefLabel":{"de":"Transformator Generator Motor"},"definition":{"de":"Nicht erst seit dem Aufkommen von Elektroautos spielen Elektromotoren eine wichtige Rolle in der Technik: Sie bewegen alle Arten von technischen Geräten vom Rasenmäher bis hin zum Atom-U-Boot. Jeder Motor kann gleichzeitig zudem als Generator genutzt werden und elektrischen Strom erzeugen. Motoren nutzen hierzu eine geschickte Anordnung von Magneten und Spulen. Zum Transport des elektrischen Stroms über große Entfernungen ist es wichtig, die passende Spannung zu wählen. Für die Umwandlung verschiedener Spannungen ineinander werden in der Elektrotechnik Transformatoren genutzt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c964d437-93ce-48f0-9a57-0d5bfe369d8f","notation":["c964d437-93ce-48f0-9a57-0d5bfe369d8f"],"prefLabel":{"de":"Halbleiterphysik"},"definition":{"de":"Halbleiterbauelemente sind aus unserem Alltag nicht mehr wegzudenken: Die gesamte Computertechnik basiert auf Halbleitern. Ein Halbleiter kann als Festkörper mit besonderen Eigenschaften verstanden werden: Anders als ein Metall leitet er den elektrischen Strom je besser, desto wärmer er ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f6ec2990-6978-4627-acb4-c4c7a9574d69","notation":["f6ec2990-6978-4627-acb4-c4c7a9574d69"],"prefLabel":{"de":"Komplexe Schaltkreise"},"definition":{"de":"Komplexe Schaltkreise gehen über die Parallel- und Reihenschaltung von Verbrauchern hinaus. Sie verknüpfen verschiedene Bauteile geschickt zum Beispiel zu logischen Schaltung wie der Und- beziehungsweise Oder-Schaltung. Im täglichen Leben sind wir auch oft mit Wechselschaltungen konfrontiert, zum Beispiel beim Licht im Treppenhaus. Viele solcher komplexen Schaltungen sind aus unserem Alltag kaum wegzudenken und erfüllen wichtige Funktionen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/faeb111e-e1b3-4475-81aa-bb694ed9b96d","notation":["faeb111e-e1b3-4475-81aa-bb694ed9b96d"],"prefLabel":{"de":"Kondensator / Kapazität"},"definition":{"de":"Kondensatoren sind elektrische Bauteile, die Ladungen speichern können. Sie dienen in der Fahrradbeleuchtung als kurzfristiger Batterieersatz, in elektrischen Schaltungen zur Spannungsglättung oder der Erzeugung elektromagnetischer Schwingungen in Antennen und anderen Sendeanlagen. Kondensatoren lassen sich vor allem anhand ihrer Kapazität unterscheiden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ea309400-8364-4330-b5d5-dc63f1f27149","notation":["ea309400-8364-4330-b5d5-dc63f1f27149"],"prefLabel":{"de":"Bewegte Ladungen / Felder"},"definition":{"de":"Bewegte Ladungsträger wie beispielsweise Elektronen erzeugen elektrische und magnetische Felder. Gleichzeitig werden sie durch bereits vorhandene Felder beeinflusst: Elektrische und magnetische Felder üben eine Kraft auf bewegte Ladungsträger aus. Physikalisch wurde diese Entdeckung genutzt, um die spezifische Ladung von Elektronen und Protonen zu bestimmen. Technisch lassen sich durch geschickte Kombination von Feldern zum Beispiel Röhrenfernseher herstellen oder Elektronen mit ganz bestimmter Geschwindigkeit herausfiltern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7a02ae61-250d-4858-a589-97a5cbe07482","notation":["7a02ae61-250d-4858-a589-97a5cbe07482"],"prefLabel":{"de":"Magnetfeld"},"definition":{"de":"Bewegte Ladungsträger, zum Beispiel Elektronen in einem Kabel, erzeugen um sich herum ein Magnetfeld. Dieses Magnetfeld ist ein sogenanntes Wirbelfeld und besteht aus in sich geschlossenen magnetischen Feldlinien. Anders als das Feld eines Permanentmagneten hat es also keine Magnetpole. Andererseits haben äußere Magnetfelder einen Einfluss auf bewegte Ladungsträger. So wird ein stromdurchflossener Draht im Magnetfeld eines Hufeisenmagneten bei passender räumlicher Anordnung abgelenkt. Die auf den Draht wirkende Kraft bezeichnen Physiker:innen auch als Lorentzkraft."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a1b8163c-2d3d-4c4f-8ef8-37dd846539b2","notation":["a1b8163c-2d3d-4c4f-8ef8-37dd846539b2"],"prefLabel":{"de":"Glühelektrischer Effekt"},"definition":{"de":"Erhitzt sich ein elektrischer Leiter stark, so beginnt er zu glühen. Ab einer Temperatur von einigen hundert Grad Celsius beginnen Elektronen, aus dem Metalldraht auszutreten. Einer der Entdecker dieses Effekts war Edison, der für die Entwicklung der ersten marktreifen Glühlampe und viele weitere Erfindungen bekannt ist. Technisch wird der glühelektrische Effekt zur Erzeugung von Elektronenstrahlen genutzt, zum Beispiel in Oszilloskopen oder Röhrenmonitoren. Aber auch Leuchtstoffröhren nutzen diesen Effekt zur Erzeugung von Ladungen und der Anregung des in der Röhre befindlichen Gases."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a455f899-a3cb-4bc3-93ad-699ad9cd7403","notation":["a455f899-a3cb-4bc3-93ad-699ad9cd7403"],"prefLabel":{"de":"Elektrisches Feld"},"definition":{"de":"Elektrische Ladungsträger wie Elektronen und Protonen sind von einem elektrischen Feld umgeben. Gleichzeitig bewirken äußere elektrische Felder eine Kraft auf Ladungsträger. Dieser Effekt lässt sich zum Beispiel beobachten, wenn Ladungsträger zwischen Kondensatorplatten gebracht werden. Die Elektronen erfahren hier aufgrund des elektrischen Feldes der Kondensatorplatten eine Beschleunigung. Diesen Effekt machen Physiker:innen sich technisch zu nutze, um Elektronen gezielt auf bestimmte Bahnen zu lenken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c81409cc-0536-4203-9b2f-c51fb83797e9","notation":["c81409cc-0536-4203-9b2f-c51fb83797e9"],"prefLabel":{"de":"Kraft auf Leiter"},"definition":{"de":"Innerhalb von elektrischen Leitern bewegen sich Elektronen. Wie auch auf andere bewegte Ladungsträger wirkt auf diese bewegten Elektronen in einem Magnetfeld eine Kraft. Diese Kraft führt zu einer Bewegung des elektrischen Leiters."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/460"}]}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bd8be6d5-0fbe-4534-a4b3-773154ba6abc","notation":["bd8be6d5-0fbe-4534-a4b3-773154ba6abc"],"prefLabel":{"de":"Mathematik"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9b8c8454-2435-4d30-84c4-60eb03c972d1","notation":["9b8c8454-2435-4d30-84c4-60eb03c972d1"],"prefLabel":{"de":"Zahlenräume"},"definition":{"de":"Die Schüler*innen lernen die Zahlen des Zahlenraumes (von - bis), das Zählen und das Rechnen kennen. Dazu gehört die korrekte Schreibweise, eine unterschiedliche Darstellung und die Einordnung der Zahlen sowie deren Beziehungen zueinander."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/09a878ca-c1b1-4fa1-8857-516ab74b0482","notation":["09a878ca-c1b1-4fa1-8857-516ab74b0482"],"prefLabel":{"de":"Zahlen bis 100"},"definition":{"de":"3 / 3\n\nZahlen bis hundert zu zählen ist wie eine aufregende Schatzsuche! Du fängst bei der kleinen Zahl 1 an und zählst immer weiter, bis du zur großen Zahl 100 kommst. Auf dem Weg dorthin wirst du viele Zusammenhänge entdecken, die dich in eine spannende mathematische Welt entführen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1a186be8-1f83-44ba-81e9-3162b3b61d0e","notation":["1a186be8-1f83-44ba-81e9-3162b3b61d0e"],"prefLabel":{"de":"Zahlen bis 10"},"definition":{"de":"Zahlen bis zehn sind wie deine Freunde, die du leicht zählen kannst! Eins, zwei, drei, ... bis zehn! Sie helfen dir, wenn du Dinge zählst ."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5f1483a1-bc38-4aab-b9bd-3242d2d1ee49","notation":["5f1483a1-bc38-4aab-b9bd-3242d2d1ee49"],"prefLabel":{"de":"Zahlen bis 20"},"definition":{"de":"Zahlen bis zwanzig sind wie unsere Freunde, die uns beim Zählen und Rechnen helfen. Von eins bis zwanzig können wir viele Dinge zählen, wie Spielzeug, Tiere und sogar unsere Freunde in der Schule. Lasst uns zusammen lernen, wie aufregend die Welt der Zahlen bis zwanzig sein kann!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aaa343bb-74c9-414c-b8bf-071ed514b4b9","notation":["aaa343bb-74c9-414c-b8bf-071ed514b4b9"],"prefLabel":{"de":"Zahlen bis 100.000"},"definition":{"de":"Zahlen sind überall um uns herum! Sie helfen uns zu zählen, wie viele Süßigkeiten wir haben oder wie alt wir sind. Hier kannst du etwas über Zahlen bis zur großen Zahl 100.000 lernen. Das klingt vielleicht viel, aber es macht Spaß, sie zu entdecken und zu verstehen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d2205682-e5ed-41be-971c-ba8e1a2d037e","notation":["d2205682-e5ed-41be-971c-ba8e1a2d037e"],"prefLabel":{"de":"Zahlen bis 1000"},"definition":{"de":"Zahlen sind wie Freunde, die uns helfen, Dinge zu zählen und zu verstehen. Wir können bis zu tausend zählen, das sind ganz viele Zahlen, die uns in der Welt umgeben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e08a0890-97d5-4f83-93ed-fd10ad316a6b","notation":["e08a0890-97d5-4f83-93ed-fd10ad316a6b"],"prefLabel":{"de":"Zahlen bis 10.000"},"definition":{"de":"Hier werden wir darüber sprechen, wie wir Zahlen bis zu 10.000 verstehen und verwenden können, um noch größere Abenteuer zu erleben!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c43e833d-7343-4846-92f2-2ea093325235","notation":["c43e833d-7343-4846-92f2-2ea093325235"],"prefLabel":{"de":"Statistik und Wahrscheinlichkeitsrechnung"},"definition":{"de":"Die Stochastik als \"Mathemaik des Zufalls\" umfasst die Wahrscheinlichkeitsrechnung, Kombinatorik und die mathematische Statistik. Sie hilft im Umgang mit Situationen, die mit Unsicherheit belastet sind. \nDie zentralen Begriffe des Teilgebiets der Wahrscheinlichkeitsrechnung sind Laplace-Expermiente, bedingte Wahrscheinlichkeit, Binomial- und Bernoulliverteilung. In der Statistik werden Hypothesentest und Fehler 1. und 2. Art behandelt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a72f941-594a-4334-bf9c-2ab818227a4d","notation":["0a72f941-594a-4334-bf9c-2ab818227a4d"],"prefLabel":{"de":"Boxplots"},"definition":{"de":"Boxplots sind Diagramme, die der Darstellung von Verteilungen dienen. Sie zeigen den Bereich der mittleren 50% in der Box und jeweils die Abweichungen über die Antennen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a9bcf19-c29d-4609-9134-c14513a79a01","notation":["0a9bcf19-c29d-4609-9134-c14513a79a01"],"prefLabel":{"de":"Begriff \"Wahrscheinlichkeit\""},"definition":{"de":"Die Wahrscheinlichkeit ist ein Maß, das beschreibt, wie sehr erwartet wird, dass etwas eintrifft oder nicht eintrifft.\nDabei liegt die Wahrscheinlichkeit für das Eintreffen eines Ereignisses immer zwischen 0 und 1."}},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/19534f9b-ee10-44c0-9ae9-57a0e963e1b4","notation":["19534f9b-ee10-44c0-9ae9-57a0e963e1b4"],"prefLabel":{"de":"Statistische Grundbegriffe: Median, Mittelwert…"},"definition":{"de":"Arithmetisches Mittel, Modalwert, Median, Spannweite - das sind nur einige von vielen Kennwerten, die bei der Beschreibung von statistischen Erhebungen verwendet werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2363b894-a3c6-4bab-87a2-e185ab42499d","notation":["2363b894-a3c6-4bab-87a2-e185ab42499d"],"prefLabel":{"de":"Absolute und relative Häufigkeiten"},"definition":{"de":"Die absolute Häufigkeit ist die Anzahl, wie oft ein Ereignis bei einem Zufallsexperiment vorkommt. Die relative Häufigkeit hingegen beschreibt das Verhältnis der absoluten Häufigkeit zu der Anzahl der Versuche."}},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2d6ae136-c1e6-476e-9d84-732c2637bc00","notation":["2d6ae136-c1e6-476e-9d84-732c2637bc00"],"prefLabel":{"de":"Normalverteilung"},"definition":{"de":"Die Normalverteilung ist eine symmetrische Verteilungsform, die in der Statistik wohl am häufigsten verwendet wird. Sie wird auch als Gaußsche Glockenkurve bezeichnet, benannt nach dem Mathematiker Carl Friedrich Gauß."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4760905d-f2f9-4194-9d28-7aabdf0a02a8","notation":["4760905d-f2f9-4194-9d28-7aabdf0a02a8"],"prefLabel":{"de":"Laplace-Wahrscheinlichkeit und Summenregel"},"definition":{"de":"Liegt bei einem Experiment die Laplace-Wahrscheinlichkeit zugrunde, sind alle Ereignisse gleich wahrscheinlich. Typische Beispiele sind das Werfen eines Würfels oder einer Münze. Benannt ist die Wahrscheinlichkeit nach dem französischen Mathematiker Pierre-Simon Laplace. Addiert man alle Wahrscheinlichkeiten der einzelnen Ereignisse, erhält man 1."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6e31ba9c-0c5c-48dd-842e-e3d3829a7b74","notation":["6e31ba9c-0c5c-48dd-842e-e3d3829a7b74"],"prefLabel":{"de":"Hypothesentests"},"definition":{"de":"Das statistische Testverfahren überprüft zwei gegensätzliche Hypothesen. Mithilfe dieses Tests kann anhand der Ergebnisse einer Stichprobe entschieden werden, welche der beiden Hypothesen angenommen und welche verworfen wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/77d771ef-8dea-445b-aef1-25558dd93891","notation":["77d771ef-8dea-445b-aef1-25558dd93891"],"prefLabel":{"de":"Stochastische Unabhängigkeit"},"definition":{"de":"Sind zwei oder mehrere Ereignisse voneinander stochastisch abhängig, beeinflusst das Eintreten des einen Ereignisses das Eintreten des anderen Ereignisses. Somit ändern sich bei jedem Durchgang die Wahrscheinlichkeiten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7fabb0d2-52e0-4fcb-a747-9be403dc4ace","notation":["7fabb0d2-52e0-4fcb-a747-9be403dc4ace"],"prefLabel":{"de":"Erwartungswert und Standardabweichung"},"definition":{"de":"Um eine Verteilung genauer zu beschreiben, gibt es charakteristische Maßzahlen. Dazu zählen unter anderem der Erwartungswert und die Standardabweichung."}},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9881596e-c1ae-4f57-9f84-f62d0ca56073","notation":["9881596e-c1ae-4f57-9f84-f62d0ca56073"],"prefLabel":{"de":"Bedingte Wahrscheinlichkeit"},"definition":{"de":"Die bedingte Wahrscheinlichkeit gibt die Wahrscheinlichkeit für ein Ereignis A unter der Bedingung an, dass ein weiteres Ereignis B bereits eingetreten ist. Veranschaulichen kann man bedingte Wahrscheinlichkeiten an einem Baumdiagramm."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9a8e2a02-c9c0-4756-869d-a84435165ccf","notation":["9a8e2a02-c9c0-4756-869d-a84435165ccf"],"prefLabel":{"de":"Kombinatorik"},"definition":{"de":"In der Kombinatorik wird die Anzahl der möglichen Anordnungen von Objekten gezählt.\nDabei wird unterschieden, ob die Reihenfolge relevant ist und ob Wiederholungen erlaubt sind oder nicht."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9b591c39-7864-4d9c-851b-2dcb55d125ce","notation":["9b591c39-7864-4d9c-851b-2dcb55d125ce"],"prefLabel":{"de":"Vierfeldertafel"},"definition":{"de":"Mithilfe der Vierfeldertafel können die Zusammenhänge zwischen zwei Ereignissen übersichtlich dargestellt und neue Informationen, unter anderem Wahrscheinlichkeiten, abgelesen werden. Die Vierfeldertafel eignet sich auch für die Untersuchung der Unabhängigkeit zweier Ereignisse."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aa457204-5495-4238-ae59-ceb26d4ba0ea","notation":["aa457204-5495-4238-ae59-ceb26d4ba0ea"],"prefLabel":{"de":"Weitere Verteilungen"},"definition":{"de":"Neben der Normalverteilung gibt es zahlreiche weitere Wahrscheinlichkeitsverteilungen, die für statistische Auswertungen eine wichtige Rolle spielen können. Beispiele dafür sind die Poisson-Verteilung oder die Exponentialverteilung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b344e832-9be5-48aa-8c87-80dfd3a06f4c","notation":["b344e832-9be5-48aa-8c87-80dfd3a06f4c"],"prefLabel":{"de":"Stochastische Prozesse und Matrizen"},"definition":{"de":"Überganzsmatrizen bzw. Prozessmatrizen zeigen die Übergangswahrscheinlichkeiten von Ereignissen. Damit lassen sich zukünfitge Entwicklungen vorausrechnen. Sie werden häufig benutzt, um Populationsveränderungen darzustellen und zu berechnen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c2ec6555-92c7-47b4-b9c1-e46d3e1d58a1","notation":["c2ec6555-92c7-47b4-b9c1-e46d3e1d58a1"],"prefLabel":{"de":"Abiturvorbereitung Stochastik"},"definition":{"de":"Hier sammeln wir Aufgaben und Tipps für das mündliche und schriftliche Abitur im Bereich Statistik und Wahrscheinlichkeitsrechnung"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c39feea6-12b2-416f-a191-199733f3f2fe","notation":["c39feea6-12b2-416f-a191-199733f3f2fe"],"prefLabel":{"de":"Baumdiagramm und Pfadregel"},"definition":{"de":"Ein Baumdiagramm ist ein Hilfsmittel, um mehrstufige Zufallsexperimente graphisch übersichtlich darzustellen. Jeder Zweig/Pfad zeigt ein mögliches Ergebnis des Experimentes an. Um die Wahrscheinlichkeiten genau zu berechnen, werden Pfadregeln (Produktregel, Summenregel) verwendet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/da77c781-c61e-444e-8b3b-ffbfc8c5fe2f","notation":["da77c781-c61e-444e-8b3b-ffbfc8c5fe2f"],"prefLabel":{"de":"Bernoulli-Experimente und Binomialverteilung"},"definition":{"de":"Ein Bernoulli-Experiment hat nur zwei Versuchsausgänge, man spricht von “Erfolg” und “Misserfolg”. Mithilfe der Binomialverteilung kann die ausführliche Rechnung durch eine kurze Formel gelöst werden."}}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d6479851-c1a9-4435-845b-331580dac6db","notation":["d6479851-c1a9-4435-845b-331580dac6db"],"prefLabel":{"de":"Analysis"},"definition":{"de":"Als der Deutsche Gottfried Wilhelm Leibniz und der Engländer Isaac Newton unabhängig voneinander die sogenannte \"Infinitesimalrechnung\" entwickelten, führte das zu einem großen Streit zwischen deutschen und englischen Mathematikern,  weil jede Seite glaubte, der andere hätte vom Vertreter der eigenen Seite abgeschrieben. Inzwischen weiß man, dass die beiden  dieses Teilgebiet der Mathematik tatsächlich unabhängig voneinander entwickelt haben. Zentrale Begiffe sind Funktion, Grenzwert und vor allem das Differenzieren und Integrieren. Ohne die Analysis wären der heutige Stand der Natur-, Ingenieur- und Wirtschaftswissenschaften undenkbar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bae10924-5e0e-4650-abc6-1236fd8a6bbf","notation":["bae10924-5e0e-4650-abc6-1236fd8a6bbf"],"prefLabel":{"de":"Ableitungen"},"definition":{"de":"Änderungsraten, wie zum Beispiel Geschwindigkeiten oder Beschleunigungen können mit der ersten und der zweiten Ableitung berechnet werden. Ableitungsregeln sind dabei ein wichtiges Hilfsmittel."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/022645ad-c2fa-421e-b925-4b75dd31cb55","notation":["022645ad-c2fa-421e-b925-4b75dd31cb55"],"prefLabel":{"de":"Ableitung der Umkehrfunktion"},"definition":{"de":"Diese Sammlung von Unterrichtsmaterialien widmet sich dem wichtigen mathematischen Konzept der Ableitung der Umkehrfunktion. Hier finden Sie Ressourcen, die Schülern und Studierenden helfen, das Verständnis für die Ableitung von inversen Funktionen zu vertiefen. Diese Materialien bieten klare Erklärungen und Übungen, um dieses Schlüsselthema in der Differentialrechnung zu meistern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1cea8d14-6fe0-436e-b2c5-627f3b3c590f","notation":["1cea8d14-6fe0-436e-b2c5-627f3b3c590f"],"prefLabel":{"de":"Ableitungen in Sachzusammenhängen"},"definition":{"de":"\"Wozu braucht man das?\" - eine wichtige Frage, und hier zeigen wir, wozu man Ableitungen nutzt, um mit Geschwindigkeiten, Wachstumsraten, Trends umzugehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/281c9bd0-a6e2-46a2-b49a-acd95c95f395","notation":["281c9bd0-a6e2-46a2-b49a-acd95c95f395"],"prefLabel":{"de":"Potenzregel, Faktorregel, Summenregel"},"definition":{"de":"In der Differentialrechnung helfen uns die Potenzregel, Faktorregel und Summenregel, Änderungsraten zu berechnen. Die Potenzregel hilft, wenn wir Potenzen ableiten, die Faktorregel ist nützlich, um Konstanten und Faktoren herauszuziehen, und die Summenregel erklärt, wie wir die Ableitungen von Funktionen addieren können. Diese Regeln sind wie Werkzeuge, die Mathematiker verwenden, um die steilen Wege der Kurven zu erkunden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/60e467ad-e17a-41c2-a943-a5a62fdf3d5b","notation":["60e467ad-e17a-41c2-a943-a5a62fdf3d5b"],"prefLabel":{"de":"Kettenregel"},"definition":{"de":"Die Kettenregel in der Differentialrechnung ist ein mathematisches Konzept, das uns hilft, die Ableitung (Veränderungsrate) einer Funktion zu berechnen, wenn diese Funktion aus zwei oder mehr miteinander verschachtelten Funktionen besteht. Manchmal kann man eine komplizierte Funktion ableiten, indem man sie in zwei einfachrer aufteilt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/632ae818-c36e-460c-9ee3-e57b31e88336","notation":["632ae818-c36e-460c-9ee3-e57b31e88336"],"prefLabel":{"de":"Änderungsrate und Ableitung"},"definition":{"de":"Die Änderungsrate und Ableitung sind grundlegende Konzepte in der Mathematik. Die Änderungsrate beschreibt, wie stark sich eine Variable in Abhängigkeit von einer anderen verändert, während die Ableitung eine Methode ist, um diese Änderungsrate mathematisch zu ermitteln. Beide Begriffe spielen eine entscheidende Rolle in Bereichen wie Physik, Wirtschaft und Ingenieurwissenschaften."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/76da90f0-1935-4c64-b21a-5dac34fbfae8","notation":["76da90f0-1935-4c64-b21a-5dac34fbfae8"],"prefLabel":{"de":"Quotientenregel"},"definition":{"de":"Die Quotientenregel ist ein Konzept in der Differentialrechnung, das die Berechnung der Ableitung von Funktionen beschreibt, die als Quotient zweier anderer Funktionen dargestellt werden. Diese Regel ist entscheidend, um die Veränderungsraten komplexer Funktionen zu analysieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9e997b5e-a663-4f1c-ae1a-9f611339380e","notation":["9e997b5e-a663-4f1c-ae1a-9f611339380e"],"prefLabel":{"de":"Tangenten, Sekanten & Normalen"},"definition":{"de":"Tangenten, Sekanten und Normale sind wichtige Begriffe in der Geometrie. Tangenten berühren eine Kurve oder einen Kreis genau an einem Punkt, Sekanten schneiden sie an zwei Punkten, und Normale sind Linien, die rechtwinklig auf die Kurve treffen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a9b063b0-bb81-4576-8368-1bc8b6150673","notation":["a9b063b0-bb81-4576-8368-1bc8b6150673"],"prefLabel":{"de":"Ableitung der Sinus- und Kosinusfunktion"},"definition":{"de":"Diese Sammlung von Unterrichtsmaterialien konzentriert sich auf die Ableitung der Sinus- und Kosinusfunktionen. Hier finden Sie Ressourcen und Übungen, die Schülern helfen, die grundlegenden Konzepte und Techniken der Differentialrechnung im Zusammenhang mit diesen trigonometrischen Funktionen zu verstehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e9baf912-14cc-41ed-8ad9-41a5cd5be127","notation":["e9baf912-14cc-41ed-8ad9-41a5cd5be127"],"prefLabel":{"de":"Produktregel"},"definition":{"de":"Die Produktregel ist ein grundlegendes Konzept in der Differentialrechnung und ein wichtiger Bestandteil der Ableitungsregeln.  In der Mathematik wird die Produktregel verwendet, wenn zwei oder mehr Funktionen miteinander multipliziert werden und Sie die Änderungsrate oder die Steigung dieses Produkts an einem bestimmten Punkt ermitteln müssen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0f0b213f-52f3-4da9-8a59-0d3f1182e9fd","notation":["0f0b213f-52f3-4da9-8a59-0d3f1182e9fd"],"prefLabel":{"de":"Folgen und Reihen"},"definition":{"de":"Eine (unendliche) Folge ist eine Zuordnung, die jeder natürlichen Zahl ein reelles Folgeelement zuordnet. Die einzelnen Elemente werden als Folgenglieder bezeichnet. Eine Reihe ist dabei die Summe aller oder eines Teils der Folgenglieder."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1ae0c9f6-b6ee-42a6-9733-7487934b875c","notation":["1ae0c9f6-b6ee-42a6-9733-7487934b875c"],"prefLabel":{"de":"Extremwertaufgaben"},"definition":{"de":"Wie ist ein Wert zu wählen, um ein optimales Ergebnis - sei es ein Maximum oder ein Minimum - zu erzielen? Das ist die typische Fragestellung bei Extremwertaufgaben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e6b5ba75-0fb0-443d-9ce6-145a97f52b89","notation":["e6b5ba75-0fb0-443d-9ce6-145a97f52b89"],"prefLabel":{"de":"Ganzrationale Funktionen"},"definition":{"de":"Ganzrationale Funktionen - oft auch \"Polynome\" genannt - sind Funktionen, die sich besonders für die automatische Berechnung eignen. Zusammenhänge näherungsweise durch ganzrationale Funktionen berechnen zu können, erleichtert praktische Anwendungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/213d037b-6cf2-4704-9386-f05c6eaac3e4","notation":["213d037b-6cf2-4704-9386-f05c6eaac3e4"],"prefLabel":{"de":"Skizzieren ganzrationaler Funktionen"},"definition":{"de":"Ganz-rationale Funktionen können auf einfache Weise graphisch dargestellt werden und sind in vielen mathematischen und wissenschaftlichen Anwendungen von großer Bedeutung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2533ae28-b6bc-413d-a70a-e32de3c30aae","notation":["2533ae28-b6bc-413d-a70a-e32de3c30aae"],"prefLabel":{"de":"Extremstellen"},"definition":{"de":"Extremstellen  markieren die Punkte auf einer Funktion, an denen die Steigung (oder Ableitung) null ist oder nicht existiert. Diese Punkte sind von großer Bedeutung, um Maxima und Minima von Funktionen zu bestimmen und spielen eine Schlüsselrolle in der Optimierung und Modellierung verschiedener Phänomene."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/52b9e724-d1bd-4123-b69c-73b985b3d712","notation":["52b9e724-d1bd-4123-b69c-73b985b3d712"],"prefLabel":{"de":"Grenzwerte von Funktionen"},"definition":{"de":"Grenzwerte sind ein Schlüsselkonzept in der Analysis. Erforschen Sie die Grundlagen und Anwendungen, die unser Verständnis von Funktionen und deren Verhalten an Grenzen bereichern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/67d91b73-e286-4198-8628-786192f59ed9","notation":["67d91b73-e286-4198-8628-786192f59ed9"],"prefLabel":{"de":"Verhalten im Unendlichen"},"definition":{"de":"Wenn wir schauen, was mit einer Funktion passiert, wenn die x-Werte immer größer und größer werden, erfahren wir, wie sie sich im Unendlichen verhält."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8a0c5e16-3fdf-4f0f-93d1-c17024f3ab58","notation":["8a0c5e16-3fdf-4f0f-93d1-c17024f3ab58"],"prefLabel":{"de":"Veränderungen am Funktionsgraphen"},"definition":{"de":"Veränderungen von Funktionsgraphen, wie beispielsweise das Verschieben, Strecken, Stauchen oder Spiegeln sind insbesondere bei Anwendungen bedeutungsvoll. Insofern ist es wichtig, die Auswirkungen auf Parametern und  Gleichungen zu kennen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9f58e63b-1fe9-4dc9-b6cc-3dbcecdbac0a","notation":["9f58e63b-1fe9-4dc9-b6cc-3dbcecdbac0a"],"prefLabel":{"de":"Nullstellen von ganzrationalen Funktionen"},"definition":{"de":"Die Werte von x, zu untersuchen bei denen die Funktion den Funktionswert Null erreicht, bietet Einblicke in die Verhaltensweise von Polynomen und anderen Funktionen bieten. In dieser Sammlung Text werden die Grundlagen der Nullstellen von ganzrationalen Funktionen erläutert und verschiedene Methoden zur Ermittlung dieser wichtigen Punkte vorgestellt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d74f8e58-2a3d-436a-8eee-c2c78960a3e3","notation":["d74f8e58-2a3d-436a-8eee-c2c78960a3e3"],"prefLabel":{"de":"vollständige Kurvendiskussion"},"definition":{"de":"Die vollständige Kurvendiskussion ist ein fortgeschrittenes Konzept in der Mathematik, das dir hilft, die Eigenschaften einer Funktion oder Kurve gründlich zu analysieren. Mit ihr kannst du Dinge wie Extremstellen, Wendepunkte, Symmetrien und Verhaltensweisen in verschiedenen Bereichen der Funktion herausfinden, was dir ein tieferes Verständnis für mathematische Zusammenhänge vermittelt und dir bei komplexen mathematischen Problemen helfen kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ef4b1132-c5b5-40d1-8214-ce67d4a2a4d2","notation":["ef4b1132-c5b5-40d1-8214-ce67d4a2a4d2"],"prefLabel":{"de":"Symmetrie"},"definition":{"de":"Symmetrie ist wie ein magischer Spiegel in der Welt der Formen und Muster. Wenn etwas symmetrisch ist, bedeutet das, dass es gleich aussieht, wenn man es in der Mitte teilt. Denk an einen Schmetterling: Wenn du ihn in der Mitte faltest, sehen beide Flügel genau gleich aus! Symmetrie ist überall um uns herum, von Schneeflocken über Blumen bis hin zu Gebäuden. Lass uns gemeinsam entdecken, wie wunderbar und faszinierend Symmetrie sein kann!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fe587b12-a3a3-413e-bb3a-8bf711688e77","notation":["fe587b12-a3a3-413e-bb3a-8bf711688e77"],"prefLabel":{"de":"Wendestellen"},"definition":{"de":"Wendestellen in der Mathematik sind Punkte auf einer Funktion, an denen die Krümmung der Kurve wechselt. Diese Stellen spielen eine wichtige Rolle bei der Analyse von Funktionen und deren Verhalten. In dieser Sammlung werden die Konzepte von Wendepunkten und Wendetangen näher beleuchtet und ihre Bedeutung in der Differentialrechnung und der Kurvendiskussion erläutert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c9fa4417-f7b9-4874-b37b-e8c9f5583d1e","notation":["c9fa4417-f7b9-4874-b37b-e8c9f5583d1e"],"prefLabel":{"de":"Lineare Funktionen"},"definition":{"de":"Lineare Funktionen sind Polynomfunktionen ersten Grades und haben als Graphen eine Gerade. Anhand der Funktionsgleichung kann bei linearen Funktionen direkt die Steigung und der y-Achsenabschnitt abgelesen werden. Deshalb ist es relativ einfach, die Funktionsgleichung am Graphen abzulesen. Dazu wird das Steigungsdreieck zur Hilfe genommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/25ec3458-96ec-4a6b-baf4-c17de14b38c9","notation":["25ec3458-96ec-4a6b-baf4-c17de14b38c9"],"prefLabel":{"de":"Funktionsgleichung"},"definition":{"de":"Die Funktionsgleichung bei linearen Funktionen ist wie eine besondere Regel für Mathe-Spiele. Sie hilft uns, Linien auf Papier zu zeichnen und zu verstehen. Mit dieser Regel können wir sehen, wie eine Linie steigt und fällt und wie sie anfängt. Spannend, oder?"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/643e4d4c-2ceb-424b-9aa9-216fe0f49c61","notation":["643e4d4c-2ceb-424b-9aa9-216fe0f49c61"],"prefLabel":{"de":"Steigungsdreieck"},"definition":{"de":"Ein Steigungsdreieck ist wie eine Rutsche für Mathematik! Es zeigt uns, wie steil oder flach eine Linie ist. Je steiler das Dreieck, desto schneller geht es nach oben, und je flacher es ist, desto langsamer geht es. So können wir herausfinden, wie schnell Dinge auf Graphen bergauf oder bergab gehen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3976774a-b3fb-4bc3-91f9-0475b70a6de1","notation":["3976774a-b3fb-4bc3-91f9-0475b70a6de1"],"prefLabel":{"de":"Quadratische Funktionen"},"definition":{"de":"Quadratische Funktionen sind Polynomfunktionen von Grad 2. Die Graphen werden als Parabeln bezeichnet. An der Funktionsgleichung kann man ablesen, ob die Parabel nach oben oder unten geöffnet, gestaucht oder gestreckt ist. Sie kann auch nach oben oder unten und nach links oder rechts verschoben sein."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2e610a04-b222-433e-96d7-a42bc06efc5f","notation":["2e610a04-b222-433e-96d7-a42bc06efc5f"],"prefLabel":{"de":"Normalparabel"},"definition":{"de":"Die Normalparabel ist eine besondere Kurve, die wie ein freundliches Lächeln aussieht. Sie ist symmetrisch um ihre Mitte und hilft uns, viele spannende Dinge in der Mathematik zu entdecken, wie zum Beispiel Quadratische Gleichungen und Symmetrie."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8be67366-0153-4674-8009-eb9fd472fda6","notation":["8be67366-0153-4674-8009-eb9fd472fda6"],"prefLabel":{"de":"Scheitelpunktsform der Parabelgleichung"},"definition":{"de":"Die Scheitelpunktsform der Parabelgleichung ist eine elegante Möglichkeit, Parabeln darzustellen. In dieser Form wird der Scheitelpunkt der Parabel und ihre Öffnungsrichtung auf einen Blick deutlich, was die Analyse und das Verständnis dieser Kurven erheblich erleichtert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d2b52e34-3ebc-4433-a11b-de605774ac3f","notation":["d2b52e34-3ebc-4433-a11b-de605774ac3f"],"prefLabel":{"de":"Streckung und Verschiebung der Normalparabel"},"definition":{"de":"Manchmal möchten wir die Parabel, die wie ein U aussieht, verändern, um sie größer oder kleiner zu machen. Das nennt man Streckung und Stauchung. Oder wir können sie nach links oder rechts schieben, das nennt man Verschiebung. Das macht die Parabel zu einem echten Abenteurer!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e5b98357-a31b-4c5e-9870-c72a9a2a538d","notation":["e5b98357-a31b-4c5e-9870-c72a9a2a538d"],"prefLabel":{"de":"Nullstellenberechnung bei quadratischen Funktionen"},"definition":{"de":"Nullstellenberechnung bei quadratischen Funktionen hilft uns herauszufinden, wo die Funktion auf null geht, also wo sie den Boden berührt. Das ist wichtig, um Rätsel zu lösen und herauszufinden, wo Dinge oben oder unten sind. Wir können das mit Mathe-Zaubertricks machen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f4c52208-f227-49d7-b3aa-de4c8dab203c","notation":["f4c52208-f227-49d7-b3aa-de4c8dab203c"],"prefLabel":{"de":"Integralrechnung"},"definition":{"de":"Die Integralrechnung ist nicht nur die Umkehrung der Differenzialrechnung, sondern auch ein mächtiges Hilfmittel für die Berechung des Flächeninhalts einer \"krummen\" Funktion."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/311beb2e-2355-44c9-b3dc-a42831db5a49","notation":["311beb2e-2355-44c9-b3dc-a42831db5a49"],"prefLabel":{"de":"Partialbruchzerlegung"},"definition":{"de":"Mit Hilfe der Partialbruchzerlegung lassen sich Integrale berechnen, die anderweitig nicht zu berechnen sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/352fe1fb-70cd-46a5-9665-20e98ddf0288","notation":["352fe1fb-70cd-46a5-9665-20e98ddf0288"],"prefLabel":{"de":"Einführung des Integrals"},"definition":{"de":"Hier gibt es Materialien zur Einführung des Integralbegriffs, einerseits unter Verwendung von Unter- und Obersummen, andererseits mit dem Begriff der Stammfunktion. Auch zum \"orientierten Flächeninhalt\" findet man hier Material."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3a387bbd-dd2c-44cf-8f7e-46f10f9e4368","notation":["3a387bbd-dd2c-44cf-8f7e-46f10f9e4368"],"prefLabel":{"de":"Stammfunktionen"},"definition":{"de":"Stammfunktionen repräsentieren eine wichtige mathematische Operation, nämlich das Umkehren von Ableitungen. Sie spielen eine Schlüsselrolle in der Berechnung von Integralen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8c88cce4-1fd3-4118-9d6b-108a2a2b0614","notation":["8c88cce4-1fd3-4118-9d6b-108a2a2b0614"],"prefLabel":{"de":"Numerische Integration"},"definition":{"de":"Numerische Integration ist ein Verfahren in der mathematischen Analysis, das dazu dient, die Fläche unter einer gegebenen Funktion anzunähern. Diese Methode ist nützlich, wenn es schwierig oder unmöglich ist, die Fläche durch eine Formel zu berechnen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a11a4722-ff9a-4431-ba23-7cab72542fd0","notation":["a11a4722-ff9a-4431-ba23-7cab72542fd0"],"prefLabel":{"de":"Uneigentliche Integrale"},"definition":{"de":"Hier werden Integrale und zugehörige Grenzwerte betrachtet"}},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aa5eebd9-e978-4b14-8175-95500292f3ec","notation":["aa5eebd9-e978-4b14-8175-95500292f3ec"],"prefLabel":{"de":"Rauminhalte von Rotationskörpern"},"definition":{"de":"Rotationskörper sind geometrische Figuren, die durch das Drehen einer geschlossenen Fläche um eine Achse entstehen. Ihre Rauminhalte lassen sich leicht mit den Methoden der Integralrechnung bestimmen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c169d6b4-5e71-4fd8-8ab8-7bf29f3c1010","notation":["c169d6b4-5e71-4fd8-8ab8-7bf29f3c1010"],"prefLabel":{"de":"Flächen berechnen"},"definition":{"de":"Hier wird die Integralrechnung angewendet, um krummlienig begrenzte Flächen zu berechnen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cc62ae71-e18f-48d8-83ec-ec36ece96705","notation":["cc62ae71-e18f-48d8-83ec-ec36ece96705"],"prefLabel":{"de":"Hauptsatz der Differential- und Integralrechnung"},"definition":{"de":"Hauptsatz der Differential- und Integralrechnung, bei der Ableitung und dem Integral um zueinander inverse Operationen für Funktionen handelt, so wie Addition und Subtraktion für Zahlen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4e87747e-45a5-4f15-8810-6eebb8f7a108","notation":["4e87747e-45a5-4f15-8810-6eebb8f7a108"],"prefLabel":{"de":"Abiturvorbereitung Analysis"},"definition":{"de":"Bald gehts ins Abitur und gute Vorbereitung ist alles. Hier lernst du, wie man selbst knifflige Analysis-Aufgaben löst, sodass du keine Angst vor der Prüfung haben musst. Wir wünschen dir viel Erfolg!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/906a330f-0171-4656-a290-7fdc39cadad6","notation":["906a330f-0171-4656-a290-7fdc39cadad6"],"prefLabel":{"de":"Exponential- und Logarithmusfunktion"},"definition":{"de":"Für den \"gesunden Menschenverstand\" ist es schwierig, exponentielles Wachstum richtig einzuschätzen. In diesem Bereich sammeln wir Materialien, die bei der Berechnung exponentieller Wachstums- und Zerfallsprozesse helfen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/70971763-d271-4ea3-9a50-4ea406ab8736","notation":["70971763-d271-4ea3-9a50-4ea406ab8736"],"prefLabel":{"de":"Ableitung und Stammfunktionen von Exponentiafunktionen"},"definition":{"de":"Eine Exponentialfunktion ist wie eine besondere Zauberformel in der Mathematik. Sie hilft uns, Dinge zu verstehen, die schnell wachsen oder schrumpfen. Die Ableitung und die Stammfunktion sind wie magische Werkzeuge, mit denen wir herausfinden können, wie steil die Kurve einer Exponentialfunktion ist und wie sie entstanden ist."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/88f3f7d9-7e55-44ca-b79e-b85cf973d1d0","notation":["88f3f7d9-7e55-44ca-b79e-b85cf973d1d0"],"prefLabel":{"de":"Die eulersche Zahl e"},"definition":{"de":"Die Zahl \"e\" wurde erstmals vom schweizerischen Mathematiker Leonhard Euler im 18. Jahrhundert eingeführt und ist ein fundamentales Element in Bereichen wie Analysis, Wahrscheinlichkeitstheorie, und komplexer Analysis. Sie ist eine der fundamentalen Konstanten, die die Wachstums- und Zerfallsprozesse in der Natur beschreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8a2f0b2d-0c93-4145-b10b-8c9acefe922a","notation":["8a2f0b2d-0c93-4145-b10b-8c9acefe922a"],"prefLabel":{"de":"Wachstums- und Zerfallprozesse"},"definition":{"de":"Exponentielles Wachstum beschreibt Änderungsprozesse, bei denen sich ein Wert in gleichen Abständen immer um denselben Faktor ändert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d8884ca1-d396-4019-b85e-810f65ecb6a1","notation":["d8884ca1-d396-4019-b85e-810f65ecb6a1"],"prefLabel":{"de":"Der natürliche Logarithmus"},"definition":{"de":"In dieser Sammlung finden Sie Ressourcen, um den natürlichen Logarithmus als Umkehrung der e-Funktionzu verstehen, seine mathematischen Eigenschaften zu erkunden und seine vielfältigen Anwendungen in den Naturwissenschaften, der Statistik und der Ingenieurwissenschaft zu schätzen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8fef32ea-3378-4736-9f79-7a6c33538ba9","notation":["8fef32ea-3378-4736-9f79-7a6c33538ba9"],"prefLabel":{"de":"Gebrochenrationale Funktionen"},"definition":{"de":"Eine Funktion f ist eine gebrochenrationale Funktion, wenn deren Funktionsterm aus dem Quotienten zweier Polynome p(x) und q(x) besteht."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ae67fc1c-895c-4de6-979c-47a068ceba0b","notation":["ae67fc1c-895c-4de6-979c-47a068ceba0b"],"prefLabel":{"de":"Sinus- und Cosinusfunktion"},"definition":{"de":"Die Sinus- und Kosinusfunktionen sind grundlegende mathematische Konzepte in der Trigonometrie. Sie beschreiben das periodische Verhalten von harmonischen Schwingungen und finden in zahlreichen Bereichen wie Physik, Ingenieurwissenschaften und Signalverarbeitung Anwendung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bd85676b-8660-400b-9521-330b5b09cad1","notation":["bd85676b-8660-400b-9521-330b5b09cad1"],"prefLabel":{"de":"Funktionsbegriff"},"definition":{"de":"Der Begriff der eindeutigen Zuordnung von Variablen ist ein Grundbegriff der Analysis. Hier sammeln wir Materialien, um den Funktonsbgegriff schülergerecht zu vermitteln."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d4034967-a4b5-4e92-b9d6-dbcba47237e5","notation":["d4034967-a4b5-4e92-b9d6-dbcba47237e5"],"prefLabel":{"de":"Potenzfunktionen"},"definition":{"de":"Potenzfunktionen haben einen natürlichen oder ganzzahligen Exponenten. Die Graphen mit natürlichem Exponenten nennt man Parabeln n-ter Ordnung, die Graphen mit ganzzahligem Exponenten Hyperbel nter-Ordnung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e98aab95-1bc8-4592-ba83-16a29a9db1ff","notation":["e98aab95-1bc8-4592-ba83-16a29a9db1ff"],"prefLabel":{"de":"Vollständige Induktion"},"definition":{"de":"Die vollständige Induktion beschreibt eine Beweismethode mithilfe derer eine Aussage für alle natürlichen Zahlen bewiesen wird. Begonnen wird der Beweis mit einem Startwert, bevor die Aussage auch für alle Nachfolger bewiesen wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f5a27207-6d12-44c5-a240-f9197b84c1d0","notation":["f5a27207-6d12-44c5-a240-f9197b84c1d0"],"prefLabel":{"de":"Steckbriefaufgaben"},"definition":{"de":"Gleichung einer unbekannten Funktion gesucht...Die Eigenschaften des Graphen der Funktion (Position der Hoch-, Tief-, Wendepunkte, Nullstellen, ...) sind durch die Aufgabenstellung gegeben. Durch die Bestimmung von Funktionen mit vorgegebenen Eigenschaften lassen sich die Zusammenhänge der Differenzial- und Integralrechnung vertiefend üben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5e7a12b0-2314-4bb0-9fd5-5c067cfb2b3e","notation":["5e7a12b0-2314-4bb0-9fd5-5c067cfb2b3e"],"prefLabel":{"de":"Trigonometrie"},"definition":{"de":"Wie wäre ein modernes Verkehrswesen denkbar ohne eine genaue Bestimmung, wo man sich befindet und welche Route zum Ziel führt? Alles dies ist erst möglich nach einer exakten \"Vermessung der Welt\", die ohne Trigonometrie nicht funktioniert. Dabei wirkt dieses Teilgebiet der Mathematik auf den ersten Blick sehr eingeschränkt: Es befasst sich mit Dreiecken und den trigonometrischen Funktionen Sinus, Kosinus und Tangens. Aber gerade in dieser Konzentration liegen die großen Erfolge der Trigonometrie."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1ab711ea-181c-4daf-aa83-4e1382d52e4e","notation":["1ab711ea-181c-4daf-aa83-4e1382d52e4e"],"prefLabel":{"de":"Winkelfunktionen im rechtwinkligen Dreieck"},"definition":{"de":"Zu den trigonometrischen Funktionen, auch Winkelfunktionen genannt, zählen der Sinus, Kosinus und Tangens. Mit diesen kann sowohl die Größe von Winkeln als auch die Länge von Seiten in einem rechtwinkligen Dreieck berechnet werden. Die Winkelfunktionen werden eindeutig über die Zuordnung “Winkel zu Seitenverhältnis” definiert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3ad81f6e-7f96-41a7-851f-0064d623e4b5","notation":["3ad81f6e-7f96-41a7-851f-0064d623e4b5"],"prefLabel":{"de":"Sinussatz und Kosinussatz"},"definition":{"de":"Mit Pythagoras wird meist der Satz der Pythagoras verbunden. Allerdings wurde dieser Lehrsatz nicht von ihm selbst, sondern wahrscheinlich von seinen Schülerinnen und Schülern entdeckt. Unabhängig davon ist dieser Spezialfall des Kosinussatzes einer der fundamentalen Sätze der euklidischen Geometrie."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d7bdc30e-a6c9-4abd-91de-ecd95b05b51d","notation":["d7bdc30e-a6c9-4abd-91de-ecd95b05b51d"],"prefLabel":{"de":"Bogenmaß"},"definition":{"de":"Die Größe eines Winkels wird üblicherweise im Gradmaß angegeben, es gibt eine weitere Maßeinheit: das Bogenmaß. Es wird als Vielfaches der Kreiszahl pi angegeben und findet häufig Anwendung in der Physik, zum Beispiel zur Berechnung von Kreisbahnen. Ein großer Vorteil ist, dass dadurch die Länge des zugehörigen Kreisbogens abgelesen werden kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/eabd1c50-886f-4937-a242-1d10bb8c71a2","notation":["eabd1c50-886f-4937-a242-1d10bb8c71a2"],"prefLabel":{"de":"Winkelfunktionen am Einheitskreis"},"definition":{"de":"Die Winkelfunktionen im rechtwinkligen Dreieck sind für Winkel von 0 Grad bis 90 Grad definiert. Eine Erweiterung ist im Einheitskreis, einem Kreis mit dem Radius 1, möglich. Je nachdem, wie der Winkel verändert wird, ergeben sich andere Werte für den Sinus, Kosinus oder Tangens. Da die Winkelfunktionen periodisch sind, wiederholen sich diese Werte."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f15f727c-9a5a-4f20-b1be-483c15477648","notation":["f15f727c-9a5a-4f20-b1be-483c15477648"],"prefLabel":{"de":"Analytische Geometrie"},"definition":{"de":"\"Rechnen statt zeichnen\" ist das Motto der Analytischen Geometrie und damit ist sie zur Grundlage vieler moderner Technologien geworden: Von der Gafik heutiger Computerspiele, über rechnergestützte Konstruktionsverfahren bis zum GPS-gestützten Navigationssystem. Zentrale Grundbegriffe der analytischen Geometrie sind Vektoren, Geraden und Ebenen. Ebenso werden die Lagebeziehungen, Abstände und Winkel zwischen Objekten betrachtet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2b4c089d-47d8-4f94-a20a-ba903ddceead","notation":["2b4c089d-47d8-4f94-a20a-ba903ddceead"],"prefLabel":{"de":"Vektorrechnung"},"definition":{"de":"Vektoren sind Objekte aus dem Vektorraum, die mit anderen Vektoren addiert (Vektoraddition) und mit Zahlen mulitpliziert (Skalarmultiplikation) werden können. Zusätzlich sind zwei weitere Operatoren definiert: das Vektorprodukt und das Skalarprodukt. Mit diesen vier Operatoren wird unter anderem die Orthogonalität und die Winkel zwischen Vektoren bestimmt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/027cfa46-ab03-4850-98b7-31bdc918f097","notation":["027cfa46-ab03-4850-98b7-31bdc918f097"],"prefLabel":{"de":"Lineare Abhängigkeit"},"definition":{"de":"Die lineare Abhängigkeit ist ein grundlegendes Konzept in der linearen Algebra und beschreibt das Verhältnis zwischen Vektoren in einem Vektorraum. In diesem Zusammenhang wird untersucht, ob ein Vektor als Linearkombination anderer Vektoren dargestellt werden kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/03bcc5d2-fdd1-4839-a98b-389fe00302ce","notation":["03bcc5d2-fdd1-4839-a98b-389fe00302ce"],"prefLabel":{"de":"Vektorprodukt"},"definition":{"de":"Das Vektorprodukt  ermöglicht die Berechnung eines neuen Vektors, der senkrecht auf zwei gegebenen Vektoren steht. In dieser Sammlung werden  die Grundlagen des Vektorprodukts erläutert und seine Anwendungen in verschiedenen mathematischen und physikalischen Bereichen beleuchtet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/29c7e960-2c03-4e26-b914-2004d95cc7ed","notation":["29c7e960-2c03-4e26-b914-2004d95cc7ed"],"prefLabel":{"de":"Addition und Subtraktion von Vektoren"},"definition":{"de":"Stell dir vor, Vektoren sind wie Pfeile auf einer Schatzkarte. Wenn wir sie addieren, kombinieren wir ihre Richtungen, um den Schatz zu finden. Beim Subtrahieren ziehen wir einen Pfeil vom anderen ab, als ob wir verlorene Schritte auf unserer Abenteuerreise zurückgehen. So können wir spannende mathematische Abenteuer erleben!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2a0008de-9bdd-409c-a2a1-4fe06f5981b8","notation":["2a0008de-9bdd-409c-a2a1-4fe06f5981b8"],"prefLabel":{"de":"Multiplikation mit einer Zahl"},"definition":{"de":"Wenn du einen Vektor mit einer Zahl multiplizierst, verändert sich die Länge des Pfeils. Das ist, als ob du den Pfeil \"streckst\" oder \"zusammendrückst\"."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/434b3438-9408-42bf-9a19-b404bbe8f855","notation":["434b3438-9408-42bf-9a19-b404bbe8f855"],"prefLabel":{"de":"Betrag eines Vektors"},"definition":{"de":"Der Betrag eines Vektors ist wie ein Maß für die Länge eines Pfeils. Er hilft uns zu verstehen, wie weit dieser Pfeil von seinem Anfangspunkt zum Ende geht. Je länger der Pfeil, desto größer ist sein Betrag."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7adc858d-3941-44aa-846e-e0eb9b28b3aa","notation":["7adc858d-3941-44aa-846e-e0eb9b28b3aa"],"prefLabel":{"de":"Vektoren"},"definition":{"de":"Vektoren werden zur Darstellung von Größen mit Richtung und Betrag verwendet, was in vielen praktischen Situationen von großer Bedeutung ist. Vektoren sind ein unverzichtbares Werkzeug, um komplexe Phänomene zu analysieren und zu modellieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a44491bb-f705-48ac-97b9-a8eeef7ad72d","notation":["a44491bb-f705-48ac-97b9-a8eeef7ad72d"],"prefLabel":{"de":"Skalarprodukt"},"definition":{"de":"Das Skalarprodukt von Vektoren ist wie ein besonderer Mathe-Verwandtschaftstest für Pfeile. Es sagt uns, wie sehr zwei Pfeile in die gleiche Richtung zeigen, und je mehr sie das tun, desto größer ist das Ergebnis. Stehen die Vektoren senkrecht aufeinander, ist das Skalarprodukt Null."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2c66dafb-26c6-4d1a-be3b-c14ffde8eb54","notation":["2c66dafb-26c6-4d1a-be3b-c14ffde8eb54"],"prefLabel":{"de":"kartesisches Koordinatensystem"},"definition":{"de":"Das kartesische Koordinatensystem ist ein orthogonales Koordinatensystem. Sowohl im zwei- als auch im dreidimensionalen Raum lassen sich damit geometrische Sachverhalte veranschaulichen und darstellen. Es wird verwendet, um Punkte, Funktionen aber auch Vektoren, Geraden und Ebenen zu zeichnen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5b25014d-2537-4743-8516-4176debd9911","notation":["5b25014d-2537-4743-8516-4176debd9911"],"prefLabel":{"de":"Geraden"},"definition":{"de":"Geraden sind Elemente der Geometrie. Sie sind gerade, unendlich lang und besitzen unendliche viele Punkte. In der Vektorgeometrie werden sie durch einen Ortsvektor und einen Richtigungsvektor definiert. Mit Hilfe einer Veriablen kann jeder Punkt auf der Geraden dargestellt und ermittelt werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5034283f-72e0-4fc4-8106-5b656d4fb52d","notation":["5034283f-72e0-4fc4-8106-5b656d4fb52d"],"prefLabel":{"de":"Koordinatenform Geraden"},"definition":{"de":"Geradengleichungen in Koordinatenform sind wie Schatzkarten, die uns den Weg zu den Punkten auf einer geraden Linie zeigen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6add1d0d-6242-4758-8d28-26178b011c71","notation":["6add1d0d-6242-4758-8d28-26178b011c71"],"prefLabel":{"de":"Parameterform Geraden"},"definition":{"de":"In der analytischen Geometrie wird die Parameterform einer Geradengleichung verwendet, um eine Linie im Koordinatensystem zu beschreiben. Sie verwendet Parameter, um die Position und Richtung der Linie  festzulegen. Bei vielen Anwendungen ist das hilfreich."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/51e7c36b-c24b-43dd-8a9d-0d8493b96306","notation":["51e7c36b-c24b-43dd-8a9d-0d8493b96306"],"prefLabel":{"de":"Abstände berechnen"},"definition":{"de":"Bei der Abstandsberechnung werden die Abstände verschiedener geometrischen Figuren, zum Beispiel Punkte, Vektoren, Geraden und Ebenen bestimmt. Dazu wird meist das Lotfußpunktverfahren verwendet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5eccdf08-c10f-488c-9198-d0e068344af4","notation":["5eccdf08-c10f-488c-9198-d0e068344af4"],"prefLabel":{"de":"Abiturvorbereitung Analytische Geometrie"},"definition":{"de":"Hier findet man alles, um fit fürs Abitur zu werden. So werden alte Abituraufgaben erläutert und vorgerechnet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/23d2bb8e-9a82-4cf0-be64-6baa09ffbca0","notation":["23d2bb8e-9a82-4cf0-be64-6baa09ffbca0"],"prefLabel":{"de":"Lagebeziehungen"},"definition":{"de":"Lagebeziehungen kennzeichnen die Beziehungen zwischen geometrischen Objekten wie Punkten, Geraden und Ebenen. Um die Lage solcher Objekte zu bestimmen, werden lineare Gleichungssysteme zur Hilfe genommen. Diese Beziehungen werden auch meist im Anwendungskontext für Kollisions- und Schattenaufgaben verwendet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8c3b04ac-c4d8-4ef1-83d8-52e1aef18899","notation":["8c3b04ac-c4d8-4ef1-83d8-52e1aef18899"],"prefLabel":{"de":"Gerade/Ebene"},"definition":{"de":"Entdecken Sie die faszinierenden Lagebeziehungen zwischen Geraden und Ebenen. In dieser Materialsammlung finden Sie wichtige Konzepte und Beispiele, die Ihnen helfen werden, die Wechselwirkungen und Geometrie hinter diesen fundamentalen Beziehungen zu verstehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ac67100e-623d-4f70-b30f-f650487951d8","notation":["ac67100e-623d-4f70-b30f-f650487951d8"],"prefLabel":{"de":"Gerade/Gerade"},"definition":{"de":"In der Mathematik beschäftigen wir uns mit Geraden im Raum, um herauszufinden, wie sie zueinander liegen. Dabei untersuchen wir, ob sie sich schneiden, parallel verlaufen oder vielleicht sogar \"wundschief\" sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/de8211dc-2499-4530-8b61-2d6b6c1cd453","notation":["de8211dc-2499-4530-8b61-2d6b6c1cd453"],"prefLabel":{"de":"Ebene/Ebene"},"definition":{"de":"In der analytischen Geometrie erforschen wir, wie Ebenen sich zueinander verhalten. Wir schauen, ob sie sich schneiden, parallel verlaufen oder sich berühren, wie Freunde auf einem Abenteuerspielplatz."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cff5d0be-bbc5-4c35-b228-35af17e9a045","notation":["cff5d0be-bbc5-4c35-b228-35af17e9a045"],"prefLabel":{"de":"Ebenen"},"definition":{"de":"Ebenen sind Elemente der Geometrie. Sie sind unendlich groß und besitzen unendlich viele Punkte. In der Vektorgeometrie werden sie durch einen Ortsvektor und zwei Spannvektoren definiert. Mit Hilfe von zwei Veriablen kann jeder Punkt auf der Ebene dargestellt und ermittelt werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/99734797-9ceb-45ff-a53c-2b3794bb1dd8","notation":["99734797-9ceb-45ff-a53c-2b3794bb1dd8"],"prefLabel":{"de":"Ebenen in Parameterform"},"definition":{"de":"Eine Ebene in Parameterform ist wie ein magisches Blatt Papier, auf dem wir Punkte zeichnen können, um eine Fläche zu erstellen. Die Punkte werden mit speziellen Zahlenpaaren beschrieben, die uns verraten, wo sie auf dieser zauberhaften Fläche liegen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/99c88bff-426b-4705-9f64-1f562cca74e5","notation":["99c88bff-426b-4705-9f64-1f562cca74e5"],"prefLabel":{"de":"Umwandlungen"},"definition":{"de":"Ebenengleichungen in verschiedene Darstellungen zu überführen erleichtert die Untersuchung und Lösung geometrischer Probleme sowie die Anwendung  in verschiedenen mathematischen und physikalischen Kontexten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ac9e6019-ff7d-409a-8fbd-eb19899fd633","notation":["ac9e6019-ff7d-409a-8fbd-eb19899fd633"],"prefLabel":{"de":"Ebenen in Koordinatenform"},"definition":{"de":"Ebenen in Koordinatenform sind wie unsichtbare, flache Blätter im Raum, und wir können sie präzise lokalisieren, indem wir ihnen eine \"Adresse\" in Form von drei Zahlen geben, die die Koordinaten der Ebene festlegen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d5fade49-52b4-4591-b8a2-c7676ce4f767","notation":["d5fade49-52b4-4591-b8a2-c7676ce4f767"],"prefLabel":{"de":"Normalenform"},"definition":{"de":"Die Normalenform einer Ebenengleichung  hilft uns, die Richtung und den Abstand einer Ebene von einem Punkt aus zu verstehen, indem sie die senkrechte Linie (Normalenvektor) und den Abstand zur Ebene beschreibt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b52087e6-a5c6-4b21-942b-bb63519444b1","notation":["b52087e6-a5c6-4b21-942b-bb63519444b1"],"prefLabel":{"de":"Winkel zwischen Vektoren, Geraden, Ebenen"},"definition":{"de":"In der linearen Algebra und analytischen Geometrie wird häufig nach der Bestimmung des Winkels zwischen Vektroren, Geraden und Ebenen gesucht. Mit dem Skalarprodukt kann die Orthogonalität der geometrischen Figuren bestimmt werden, die für weitere Betrachtungen wichtig ist zum Beispiel bei der Erstellung von Normalenvektoren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f14774b2-3dbb-464d-b9e4-70f4ddc919c6","notation":["f14774b2-3dbb-464d-b9e4-70f4ddc919c6"],"prefLabel":{"de":"Lineares Gleichungssystem"},"definition":{"de":"Ein lineares Gleichungssystem, kurz LGS, besteht in der linearen Algebra aus einer Menge linearer Gleichungen mit mehreren Unbekannten. Diese LGS können in eine Matrixschreibweise überführt werden und durch eine Umwandlungen in die Dreiecksform kann die Lösungsmenge bestimmt werden, die Aufschluss über die Lagebeziehung von Geraden und Ebenen gibt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f1b8cd56-6700-405b-91b1-fcca16f3b024","notation":["f1b8cd56-6700-405b-91b1-fcca16f3b024"],"prefLabel":{"de":"Anwendungsaufgaben"},"definition":{"de":"Anwendungsaufgaben bringen die erlernten Sachverhalte zu Vektoren, Geraden und Ebenen in einen alltäglichen Kontext. Hier besteht die Kunst darin, aus dem Sachzusammenhang eine mathematische Rechnung oder Vorgehensweise abzuleiten. Und anschließend das Ergebnis wieder mit der Anwendungsaufgabe in Verbindung zu setzten und eine Schlussfolgerung zu ziehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2d34688d-00b6-47f7-8522-31909c2671b4","notation":["2d34688d-00b6-47f7-8522-31909c2671b4"],"prefLabel":{"de":"Algebra"},"definition":{"de":"Früher wurde Algebra als \"Rechenkunst\" aufgefasst. Darunter werden diejenigen Gebiete zum Rechnen mit Zahlen erfasst, die üblicherweise in der Sekundarstufe I gelernt werden. Im Sinne der Kernlehrpläne geht es hier vor allem - aber nicht nur - um mathematisches Operieren mit den Grundrechenarten, Brüchen udn Dezimalzahlen. Im Zuge dessen werden Terme aufgestellt und Gleichungen gelöst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0a62a10e-374a-46e2-a7d6-0c6954e755c0","notation":["0a62a10e-374a-46e2-a7d6-0c6954e755c0"],"prefLabel":{"de":"Prozente & Zinsen"},"definition":{"de":"Prozente stellen Anteile von einem Ganzen mit dem Prozentzeichen % dar. Die Zinsrechnung ist eine Anwendung der Prozentrechnung und beschäftigt sich mit Geldanlagen. Allgemeinen Einsatz findet die Prozent- und Zinsrechnung im Alltag bei Sonderangeboten und dem Vergleich von Geldanlageerträgen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/06b5437d-4843-48b8-b336-e030017b13b3","notation":["06b5437d-4843-48b8-b336-e030017b13b3"],"prefLabel":{"de":"Prozentsatz"},"definition":{"de":"Der Prozentsatz ist eine mathematische Angabe, die das Verhältnis eines Teils zu einem Ganzen in Hundertsteln ausdrückt. Dieses Konzept wird häufig verwendet, um Anteile, Gewinne, Rabatte, Zinssätze und viele andere Arten von Verhältnissen oder Vergleichen darzustellen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2195791b-bc99-4f5c-88bd-ad72fe5f1f01","notation":["2195791b-bc99-4f5c-88bd-ad72fe5f1f01"],"prefLabel":{"de":"Berechnung des Zeitraums"},"definition":{"de":"Die Berechnung des Zeitraums bezieht sich darauf, wie lange Geld auf einem Konto angelegt oder geliehen wird und beeinflusst die Zinserträge oder Zinsausgaben. In einfachen Worten: Wenn du Geld sparst oder leihst, ist die Dauer, für die du es behältst oder ausleihst, wichtig."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3d5db29e-53e7-4455-93e7-2b7db4210b84","notation":["3d5db29e-53e7-4455-93e7-2b7db4210b84"],"prefLabel":{"de":"Zinsen"},"definition":{"de":"Zinsen sind wie ein kleines Extra-Geld, das du bekommst, wenn du dein Geld in der Bank aufbewahrst. Wenn du dein Geld dort länger lässt, wird es mehr! Das ist, als ob du für dein Sparschwein eine Belohnung bekommst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/84dc3d52-3203-4443-bda9-71a7132a27c9","notation":["84dc3d52-3203-4443-bda9-71a7132a27c9"],"prefLabel":{"de":"Zinseszinsen"},"definition":{"de":"Zinseszinsen sind ein mächtiges Finanzkonzept, bei dem Ihr Geld für Sie arbeitet, indem Sie nicht nur Zinsen auf Ihr ursprüngliches Kapital verdienen, sondern auch auf die bereits verdienten Zinsen. Diese einfache Idee kann enorme Auswirkungen haben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9c2cf28c-9294-4b9f-88e8-2931384b2410","notation":["9c2cf28c-9294-4b9f-88e8-2931384b2410"],"prefLabel":{"de":"Sachaufgaben Prozente & Zinsen"},"definition":{"de":"Stell dir vor, du legst dein Taschengeld in eine magische Kiste und jedes Jahr kommt ein kleiner Bonus dazu – das ist wie Zinsen! Wir können auch Prozente benutzen, um Rabatte im Spielzeugladen zu bekommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a53348c1-0233-4a40-bf2a-4fe4baab449d","notation":["a53348c1-0233-4a40-bf2a-4fe4baab449d"],"prefLabel":{"de":"Grundlagen der Prozentrechnung"},"definition":{"de":"Prozentrechnung hilft uns zu verstehen, wie Teile von etwas Großem in Prozenten ausgedrückt werden. Zum Beispiel, wenn du wissen möchtest, wie viel Rabatt du im Spielzeugladen bekommst, ist die Prozentrechnung dein bester Freund. Wir werden gemeinsam lernen, wie das funktioniert – es ist gar nicht so schwer!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b6863788-84bc-4240-8815-f8f6eb164684","notation":["b6863788-84bc-4240-8815-f8f6eb164684"],"prefLabel":{"de":"Formel umstellen"},"definition":{"de":"In dieser Sammlung bieten wir Materialien die zeigen, wie man aus der Formel für den Prozentwert auch die Formel für den Prozentsatz und den Grundwert erhält."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b8523db2-c88b-48ef-a6bd-d838ec1be360","notation":["b8523db2-c88b-48ef-a6bd-d838ec1be360"],"prefLabel":{"de":"Zinssatz"},"definition":{"de":"Der Zinssatz, auch als Nominalzins oder Zinsrate bezeichnet, ist ein zentrales Konzept in der Finanzwelt und der Wirtschaft. Er stellt den Prozentsatz dar, zu dem Geld verliehen oder angelegt wird, und bestimmt, wie viel zusätzliches Geld (Zinsen) ein Kreditnehmer  zahlen muss oder wie viel ein Anleger für die Bereitstellung von Kapital verdienen kann."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b9c6e9d5-64e1-412c-8906-8b1ceafd165a","notation":["b9c6e9d5-64e1-412c-8906-8b1ceafd165a"],"prefLabel":{"de":"Grundwert"},"definition":{"de":"Der Grundwert ist wie der Startpunkt auf einer Schatzkarte. Er zeigt uns, wie viel etwas am Anfang wert ist, bevor es sich verändert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bdd22416-da27-4c50-a587-d01b5c544f12","notation":["bdd22416-da27-4c50-a587-d01b5c544f12"],"prefLabel":{"de":"Kapital"},"definition":{"de":"Kapital bezieht sich auf den ursprünglichen Geldbetrag, auf das Zinsen berechnet werden. Es ist ein zentraler Begriff in der Finanzwelt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dd6c6cee-c902-445b-ab87-47818cb0c940","notation":["dd6c6cee-c902-445b-ab87-47818cb0c940"],"prefLabel":{"de":"Prozentwert"},"definition":{"de":"Der Prozentwert ist wie eine besondere Zahl, die uns sagt, wie viel von etwas wir haben. Stell dir vor, du hast eine Torte, und der Prozentwert sagt dir, wie viel von der Torte du schon gegessen hast. Das ist sehr nützlich, um herauszufinden, wie viel von etwas übrig ist!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e68627d7-5cb8-45b4-a2ff-e65d258ddbdd","notation":["e68627d7-5cb8-45b4-a2ff-e65d258ddbdd"],"prefLabel":{"de":"Wurzeln"},"definition":{"de":"Das Wurzelziehen ist die Umkehrung des Potenzierens. Wurzeln können sowohl aus Zahlen als auch aus Termen gezogen werden. Im reellen Bereich existiert die Wurzel einer Zahl genau dann, wenn diese Zahl positiv ist. Für negative Zahlen existieren Wurzeln nur im imaginären Zahlenbereich. Für die Rechnung mit Wurzeln wurden Wurzelgesetze aufgestellt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/089688c5-1ff2-4890-a87b-3f2e25d0792a","notation":["089688c5-1ff2-4890-a87b-3f2e25d0792a"],"prefLabel":{"de":"Wurzel näherungsweise berechnen"},"definition":{"de":"Stell dir vor, du hast einen großen Haufen bunte Legosteine, und du möchtest wissen, wie viele Steine ungefähr darin sind, aber du willst nicht jeden Stein zählen. Das ist so ähnlich wie Wurzeln näherungsweise berechnen – wir versuchen, eine gute Schätzung zu finden, ohne alles genau zu zählen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fbb5f055-87e9-44e2-bce3-d3c39f8397c1","notation":["fbb5f055-87e9-44e2-bce3-d3c39f8397c1"],"prefLabel":{"de":"Wurzelgesetze"},"definition":{"de":"Wurzelgesetze sind wie Zaubertricks in der Mathematik! Sie helfen uns, wenn wir Zahlenwurzeln ziehen. Es gibt Regeln, die uns zeigen, wie man Wurzeln addieren, subtrahieren und miteinander teilen kann, um tolle Mathe-Magie zu erleben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ba0dda9d-8c4c-42ab-b28b-4bc673086b98","notation":["ba0dda9d-8c4c-42ab-b28b-4bc673086b98"],"prefLabel":{"de":"Grundrechenarten"},"definition":{"de":"Addieren, Subtrahieren, Mulitplizieren und Dividieren sind die Grundrechenarten. Sie werden täglich eingesetzt und benötigt, zum Beispiel indem der Einkauf oder eine benötigte Farbmenge berechnet wird. Besonders die schriftlichen Verfahren zur Berechnung werden im Alltag eingesetzt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/0ce8c644-3fed-4d64-a63b-b0ae2c1b6311","notation":["0ce8c644-3fed-4d64-a63b-b0ae2c1b6311"],"prefLabel":{"de":"Runden und Überschlag"},"definition":{"de":"Beim Runden geht es darum, Zahlen einfacher zu machen. Wenn wir eine Überschlagsrechnung machen, schätzen wir, wie viel etwas kostet oder wie viele Dinge wir haben, ohne genau zu zählen. Das hilft uns, schneller und besser mit Zahlen zu spielen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1709dc07-b126-488f-a040-51be3c513cfc","notation":["1709dc07-b126-488f-a040-51be3c513cfc"],"prefLabel":{"de":"schriftliche Addition"},"definition":{"de":"Schriftliche Addition: Du schreibst die Zahlen untereinander und addierst sie, baber der Reihe nach von rechts nach links. Wenn die Summe größer als 9 ist, merkst du dir den Rest und schreibst die Zehnerzahl über die nächste Spalte. So kannst du große Zahlen ganz einfach zusammenzählen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/374cb1a6-ca80-4ead-9c77-7e8f9a29bcc3","notation":["374cb1a6-ca80-4ead-9c77-7e8f9a29bcc3"],"prefLabel":{"de":"schriftliche Multiplikation"},"definition":{"de":"Nicht alles kann man im Kopf rechnen. und so ist die Schriftliche Multiplikation eine Methode, um große Zahlen zu multiplizieren.  Damit kannst du auch schwierige Multiplikationenaufgaben leicht lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4441b15b-bbae-46bf-8311-b966f51cc957","notation":["4441b15b-bbae-46bf-8311-b966f51cc957"],"prefLabel":{"de":"Zahlen zerlegen"},"definition":{"de":"Zahlen zerlegen ist ein einfacher Weg, um große Zahlen in kleinere Teile aufzuteilen, um sie besser zu verstehen oder zu bearbeiten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/479c3c32-d2d5-4ed5-8109-4246aac046e0","notation":["479c3c32-d2d5-4ed5-8109-4246aac046e0"],"prefLabel":{"de":"Rechengesetze"},"definition":{"de":"Rechengesetze sind wie Zauberregeln in der Mathematik. Sie helfen uns, Zahlenleichter zu addieren, subtrahieren, multiplizieren und teilen. Mit diesen Gesetzen können wir knifflige Matheaufgaben ganz einfach lösen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/66835c9b-d835-4c3c-9b49-0bb2ad90595d","notation":["66835c9b-d835-4c3c-9b49-0bb2ad90595d"],"prefLabel":{"de":"schriftliche Subtraktion"},"definition":{"de":"Schriftliche Subtraktion ist wie ein Zaubertrick, um große Zahlen voneinander abzuziehen. Wir schreiben die Zahlen untereinander und ziehen sie eine nach der anderen ab. So können wir herausfinden, wie viele Dinge übrig bleiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/66fc2282-f854-4854-a747-d4ed7be10571","notation":["66fc2282-f854-4854-a747-d4ed7be10571"],"prefLabel":{"de":"Subtraktion"},"definition":{"de":"Subtraktion ist wie Zauberei für Zahlen! Wenn du Subtraktion verwendest, kannst du herausfinden, wie viel kleiner eine Zahl im Vergleich zu einer anderen ist. Zum Beispiel, wenn du 8 Äpfel hast und 3 davon isst, kannst du durch Subtraktion herausfinden, wie viele Äpfel du noch übrig hast."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/85366f86-98a5-4ba1-a536-db066bdb9165","notation":["85366f86-98a5-4ba1-a536-db066bdb9165"],"prefLabel":{"de":"Addition"},"definition":{"de":"Wenn du zwei Bonbons und drei Bonbons zusammenzählst, bekommst du fünf Bonbons. Das ist, was Addition bedeutet - das Hinzufügen von Zahlen, um herauszufinden, wie viele Dinge du insgesamt hast."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8753d1b0-c985-4b63-98b3-93d90805bc07","notation":["8753d1b0-c985-4b63-98b3-93d90805bc07"],"prefLabel":{"de":"Sachaufgaben zur Vertiefung der Grundrechenarten"},"definition":{"de":"Sachaufgaben sind wie spannende Rätsel, die dir helfen, Mathe besser zu verstehen. Mit ihnen kannst du das Addieren, Subtrahieren, Multiplizieren und Teilen ganz praktisch anwenden, um echte Probleme zu lösen. Bereit, dich auf ein Mathe-Abenteuer zu begeben?"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8f73479d-b9bc-4033-8f3d-249a83c3fc87","notation":["8f73479d-b9bc-4033-8f3d-249a83c3fc87"],"prefLabel":{"de":"Rechenvorteile"},"definition":{"de":"Rechenvorteile sind wie kleine Superkräfte für Zahlen! Damit kannst du schneller rechnen und knifflige Aufgaben ganz einfach lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c4bd25a6-78be-44e0-9fef-b53bb9ff4708","notation":["c4bd25a6-78be-44e0-9fef-b53bb9ff4708"],"prefLabel":{"de":"schriftliche Division"},"definition":{"de":"Um herauszufinden, wie oft eine kleinere Zahl in die größere passt, dividieren wir.  Und immer wenn wir das im Kopf nicht schaffen, machen wir es schriftlich."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c6f4b917-7506-4f4f-9145-bfe6659bfad7","notation":["c6f4b917-7506-4f4f-9145-bfe6659bfad7"],"prefLabel":{"de":"Zahlen vergleichen"},"definition":{"de":"Stell dir vor, Zahlen sind wie Freunde, die miteinander spielen. Manchmal sind sie groß, manchmal klein. Und wenn wir Zahlen vergleichen, finden wir heraus, welche Zahl die größte oder die kleinste ist. Lass uns gemeinsam entdecken, wie das geht!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cfcfa430-7baf-4a6e-8ece-17b75a2a2f69","notation":["cfcfa430-7baf-4a6e-8ece-17b75a2a2f69"],"prefLabel":{"de":"Sachaufgaben"},"definition":{"de":"Sachaufgaben sind wie kleine Abenteuer in der Mathe-Welt! In ihnen findest du spannende Geschichten und musst dann herausfinden, wie du Zahlen und Mathe verwenden kannst, um das Problem zu lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e5b49679-8c00-4b3c-aca4-8735fd08b182","notation":["e5b49679-8c00-4b3c-aca4-8735fd08b182"],"prefLabel":{"de":"Division"},"definition":{"de":"Division ist eine wichtige mathematische Operation, bei der wir Dinge gleichmäßig aufteilen. Stell dir vor, du hast eine Tafel Schokolade und möchtest sie mit deinen Freunden teilen. Die Division hilft dir herauszufinden, wie viele Stücke jeder bekommen kann, damit alle fair behandelt werden."}},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/edf5fae0-e45d-4a69-8d11-c99da8601e6f","notation":["edf5fae0-e45d-4a69-8d11-c99da8601e6f"],"prefLabel":{"de":"Multiplikation"},"definition":{"de":"Wenn wir multiplizieren, kombinieren wir Zahlen, um herauszufinden, wie viele Dinge wir insgesamt haben. Zum Beispiel, wenn du 3 Tüten hast und in jeder Tüte 4 Süßigkeiten hat, dann hast du insgesamt 12 Süßigkeiten!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7fccdc6c-dc31-40c4-8a4b-6cf0b55b433c","notation":["7fccdc6c-dc31-40c4-8a4b-6cf0b55b433c"],"prefLabel":{"de":"Negative Zahlen"},"definition":{"de":"Negative Zahlen beschreiben Zustände oder Vorgänge bei denen weniger als nichts vorhanden ist. Mit alltäglichen Beispielen, wie der Temperatur und Aufzügen, können die negativen Zahlen vermittelt werden. Ebenfalls sind negative Zahlen für das Bankwesen von großer Bedeutung, indem sie Schulden beschreiben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1303794b-2d57-4c9d-bcfc-011727e7f606","notation":["1303794b-2d57-4c9d-bcfc-011727e7f606"],"prefLabel":{"de":"Einführung der negativen Zahlen"},"definition":{"de":"Normalerweise zählen wir immer von 0 und höher, aber manchmal müssen wir andersrum, zum Beispiel wenn es um Schulden oder Temperaturen unter Null geht. Das sind die besonderen Zahlen, die wir 'negative Zahlen' nennen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2e86dceb-602c-40e8-b192-ad9c7ce30ab1","notation":["2e86dceb-602c-40e8-b192-ad9c7ce30ab1"],"prefLabel":{"de":"Addition & Subtraktion negativer Zahlen"},"definition":{"de":"Die Addition und Subtraktion negativer Zahlen sind grundlegende mathematische Operationen, die in vielen Bereichen des täglichen Lebens und der Wissenschaft eine wichtige Rolle spielen. Sie ermöglichen es, Veränderungen, Schulden, Temperaturunterschiede und vieles mehr präzise zu berechnen und zu verstehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/79cabc0c-7614-4ba5-b7fe-b7dcb09d2827","notation":["79cabc0c-7614-4ba5-b7fe-b7dcb09d2827"],"prefLabel":{"de":"Multiplikation & Division negativer Zahlen"},"definition":{"de":"Die Multiplikation und Division negativer Zahlen kann auf den ersten Blick verwirrend wirken. Wirhelfen, die Regeln und Prinzipien hinter diesen Operationen zu klären."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/216afeb6-e115-4405-a3bf-815abd7713fe","notation":["216afeb6-e115-4405-a3bf-815abd7713fe"],"prefLabel":{"de":"Gleichungen"},"definition":{"de":"Als Gleichung bezeichnet man einen mathematischen Ausdruck, der zwei Terme durch ein Gleichheitszeichen verbindet. Gleichungen ohne Variablen sind entweder wahre oder falsche Aussagen. Sofern mindestens eine Variable in der Gleichung vorhanden ist, wird eine Gleichung zu einer Aussageform, die durch das Einsetzen konkreter Werte wahr oder falsch wird."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/15804388-a34e-4093-a999-1b313bc9e96b","notation":["15804388-a34e-4093-a999-1b313bc9e96b"],"prefLabel":{"de":"Quadratische Gleichungen"},"definition":{"de":"Quadratische Gleichungen sind wie knifflige Rätsel in der Mathewelt. Sie haben immer eine geheimnisvolle Zahl x, die wir herausfinden müssen, und sie sehen aus wie x² + 2x + 1 = 0. Mit ein bisschen Magie der Mathematik können wir diese Zahlen entschlüsseln und herausfinden, wie groß das Geheimnis x ist!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/39f845f1-65d5-4f46-ba51-918133501101","notation":["39f845f1-65d5-4f46-ba51-918133501101"],"prefLabel":{"de":"Ungleichungen"},"definition":{"de":"Hier geht es um verschiedene Arten von Ungleichungen, zugehörigen Lösungsverfahren und Anwendungen in realen Problemstellungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/67fac83c-df8a-47e3-9a53-8a25d96f7f40","notation":["67fac83c-df8a-47e3-9a53-8a25d96f7f40"],"prefLabel":{"de":"Bruchgleichungen"},"definition":{"de":"\nBruchgleichungen sind mathematische Gleichungen, in denen Bruchausdrücke vorkommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/92c528b8-850c-49cb-b938-a64daaeb9126","notation":["92c528b8-850c-49cb-b938-a64daaeb9126"],"prefLabel":{"de":"Aufstellen & Lösen von Gleichungen"},"definition":{"de":"Gleichungen sind wie magische Rätsel in der Mathematik. Wenn wir sie lösen, finden wir heraus, welche Zahl zu einem Geheimnis führt. Zum Beispiel, wenn wir 5 + x = 10 haben, müssen wir herausfinden, was x ist, und wenn wir das Geheimnis lüften, wird x gleich 5 sein! Es ist wie Zauberei, die uns hilft, mathematische Probleme zu lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/00da2fcc-64b5-4db7-9b9b-d41e7381aca9","notation":["00da2fcc-64b5-4db7-9b9b-d41e7381aca9"],"prefLabel":{"de":"Potenzen & Logarithmen"},"definition":{"de":"Mit dem Logarithmus ergibt sich eine weitere Möglichkeit, eine Potenz zu finden, die einen bestimmten Wert liefert. Mithilfe des Logarithmus wird zu einer gegebenen Basis der entsprechende Exponent durch Anwendung von Rechengesetzten berechnet. In der Praxis ist unter anderem der Logarithmus naturalis (Logarithmus zur Basis e) von besonderer Bedeutung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/18bc7c0d-b1d6-4b36-b76d-aaab01f4cac2","notation":["18bc7c0d-b1d6-4b36-b76d-aaab01f4cac2"],"prefLabel":{"de":"Potenzen mit gebrochenen Exponenten"},"definition":{"de":"2 / 3\n\nPotenzen mit gebrochenen Exponenten sind wie Zahlen im Geheimcode. Anstatt ganze Zahlen als Exponenten zu haben, verwenden wir Brüche. Das hilft uns, komplizierte Rechnungen zu lösen und Dinge auf magische Weise zu vereinfachen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/33410c41-bbdb-466a-bf76-c4f14ea0520a","notation":["33410c41-bbdb-466a-bf76-c4f14ea0520a"],"prefLabel":{"de":"Potenzen mit negativen Exponenten"},"definition":{"de":"Negative Exponenten in der Mathematik zeigen an, dass eine Zahl in einen Bruch umgewandelt wird. Sie sind vor allem nützlich, um mit sehr  sehr kleinen Zahlen zu arbeiten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4334da03-4d41-4c52-8c23-0ab8c71ec529","notation":["4334da03-4d41-4c52-8c23-0ab8c71ec529"],"prefLabel":{"de":"Grundbegriffe Potenzen & Logarithmen"},"definition":{"de":"Potenzen sind wie magische Zahlen, die uns helfen, schneller zu rechnen. Wenn wir eine Zahl immer wieder mit sich selbst multiplizieren - das nennt man eine Potenz! Logarithmen sind da Gegenteil - wie Zauberworte, mit denen wir herausfinden können, wie oft wir eine Zahl mit sich selbst multiplizieren müssen, um eine andere Zahl zu bekommen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6224c75c-2d8a-4e2c-b152-f144ed651621","notation":["6224c75c-2d8a-4e2c-b152-f144ed651621"],"prefLabel":{"de":"Potenzgesetze"},"definition":{"de":"Potenzgesetze sind mathematische Regeln, die uns helfen, Potenzen zu verstehen und zu berechnen. Mit ihrer Hilfe können wir zum Beispiel große Zahlen einfacher darstellen und rechnen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e10ad95c-2fdb-440c-a955-f5a654d24d6a","notation":["e10ad95c-2fdb-440c-a955-f5a654d24d6a"],"prefLabel":{"de":"Logarithmengesetze"},"definition":{"de":"Logarithmen sind wie geheime Superkräfte der Zahlen! Sie helfen uns, große Zahlen in kleine Abenteuer zu verwandeln und machen schwierige Rechenaufgaben ganz einfach. Mit den Logarithmengesetzen lernen wir, wie man diese Superkräfte klug einsetzt und mathematische Rätsel im Handumdrehen löst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d6c88b81-b639-4b89-94d7-c0507a3e90be","notation":["d6c88b81-b639-4b89-94d7-c0507a3e90be"],"prefLabel":{"de":"Bruchrechnung"},"definition":{"de":"Brüche finden in vielen privaten und beruflichen Bereichen Anwendung. Sie zeigen (Stimm-)Anteile und Mischungsverhältnisse. Breuchrechnung bildet die Grundlage zum Verständnis von Gleichungsberechnungen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1d926161-36be-491c-96f4-cead912881a1","notation":["1d926161-36be-491c-96f4-cead912881a1"],"prefLabel":{"de":"Besondere Brüche"},"definition":{"de":"Besondere Brüche, wie Doppelbrüche, gemischte Zahlen und unechte Brüche, sind wie Mathe-Zaubertricks. Doppelbrüche haben einen Bruch oben und einen Bruch unten, gemischte Zahlen sind wie Mix aus einer ganzen Zahl und einem Bruch, und unechte Brüche sind größer als eins. Lass uns herausfinden, wie diese speziellen Brüche uns helfen, Mathematik noch spannender zu gestalten!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9e1b142f-3c9b-4c0a-8a67-d801bb2f863b","notation":["9e1b142f-3c9b-4c0a-8a67-d801bb2f863b"],"prefLabel":{"de":"Addition & Subtraktion von Brüchen"},"definition":{"de":"Brüche sind wie Pizzastücke. Wenn wir Brüche addieren, legen wir mehr Pizzastücke zusammen, um eine größere Pizza zu bekommen. Und wenn wir Brüche subtrahieren, nehmen wir einige Pizzastücke weg. So können wir mit Brüchen rechnen und leckere Pizzas teilen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ac18e9bf-3209-4606-a598-e8e290c9b999","notation":["ac18e9bf-3209-4606-a598-e8e290c9b999"],"prefLabel":{"de":"Grundlagen der Bruchrechnung"},"definition":{"de":"Bruchrechnung ist wie ein magisches Zauberspiel in der Welt der Mathematik. Mit Brüchen können wir Teile von Dingen in kleine Stücke schneiden und sie gerecht unter Freunden aufteilen. In diesem Abenteuer lernen wir, wie man Brüche liest, addiert und subtrahiert, um gemeinsam noch mehr Spaß zu haben!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d2f27cc4-57d1-4741-890c-d1455512c28f","notation":["d2f27cc4-57d1-4741-890c-d1455512c28f"],"prefLabel":{"de":"Erweitern & Kürzen"},"definition":{"de":"Erweitern und Kürzen sind wie Zaubertricks für Brüche! Wenn wir Brüche erweitern, machen wir Zähler und Nenner größer, und wenn wir sie kürzen, machen wir Zähler und Nenner kleiner. Das hilft uns, Matheaufgaben mit Brüchen einfacher zu lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fd260ea1-fe69-4eab-8343-0c8898ea1a76","notation":["fd260ea1-fe69-4eab-8343-0c8898ea1a76"],"prefLabel":{"de":"Multiplikation & Division von Brüchen"},"definition":{"de":"Hey, hast du dich jemals gefragt, wie Brüche multipliziert und geteilt werden? Das ist ganz einfach! Wenn du Brüche multiplizierst, multiplizierst du einfach die Zahlen oben und unten. Beim Teilen von Brüchen drehst du den zweiten Bruch um und multiplizierst dann. Es ist wie ein kleines Mathe-Zauberspiel!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c9022bf8-04ad-478b-9384-3474f2d44e93","notation":["c9022bf8-04ad-478b-9384-3474f2d44e93"],"prefLabel":{"de":"Dezimalzahlen"},"definition":{"de":"Dezimalzahlen stellen nicht-ganze Zahlen beziehungsweise Teile an einem Ganzen dar. Sie werden für die Darstellung irrationaler Zahlen verwendet, die nicht durch einen Bruch dargestellt werden können. Im Alltag werden Größe, Gewicht und Geld mit Dezimalzahlen dargestellt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/347055ee-17fb-425d-ac33-2c16b5f484e8","notation":["347055ee-17fb-425d-ac33-2c16b5f484e8"],"prefLabel":{"de":"Vergleich von Dezimalzahlen"},"definition":{"de":"Vergleichen von Dezimalzahlen ist wie ein Wettrennen zwischen Zahlen! Du schaust dir an, welche Zahl größer oder kleiner ist, um zu wissen, welche gewinnt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/49c990d3-3578-4486-ac9f-83c83d075c26","notation":["49c990d3-3578-4486-ac9f-83c83d075c26"],"prefLabel":{"de":"Dezimalschreibweise"},"definition":{"de":"Die Dezimalschreibweise ist wie das Verwandeln von Bruchteilen in Zahlen, damit wir sie besser verstehen und vergleichen können."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aa4c3663-5570-42fa-8dc3-b51d4c1a655f","notation":["aa4c3663-5570-42fa-8dc3-b51d4c1a655f"],"prefLabel":{"de":"Rechnen mit Dezimalzahlen"},"definition":{"de":"Rechnen mit Dezimalzahlen ist, als ob du mit Geld spielst! Dezimalzahlen haben einen Punkt, der sie in \"Euro\" und \"Cent\" teilt. Du kannst sie addieren, subtrahieren, multiplizieren und teilen, genau wie bei Ganzzahlen. Es ist super praktisch, wenn du mit Preisen, Mengen oder Brüchen umgehen möchtest."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bcceb32d-6629-40d2-a4fd-ed25155b9fc8","notation":["bcceb32d-6629-40d2-a4fd-ed25155b9fc8"],"prefLabel":{"de":"Runden von Dezimalzahlen"},"definition":{"de":"Manchmal sind Zahlen mit vielen Nachkommastellen schwer zu verstehen. Beim Runden helfen wir ihnen, indem wir sie zu freundlichen Zahlen machen, die einfacher zu verwenden sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/dd7db17a-212f-4a31-983b-c388e20b0690","notation":["dd7db17a-212f-4a31-983b-c388e20b0690"],"prefLabel":{"de":"Periodische Dezimalzahlen"},"definition":{"de":"\nPeriodische Dezimalzahlen, auch als periodische Brüche oder wiederkehrende Dezimalstellen bekannt, sind  Zahlen, bei denen sich eine oder mehrere Ziffern nach dem Dezimalpunkt in einem regelmäßigen, sich wiederholenden Muster wiederholen. "},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f119782e-7a6d-4f3a-9088-494a6fe757dd","notation":["f119782e-7a6d-4f3a-9088-494a6fe757dd"],"prefLabel":{"de":"Dezimalzahlen & Brüche"},"definition":{"de":"Dezimalzahlen sind wie Zahlen mit einem Komma, wie 0,5 oder 2,75. Brüche sind wie Teile von Ganzen, zum Beispiel 1/2 oder 3/4. Wir können Dezimalzahlen und Brüche verwenden, um Mengen und Dinge auf verschiedene Weisen darzustellen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4738dd46-e67d-4d14-98de-17c9536c6616","notation":["4738dd46-e67d-4d14-98de-17c9536c6616"],"prefLabel":{"de":"Ungleichungen"},"definition":{"de":"Hier geht es darum, wie man Ungleichungen löst. Dabei sind oft Fallunterscheidungen zu beachten"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cbd48511-a82f-48e7-bb0d-d059af6027c7","notation":["cbd48511-a82f-48e7-bb0d-d059af6027c7"],"prefLabel":{"de":"Teiler, Primzahlen, Vielfache"},"definition":{"de":"Die Zerlegung und Analyse der Zahlen bietet eine Grundlage für die weiteren Themengebiete der Mathematik. Mit dem Wissen der Teiler und Vielfache wird ein grundlegendes Verständnis für Zahlen aufgebaut, welches zum Beispiel bei der Bruchrechnung die Grundlage bildet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4d990f30-edbf-4549-b443-ec93d604e26a","notation":["4d990f30-edbf-4549-b443-ec93d604e26a"],"prefLabel":{"de":"Primzahlen"},"definition":{"de":"In dieser Sammlung haben wir Material, in denen erklärt wird, was Primzahlen sind und wie man sie finden kann - zum Beispiel mit dem Sieb des Eratosthenes"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c7d31c95-cf9b-4907-b226-37286a5b370a","notation":["c7d31c95-cf9b-4907-b226-37286a5b370a"],"prefLabel":{"de":"Primfaktorzerlegung"},"definition":{"de":"Zerlegung in Primfaktoren - ein wichtiges Hilfsmittel, wenn man den größtengemeinsamen Teiler oder das kleinst gemeinsame Vielfache bestimmen will"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/cb98a12d-d040-4e9c-8e0b-c2c30097a8d5","notation":["cb98a12d-d040-4e9c-8e0b-c2c30097a8d5"],"prefLabel":{"de":"Teilbarkeitsregeln"},"definition":{"de":"Wann ist eine Zahl durch Zwei, Drei, Vier, Fünf, Sechs oder Neun teilbar? Wichtige Fragen, auf die diese Teilbarkeitsregeln Antworten geben"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f0585c9f-faf2-4e71-b361-25dd0eccc8ea","notation":["f0585c9f-faf2-4e71-b361-25dd0eccc8ea"],"prefLabel":{"de":"Gemeinsame Teiler & Vielfache"},"definition":{"de":"Gemeinsame Teiler und gemeinsame Vielfache. Darunter auch der größte gemeinsame Teiler (ggT) und das kleinste gemeinsame Vielfache (kgV)"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bf9543bb-2999-4efc-b9c7-e0e852af5c7e","notation":["bf9543bb-2999-4efc-b9c7-e0e852af5c7e"],"prefLabel":{"de":"Terme"},"definition":{"de":"Terme sind sinnvolle mathematische Rechenvorschriften. Sie können Zahlen und Variablen mit Rechenoperatoren und Klammern verbinden. Allerdings enthalten sie keine Relationszeichen. Auch jedes Variablen- oder Konstantensymbol ist ein Term."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ab03c6ab-f732-476d-9f80-959535894cc7","notation":["ab03c6ab-f732-476d-9f80-959535894cc7"],"prefLabel":{"de":"Binomische Formeln"},"definition":{"de":"Binomische Formeln sind wie Mathe-Zaubertricks! Sie helfen uns, Klammern zu öffnen und Begriffe zu vereinfachen. Mit diesen Formeln kannst du magische Verwandlungen in deinen Matheaufgaben durchführen und schwierige Probleme ganz einfach lösen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ae6fc547-6b05-41b0-8d4e-a3d857c3685f","notation":["ae6fc547-6b05-41b0-8d4e-a3d857c3685f"],"prefLabel":{"de":"Vereinfachung von Termen"},"definition":{"de":"Stell dir vor, Terme sind wie Puzzleteile in der Mathematik. Manchmal sind sie ein bisschen kompliziert, aber keine Sorge! Wir können Terme vereinfachen, indem wir ähnliche Teile zusammenfügen, wie wenn wir Puzzleteile passend machen. Das macht Mathe viel einfacher und spaßiger!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c15f9590-62af-442e-a1f0-d66d1db33a84","notation":["c15f9590-62af-442e-a1f0-d66d1db33a84"],"prefLabel":{"de":"Terme & Variablen"},"definition":{"de":"Terme und Variablen sind wie Puzzleteile in der Mathe-Welt. Aus ihnen bauen wir Sätze und Formeln  zusammen, um coole Rechnungen zu machen. Mit ihnen können wir Geheimnisse - nicht nur in Matheaufgaben - entdecken und knacken!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b68335b8-6a1f-4c45-ab11-da49d02a1786","notation":["b68335b8-6a1f-4c45-ab11-da49d02a1786"],"prefLabel":{"de":"Proportionale und antiproportionale Zuordnungen"},"definition":{"de":"Anhand der Wertetabelle oder dem Graphen kann man erkennen, ob es sich um eine proportionale oder antiproportionale Zuordnung handelt. Bei einer proportionalen Zuordnung gilt: je mehr x, desto mehr y, der Graph ist eine Ursprungsgerade. Umgekehrt ist es bei der antiproportionalen Zuordnung: je mehr x, desto weniger y und der Graph stellt eine Hyperbel dar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ca5a3030-f830-45e2-88eb-2b32883a9888","notation":["ca5a3030-f830-45e2-88eb-2b32883a9888"],"prefLabel":{"de":"Römische Zahlen"},"definition":{"de":"Römische Zahlen stellen natürlilche Zahlen durch sieben lateinische Buchstaben dar. Sie wurden im antiken Rom verwendet und stellen auch heute noch zum Beispiel Jahreszahlen an hinstorischen Bauwerken dar."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ca93c23d-ae33-4100-91e4-0769c8079e97","notation":["ca93c23d-ae33-4100-91e4-0769c8079e97"],"prefLabel":{"de":"Dualzahlen"},"definition":{"de":"Wie kann ein Computer rechnen, obwohl er nur die Ziffern 0 und 1 kennt? In der Tat kann man mit diesen zwei Ziffern sämtliche Zahlen darstellen, wenn man das Dualsystem nutzt. Wie man das den Schülerinnen und Schülern erklären kann, dazu sammeln wir hier gutes Unterrichtsmaterial."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d074eb41-bb3d-4ce2-b71b-6fce18cdd36a","notation":["d074eb41-bb3d-4ce2-b71b-6fce18cdd36a"],"prefLabel":{"de":"Zahlenmengen"},"definition":{"de":"Zahlenmengen umfassen eine fest definierte Menge an Zahlen. Die elementaren Zahlenmengen sind aufbauend definiert und sind jeweils in der nächstgrößeren Zahlenmenge vollständig enthalten. Verständnis von Zahlenmengen findet zum Beispiel in der Oberstufe zum Bestimmen des Definitions- und Wertebereich von Funktionen Anwendung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/3faa2935-ee4b-46b3-b88b-e493922b69d0","notation":["3faa2935-ee4b-46b3-b88b-e493922b69d0"],"prefLabel":{"de":"Einheiten umrechnen"},"definition":{"de":"Mit Einheiten und Größen zu rechen und sie umzurechnen oder umzuwandeln ist eine Basiskompetenz für das mathematische Schulleben - aber auch für den Alltag. \"Einheiten umrechnen\" beinhaltet Zeiten (z.B. Tage, Stunden, Minuten), Flächen (z.B. Quadratmeter, Hektar), Längen (z.B. Meter, Kilometer), Volumen (z.B. Liter, Kubikmeter) und Gewicht (z.B. Gramm, Kilogramm)."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/71221a00-3150-486c-a80b-2bc56ad7d811","notation":["71221a00-3150-486c-a80b-2bc56ad7d811"],"prefLabel":{"de":"Längeneinheiten"},"definition":{"de":"2 / 2\n\nLängeneinheiten helfen uns dabei, die Größe von Dingen zu messen. Sie sagen uns, wie lang etwas ist. Zum Beispiel, ein Meter ist so lang wie drei Schritte, und ein Zentimeter ist viel kleiner, etwa so groß wie eine Gummibärchen. Wir benutzen Längeneinheiten, um zu verstehen, wie groß oder klein die Dinge um uns herum sind."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7ebb1afa-f665-4486-96ab-a6c0c71b3eeb","notation":["7ebb1afa-f665-4486-96ab-a6c0c71b3eeb"],"prefLabel":{"de":"Zeiten"},"definition":{"de":"Auf einer Uhr kannst du sehen, wann es Zeit ist, aufzustehen, zu essen oder zu schlafen. Die Uhr hat Zeiger, die wie magische Hände die Minuten und Stunden zeigen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a4364d7d-8028-4e06-a368-93b970d22af1","notation":["a4364d7d-8028-4e06-a368-93b970d22af1"],"prefLabel":{"de":"Flächeneinheiten"},"definition":{"de":"Flächeneinheiten helfen uns, zu verstehen, wie groß Dinge sind. Wir messen Flächen mit Einheiten wie Quadratzentimeter, Quadratmetern und Quadratkilometern. Damit können wir Flächen von kleinen Blättern bis zu großen Feldern messen und vergleichen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bd0d6322-0478-45e7-9ae2-89265d04080a","notation":["bd0d6322-0478-45e7-9ae2-89265d04080a"],"prefLabel":{"de":"Gewicht"},"definition":{"de":"Gewicht ist, wie schwer etwas ist. Zum Beispiel, ein Teddybär wiegt weniger als ein Auto, weil er viel leichter ist. Wir können Dinge wie Bücher, Spielzeug und Essen wiegen, um herauszufinden, wie schwer sie sind"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/e99cae99-b614-4828-95cb-f51ca96e9360","notation":["e99cae99-b614-4828-95cb-f51ca96e9360"],"prefLabel":{"de":"Volumeneinheiten"},"definition":{"de":"Zinsen sind wie ein kleines Extra-Geld, das du bekommst, wenn du dein Geld in der Bank aufbewahrst. Wenn du dein Geld dort länger lässt, wird es mehr! Das ist, als ob du für dein Sparschwein eine Belohnung bekommst."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/90c6411f-eadc-4478-b668-866ed6eb9012","notation":["90c6411f-eadc-4478-b668-866ed6eb9012"],"prefLabel":{"de":"Geometrie"},"definition":{"de":"Historisch gesehen ist die Geometrie Euklids das älteste Teilgebiet der Mathematik. Sie galt seit der Antike als ein Vorbild für exaktes Denken, sodass nicht wenige Philosophen forderten, wissenschaftliches Arbeiten solle immer \"more geometrico\" - also nach Art der Geometrie - erfolgen. Zentrale Grundbegriffe der Geometrie sind Punkt, Gerade, Abstände und Winkel sowie das Kontruieren mit Zirkel und Lineal."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/8a752bc4-e616-4f70-a1b6-60099d43cd2a","notation":["8a752bc4-e616-4f70-a1b6-60099d43cd2a"],"prefLabel":{"de":"Ebene Geometrie"},"definition":{"de":"Konstruieren mit Zirkel und Lineal - das war in der Antike die Hauptaufgabe der Geometrie. Inzwischen ist diese Beschränkung aufgehoben, denn mit der Kreiszahl Pi kann nun auch der Flächeninhalt eines Kreises bestimmt werden - eine Aufgabe, die als \"Quadratur des Kreises\" sprichwörtlich geworden ist, und die mit Zirkel und Lineal allein nicht funktioniert."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/027c17e9-614e-490f-a9b8-99a9157ae85e","notation":["027c17e9-614e-490f-a9b8-99a9157ae85e"],"prefLabel":{"de":"Vierecke"},"definition":{"de":"Vierecke sind besondere Formen, die aus genau vier Seiten bestehen. Sie können wie Fenster, Türen oder sogar Geschenkboxen aussehen! Vierecke sind überall um uns herum, und sie helfen uns, die Welt der Formen zu entdecken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/05e67c0d-2239-43cb-84b3-abc17c605dc0","notation":["05e67c0d-2239-43cb-84b3-abc17c605dc0"],"prefLabel":{"de":"Inkreis & Umkreis"},"definition":{"de":"Im Matheland gibt es zwei besondere Kreise: der Inkreis und der Umkreis. Der Inkreis sitzt ganz drinnen im Viereck oder Dreieck, während der Umkreis das ganze Viereck oder Dreieck umgibt. Sie helfen uns, die Geheimnisse der Formen zu entdecken!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1d55ee54-6a27-4a48-9f55-312db04aaa8b","notation":["1d55ee54-6a27-4a48-9f55-312db04aaa8b"],"prefLabel":{"de":"Symmetrie"},"definition":{"de":"Symmetrie bedeutet, dass Dinge auf eine besondere Art gleich aussehen. Wenn etwas symmetrisch ist, dann ist die eine Seite genauso wie die andere. Das kann wie ein Spiegelbild sein und macht viele Dinge in der Natur und Kunst besonders schön und interessant."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/2d1182dd-dff5-45ca-ad80-868ef3957822","notation":["2d1182dd-dff5-45ca-ad80-868ef3957822"],"prefLabel":{"de":"Kreise"},"definition":{"de":"Kreise sind wie magische Formen ohne Ecken. Sie haben immer denselben Abstand von der Mitte und sehen aus wie leckere Kekse! In der Mathematik und im Leben begegnen wir Kreisen überall."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4962e9d9-fb54-4faf-9116-8177decf2b14","notation":["4962e9d9-fb54-4faf-9116-8177decf2b14"],"prefLabel":{"de":"Winkel"},"definition":{"de":"Wenn zwei Linien sich treffen, entstehen Winkel. Davon gibt es  verschiedene Arten, wie spitze Winkel, rechte Winkel , stumpfe Winkel. Sie helfen uns, die Welt der Formen zu entdecken."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/51ba2b6a-95dc-4dbc-85e4-fb3a5156e2f2","notation":["51ba2b6a-95dc-4dbc-85e4-fb3a5156e2f2"],"prefLabel":{"de":"Strahlensätze"},"definition":{"de":"Strahlensätze sind ein fundamentales Konzept in der Geometrie, das es uns ermöglicht, Proportionen und Beziehungen zwischen geometrischen Figuren zu verstehen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5a376d0e-a7a9-4f2e-ade8-cb2913182ff3","notation":["5a376d0e-a7a9-4f2e-ade8-cb2913182ff3"],"prefLabel":{"de":"Senkrechte und Parallelen"},"definition":{"de":"Hey, kleine Entdecker! Hier gehts um zwei besondere Arten von Linien: Senkrechte und Parallelen. Stell dir Senkrechte wie Baumstämme vor, die senkrecht in den Boden ragen. Parallelen hingegen sind wie Gleise für Züge. Sie verlaufen niemals zusammen, aber sie sind immer in derselben Richtung, ohne sich zu treffen. Das ist ziemlich cool, oder?"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/703fee62-9a7e-4ba4-8c04-c62a79f8dad2","notation":["703fee62-9a7e-4ba4-8c04-c62a79f8dad2"],"prefLabel":{"de":"Figuren kennenlernen"},"definition":{"de":"Geometrische Figuren sind Formen, die wir überall um uns herum sehen können. Sie haben lustige Namen wie Kreis, Dreieck und Viereck. Lass uns gemeinsam entdecken, wie sie aussehen und wie wir sie in der Welt um uns herum finden können!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/874aba2c-5ca9-43d7-892a-c1496bda00ea","notation":["874aba2c-5ca9-43d7-892a-c1496bda00ea"],"prefLabel":{"de":"Dreieckskonstruktionen"},"definition":{"de":"Hey, wisst ihr, was Dreiecke so besonders macht? Sie sind wie Bausteine der Mathematik! Mit Dreieckskonstruktionen können wir lernen, wie man sie zeichnet und entdecken, warum sie so wichtig sind. Lasst uns gemeinsam in die aufregende Welt der Dreiecke eintauchen!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/995536ab-bee1-4de5-9978-237caf680fac","notation":["995536ab-bee1-4de5-9978-237caf680fac"],"prefLabel":{"de":"Satzgruppe des Pythagoras"},"definition":{"de":"Der Satz des Pythagoras ist wie ein magischer Trick für rechtwinklige Dreiecke. Er hilft uns herauszufinden, wie lange die Seiten eines Dreiecks sind. Denk daran: In einem rechtwinkligen Dreieck ist die Summe der Quadrate der beiden kürzeren Seiten gleich dem Quadrat der längsten Seite! Cool, oder?"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/aa0ecc77-04b4-44e6-a1e0-1517b7638a02","notation":["aa0ecc77-04b4-44e6-a1e0-1517b7638a02"],"prefLabel":{"de":"Dreiecke"},"definition":{"de":"Dreiecke sind spezielle Formen mit genau drei Seiten. Man kann sie in vielen Dingen wie Pizzastücken oder Verkehrsschildern finden. Dreiecke haben oft lustige Namen wie gleichschenklig oder rechtwinklig, und sie sind überall um uns herum!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/bdac8950-7b4d-4388-b33a-7766a6569c92","notation":["bdac8950-7b4d-4388-b33a-7766a6569c92"],"prefLabel":{"de":"Winkelsummen"},"definition":{"de":"Winkelsummen sind wie ein Rätsel in der Mathematik. Wenn wir alle Winkel in einer Form oder einem Dreieck addieren, bekommen wir eine besondere Zahl. Zum Beispiel haben alle Quadrate vier rechte Winkel und deshalb eine Winkelsumme von 360 Grad. Das macht Mathe spannend!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ebdd08b9-345d-46b9-96d5-8d7c39d53dc1","notation":["ebdd08b9-345d-46b9-96d5-8d7c39d53dc1"],"prefLabel":{"de":"Rechtecke"},"definition":{"de":"Rechtecke sind spezielle Formen in Mathe mit Seiten,  die sich rechtwinklig treffen. Rechtecke sehen aus wie Türme, und sie sind sehr nützlich, um Dinge wie Bücher oder Bilderrahmen darzustellen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fb28b42d-a262-48bc-86a9-0b27ce7f70bd","notation":["fb28b42d-a262-48bc-86a9-0b27ce7f70bd"],"prefLabel":{"de":"Satz des Thales"},"definition":{"de":"Der Satz des Thales ist ein besonderes Geheimnis der Geometrie. Er zeigt uns, dass wenn wir einen Halbkreis haben, dann sind alle Winkel an der Kreislinie immer genau 90 Grad groß. Das ist wie ein versteckter Zaubertrick in der Mathematik!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6a1aa6f4-5a29-4d66-94c9-35845524c775","notation":["6a1aa6f4-5a29-4d66-94c9-35845524c775"],"prefLabel":{"de":"Räumliche Geometrie"},"definition":{"de":"Vor Jahren behauptete ein Bestseller-Autor, die Pyramiden von Gizeh seien das Werk von Außerirdischen.\nAber das stimmt nicht. Das Wissen, das die Geometrie schon in der Antike über dreidimensionale Körper bereithielt, sieht harmlos aus, enthält aber enormes Potential - nicht nur für ägyptische Monumentalbauten."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4094a984-2d86-4dd5-ba58-fdb8c6224606","notation":["4094a984-2d86-4dd5-ba58-fdb8c6224606"],"prefLabel":{"de":"Prisma"},"definition":{"de":"Ein Prisma ist wie ein magischer Kasten, es hat zwei gleiche Formen oben und unten und viele Seiten dazwischen. Prismen sehen aus wie bunte Türme und können uns helfen, spannende Dinge über Formen und Farben zu lernen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/48ba081b-b725-41d7-9822-0fde3457a512","notation":["48ba081b-b725-41d7-9822-0fde3457a512"],"prefLabel":{"de":"Kegelschnitt"},"definition":{"de":"Kegelschnitte sind wie magische Muster in der Natur und der Mathematik. Sie sehen aus wie Kreise, Ellipsen, Parabeln und Hyperbeln und verstecken sich überall, von Regenbögen bis zu den Bahnen von Planeten!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6ab1314a-fc25-48df-a00d-947bcfa6e4f0","notation":["6ab1314a-fc25-48df-a00d-947bcfa6e4f0"],"prefLabel":{"de":"Würfel & Quader"},"definition":{"de":"Ein Würfel ist ein besonderer Kasten, der auf jeder Seite genauso groß ist. Er hat sechs gleiche quadratische Gesichter. Ein Quader ist ähnlich, aber er kann unterschiedlich lange Seiten haben. Du kannst dir vorstellen, dass ein Quader wie ein Geschenkkarton aussieht. Beide sind lustige Formen, die wir in der Mathematik entdecken können!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/781ccad4-a187-4290-bbea-f6fb423a1acc","notation":["781ccad4-a187-4290-bbea-f6fb423a1acc"],"prefLabel":{"de":"Kugel"},"definition":{"de":"Eine Kugel ist wie eine runde Zauberwelt! Sie hat keine Ecken und keine Kanten, nur eine glatte Oberfläche. Wenn du sie rollst, kann sie überall hin gehen – Kugeln sind einfach faszinierend!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9273f22f-7cfb-4fec-9e40-fcad42783024","notation":["9273f22f-7cfb-4fec-9e40-fcad42783024"],"prefLabel":{"de":"Kegel"},"definition":{"de":"Kegel sind geometrische Körper, die sich durch eine kreisförmige Grundfläche und eine Spitze auszeichnen. Sie kommen in vielfältigen Anwendungen vor und spielen eine wichtige Rolle in der Geometrie und Physik."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/a228e1a2-d5ef-4c25-b4eb-3cd3e21b95d6","notation":["a228e1a2-d5ef-4c25-b4eb-3cd3e21b95d6"],"prefLabel":{"de":"Körper kennenlernen"},"definition":{"de":"Diese Sammlung  bietet Ihnen eine  Einführung in die Vielfalt und Bedeutung von geometrischen Körpern."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b9451767-96d7-4eaa-9aa1-dd3b3ffa43fa","notation":["b9451767-96d7-4eaa-9aa1-dd3b3ffa43fa"],"prefLabel":{"de":"Zylinder"},"definition":{"de":"Ein Zylinder ist wie eine große Dose oder eine lustige Säule. Er hat einen runden Boden und eine gerade Seite, die sich nach oben erstreckt. Zylinder sehen aus wie Becher, und man kann sie für viele Dinge verwenden, sogar um leckere Getränke zu trinken!"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fe680e4c-e755-4fb4-aaf4-bbd5d54154ea","notation":["fe680e4c-e755-4fb4-aaf4-bbd5d54154ea"],"prefLabel":{"de":"Pyramide"},"definition":{"de":"Die Pyramide ist eine faszinierende geometrische Form, die in der Architektur und Mathematik eine bedeutende Rolle spielt. Mit ihrer charakteristischen dreieckigen Struktur und ihrer historischen Bedeutung in verschiedenen Kulturen, von den ägyptischen Pyramiden bis zu modernen Interpretationen, verkörpert sie das Zusammenspiel von Stabilität und Ästhetik."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/380"}]}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/eef047a3-58ba-419c-ab7d-3d0cfd04bb4e","notation":["eef047a3-58ba-419c-ab7d-3d0cfd04bb4e"],"prefLabel":{"de":"Medienbildung"},"definition":{"de":"Hier findest du ausgewählte Materialien, Hinweise auf Portale, Tools und Apps sowie Hintergrundartikel zum Thema Medienbildung und Mediendidaktik sowie Medientheorie für das Lernen und Lehren mit und über Medien. Der Aufbau orientiert sich am Kompetenzrahmen der KMK „Bildung in der digitalen Welt“, der auch – mit Anpassungen – in den Bundesländern übernommen wurde."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7d0c51ba-0a01-40dc-bed5-352c994d2acd","notation":["7d0c51ba-0a01-40dc-bed5-352c994d2acd"],"prefLabel":{"de":"Informieren und Recherchieren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/061cdd4e-5068-40ba-b2e7-95cf9c9f0b9d","notation":["061cdd4e-5068-40ba-b2e7-95cf9c9f0b9d"],"prefLabel":{"de":"Speichern und Abrufen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d300315f-d470-430a-b036-810d872e8829","notation":["d300315f-d470-430a-b036-810d872e8829"],"prefLabel":{"de":"Suchen und Filtern"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/f5db2fb3-bc87-4024-8f1a-c3cb6d5aa4ef","notation":["f5db2fb3-bc87-4024-8f1a-c3cb6d5aa4ef"],"prefLabel":{"de":"Auswerten und Bewerten"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9083a870-fe5c-4feb-87a1-bf9e78530430","notation":["9083a870-fe5c-4feb-87a1-bf9e78530430"],"prefLabel":{"de":"Kommunizieren und Kooperieren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/1472bbf4-b281-4263-8002-285acd7d0d1b","notation":["1472bbf4-b281-4263-8002-285acd7d0d1b"],"prefLabel":{"de":"Regeln und Netiquette beachten"},"definition":{"de":"Regeln im Internet werden auch Netiquette genannt. Der Begriff kommt aus dem englischen net für das „Netz“ und dem französischen etiquette für die „Verhaltensregeln“. Es geht also um den guten, angemessenen und respektvollen Umgang bei der Kommunikation im Internet."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7b7d676c-8ea6-4dc4-a76c-01befe4d4596","notation":["7b7d676c-8ea6-4dc4-a76c-01befe4d4596"],"prefLabel":{"de":"Digitale Gesellschaft (mit) gestalten"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/eb5976cd-e267-45ee-a9da-685f718fbf2d","notation":["eb5976cd-e267-45ee-a9da-685f718fbf2d"],"prefLabel":{"de":"Zusammenarbeiten und Teilen "},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/d56bc198-e56e-41cb-b418-8cf290005c25","notation":["d56bc198-e56e-41cb-b418-8cf290005c25"],"prefLabel":{"de":"Bedienen und Anwenden"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/214ee652-cc4c-4adf-8842-9b6592ced97a","notation":["214ee652-cc4c-4adf-8842-9b6592ced97a"],"prefLabel":{"de":"Grundlagen kennenlernen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6793e4d2-3997-4434-9ef3-294676377636","notation":["6793e4d2-3997-4434-9ef3-294676377636"],"prefLabel":{"de":"Daten organisieren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c22d0bce-1ff0-49c2-824c-5a4c2028ade1","notation":["c22d0bce-1ff0-49c2-824c-5a4c2028ade1"],"prefLabel":{"de":"Persönliche Daten und Privatsphäre schützen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6ee037ac-ad61-4d2a-b168-40b52599dba6","notation":["6ee037ac-ad61-4d2a-b168-40b52599dba6"],"prefLabel":{"de":"Analysieren und Reflektieren"},"definition":{"de":"Analysieren und Reflektieren fokussiert den bewussten und kritischen Umgang mit digitalen Medien. Hier findest du Informationen und Unterrichtsmaterialien zur Medienanalyse,Medienbewertung und Medienreflektion sowie zur Förderung einer selbstgesteuerten Mediennutzung unter Beachtung des Jugendmedienschutzes."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/34561f6b-7493-4692-b43d-3e1e012cffd2","notation":["34561f6b-7493-4692-b43d-3e1e012cffd2"],"prefLabel":{"de":"Medien in der digitalen Welt verstehen und reflektieren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/533b7cb1-1f53-4830-b931-55b4569c505c","notation":["533b7cb1-1f53-4830-b931-55b4569c505c"],"prefLabel":{"de":"Meinungsbildung verstehen und Manipulation erkennen"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/7505326d-d855-439e-a3d2-482c6ed16393","notation":["7505326d-d855-439e-a3d2-482c6ed16393"],"prefLabel":{"de":"Selbstgesteuerte Mediennutzung fördern"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/c4241a5d-a22b-445b-bfeb-a95c3bd111db","notation":["c4241a5d-a22b-445b-bfeb-a95c3bd111db"],"prefLabel":{"de":"Fake News"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/480"},{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/4e972329-8d73-4ea8-aaf9-bf40dbb111ad","notation":["4e972329-8d73-4ea8-aaf9-bf40dbb111ad"],"prefLabel":{"de":"Produzieren und Präsentieren"},"definition":{"de":"Produzieren und Präsentieren legt den Fokus auf die Medienproduktion im schulischen Kontext. Sei es ein kreatives Audio-, Foto- oder Filmprojekt oder die grundlegende Arbeit mit Text- und Präsentationsprogrammen, die du mit deinen Schüler:innen umsetzen möchtest. Dieser Themenbereich bietet dir zahlreiche Materialien für den Einsatz unterschiedlicher Medien für dein individuelles Medienprodukt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/54d3176e-84e0-49da-bb08-00aaae3eaeb5","notation":["54d3176e-84e0-49da-bb08-00aaae3eaeb5"],"prefLabel":{"de":"Filme"},"definition":{"de":"Filme sind ein vielseitiges Medienprodukt, das in unterschiedlichen Klassen und Fächern erstellt werden kann. Die Möglichkeiten der Arbeit mit dem Greenscreen, die Aufnahme von Screencasts, die Erstellung von Lernvideos und die Produktion von Stop Motion Filmen mit unterschiedlichen Endgeräten und Programmen werden hier vorgestellt. Ergänzt werden diese kreativen Ansätze durch Methoden und Tools zur Filmanalyse und -bewertung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/24576de9-1c28-4078-89c2-92504eedc71a","notation":["24576de9-1c28-4078-89c2-92504eedc71a"],"prefLabel":{"de":"Kreative Medienprodukte"},"definition":{"de":"Hier findest du Materialien für kreative Medienprodukte und Unterrichtsideen mit Filmen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/5c4a5347-1570-4828-8df5-cbf7573f32d2","notation":["5c4a5347-1570-4828-8df5-cbf7573f32d2"],"prefLabel":{"de":"Videobearbeitung"},"definition":{"de":"Hier findest du Materialien rund um Programme für die Videobearbeitung."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/76fff3c4-1927-4daa-95e0-f9e3d4eaca84","notation":["76fff3c4-1927-4daa-95e0-f9e3d4eaca84"],"prefLabel":{"de":"Videos suchen und finden"},"definition":{"de":"Hier findest du (Lern-)Videos."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/6b70b987-7693-461d-8ed1-3fb18effe0eb","notation":["6b70b987-7693-461d-8ed1-3fb18effe0eb"],"prefLabel":{"de":"Textverarbeitung"},"definition":{"de":"Word, OpenOffice, Pages und Co. ermöglichen das Verfassen digitaler Texte. Textverarbeitungsprogramme bieten jedoch neben dem grundlegenden Verfassen von Texten viele Möglichkeit zur kreativen Arbeit mit digitalen Medien. So können gemeinsame Geschichten, Drehbücher oder Schülerzeitungen entstehen. Kleine Programme und Apps ergänzen die klassischen Textverarbeitungsprogramme, indem sie beispielsweise die Erstellung von Wortwolken oder E-Books erlauben."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/96c17808-7006-4027-9dcf-1649722e567e","notation":["96c17808-7006-4027-9dcf-1649722e567e"],"prefLabel":{"de":"Fotos"},"definition":{"de":"Fotos und Bilder sind eine tolle Möglichkeit auch schon mit jüngeren Schüler:innen kreative Projekte umzusetzen. Die Planung, Durchführung und Bewertung von Fotoprojekten werden ebenso vorgestellt wie unterschiedliche Programme und Apps zur Bildbearbeitung sowie zur Gestaltung von Comics und Bildergeschichten. Dabei werden das Aufnehmen, Bearbeiten, Verändern und Speichern von Fotoaufnahmen berücksichtigt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b5c5847b-747d-4838-8c2a-5186f9768696","notation":["b5c5847b-747d-4838-8c2a-5186f9768696"],"prefLabel":{"de":"Tabellenkalkulation"},"definition":{"de":"Das Erstellen von Tabellen mit unterschiedlichen digitalen Werkzeugen findet insbesondere in den Naturwissenschaften Anwendung. Darüber hinaus können Tabellenkalkulationsprogramme noch viel mehr - beispielsweise können Diagramme oder Umfragen erstellt und in andere Medienprodukte eingearbeitet werden."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/ec88e105-2fa8-4b9b-8e9f-72c294aa131a","notation":["ec88e105-2fa8-4b9b-8e9f-72c294aa131a"],"prefLabel":{"de":"Audios & Podcasts"},"definition":{"de":"Die Produktion von Audios und Podcasts kann in vielseitigen Formaten umgesetzt werden. Die Erstellung von Interviews, Hörspielen oder Radiobeiträgen sind nur einige Beispiele. Diese Audioprojekte umfassen eine Planung, die eigentliche Durchführung sowie die Präsentation und die abschließende Bewertung. Zahlreiche digitale Werkzeuge wie beispielsweise Audacity oder Garageband können die unterschiedlichen Phasen des Audioprojekts unterstützen."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fc904a80-49ef-449e-a202-4fa73d98d7d6","notation":["fc904a80-49ef-449e-a202-4fa73d98d7d6"],"prefLabel":{"de":"Digitale Präsentationen"},"definition":{"de":"Digitale Präsentation eignen sich nicht nur zur Weiterverarbeitung recherchierter Informationen sondern auch als Werkzeug für die Erstellung kreative Medienprodukte. So können vertonte Präsentationen beispielsweise eine Bildergeschichte erzählen oder durch ein Aufnahmeprogramm zu einem Film werden. Die digitalen Werkzeuge zur Erstellung von Präsentationen sind dabei so vielseitig wie die Einsatzmöglichkeiten - klassische Präsentationsprogramme werden durch Apps und browserbasierte Anwendunge ergänzt."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/fd64c9f6-cdae-459d-aa6a-00b962c091e9","notation":["fd64c9f6-cdae-459d-aa6a-00b962c091e9"],"prefLabel":{"de":"Urheber- und Nutzungsrecht sowie Quellendokumentation"},"definition":{"de":"Das Urheber- und Nutzungsrecht sowie die Quellendokumentation spielt insbesondere in der Arbeit mit digitalen Medien und kreativen Projekten eine wichtige Rolle. Open Educational Resources, offene Bildungsmaterialien, und die Creative Commons Lizenzen sowie spezielle Portale und Suchmaschinen ermöglichen die rechtlich korrekte Nutzung von Materialien und Medien."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/9353dbac-4cd3-46ca-89e8-feac1817ec9a","notation":["9353dbac-4cd3-46ca-89e8-feac1817ec9a"],"prefLabel":{"de":"Problemlösen, Handeln und Modellieren"},"definition":{"de":"Hier findest du Materialien rund um das Thema Problemlösen, Handeln und Modellieren."},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/92f51521-2432-413f-8b85-47ce5e8ebca3","notation":["92f51521-2432-413f-8b85-47ce5e8ebca3"],"prefLabel":{"de":"Modellieren und Programmieren"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/b71f94dc-1178-482d-984a-1522facedddd","notation":["b71f94dc-1178-482d-984a-1522facedddd"],"prefLabel":{"de":"Anwendungsfelder für Algorithmen und Datenanalyse"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/900"}]}]}]},{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/66c667bc-8777-4c57-b476-35f54ce9ff5d","notation":["66c667bc-8777-4c57-b476-35f54ce9ff5d"],"prefLabel":{"de":"Religion"},"relatedMatch":[{"id":"http://w3id.org/openeduhub/vocabs/discipline/520"}],"narrower":[{"id":"http://w3id.org/openeduhub/vocabs/oeh-topics/22f6c61b-609b-44a5-88a8-1342b5256160","notation":["22f6c61b-609b-44a5-88a8-1342b5256160"],"prefLabel":{"de":"Religiosität und Welt(lichkeit)"},"relatedMatch":[{"id":"http://w3id.